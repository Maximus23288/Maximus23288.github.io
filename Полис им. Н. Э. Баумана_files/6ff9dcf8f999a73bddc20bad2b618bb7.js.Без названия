Vue.component('app-plugin-stashable-field-select', {
    template: '#app-template-plugin-stashable-field-select',
    mixins: [stashableFieldVueMixin],
    props: {
        itemsJson: {
            default: null
        },
        multiple: {
            default: false
        }
    },
    data: function () {
        return {
            items: []
        }
    },
    watch: {
        stashed: function (newVal) {
            if (!newVal) {
                this.$nextTick(function () {
                    $(this.$refs.select.$el).on('select2:close', function (e) {
                        if (!this.multiple) {
                            this.clickOverlay();
                        }
                    }.bind(this));
                }.bind(this));
            }
        }
    },
    mounted: function () {
        $(document).on('click', function (event) {
            if (!$(event.target).closest('.' + this.classWrapper).length && !$(event.target).closest('#select2-' + this.fieldUid + '-results').length) {
                if (!$(event.target).hasClass('select2-selection__choice__remove')) {
                    if (!this.stashed) {
                        this.clickOverlay();
                    }
                }
            }
        }.bind(this));
    },
    created: function () {
        if (this.itemsJson) {
            this.items = JSON.parse(this.itemsJson);
        }
        if (this.multiple && !_.isArray(this.value)) {
            if (this.value) {
                this.value = [this.value];
            } else {
                this.value = [];
            }
        }
        this.emitFieldChange(true);
    },
    computed: {
        classWrapper: function () {
            return 'js-wrapper-' + this.fieldUid;
        },
        itemsSelect: function () {
            var items = [];
            _.forEach(this.items, function (v, k) {
                if (this.inArray(v.value, this.value)) {
                    items.push(v);
                }
            }.bind(this));
            return items;
        },
        itemSelect: function () {
            var item = null;
            _.forEach(this.items, function (v, k) {
                if (v.value == this.value) {
                    item = v;
                    return;
                }
            }.bind(this));
            return item;
        }
    },
    methods: {
        clickOverlay: function () {
            if (this.valueBeforeChange != this.value) {
                this.saveIfOpen();
            } else {
                this.closeEdit();
            }
        },
        emitFieldChange: function (init) {
            init = !!init;
            var select = this.itemSelect;
            var data = (select) ? (select.data || {}) : {};
            $bus.$emit('plugin-stashable:field-change', {
                init: init,
                entity: this.entity,
                id: this.id,
                field: this.field,
                value: this.value,
                data: data
            });
        },
        getLabel: function (item) {
            return item.label || item.title;
        }
    }
});

$(function ($) {
    if ($('.js-app-plugin-stashable-field-select').length) {
        $('.js-app-plugin-stashable-field-select').each(function () {
            var el = $(this).get(0);
            new Vue({
                el: el,
                data: {}
            });
        });
    }
});
Vue.component('app-plugin-stashable-field-date', {
    template: '#app-template-plugin-stashable-field-date',
    mixins: [stashableFieldVueMixin],
    props: {},
    data: function () {
        return {}
    },
    watch: {},
    mounted: function () {
        $(this.$refs.field).datetimepicker({
            format: 'Y-m-d',
            timepicker: false,
            scrollInput: false,
            onSelectDate: function (date) {
                this.value = moment(date).format('YYYY-MM-DD');
            }.bind(this)
        });
    },
    created: function () {

    },
    computed: {
        render: function () {
            if (this.value) {
                return moment(this.value).format('DD MMMM YYYY');
            }
            return '';
        }
    },
    methods: {}
});

$(function ($) {
    if ($('.js-app-plugin-stashable-field-date').length) {
        $('.js-app-plugin-stashable-field-date').each(function () {
            var el = $(this).get(0);
            new Vue({
                el: el,
                data: {}
            });
        });
    }
});
Vue.component('app-plugin-stashable-field-attach', {
    template: '#app-template-plugin-stashable-field-attach',
    mixins: [stashableFieldVueMixin],
    props: {
        attachType: {
            default: null
        },
        itemJson: {
            default: null
        },
        iconPath: {
            default: null
        }
    },
    data: function () {
        return {
            item: null
        }
    },
    watch: {},
    mounted: function () {

    },
    created: function () {
        if (this.itemJson) {
            this.item = JSON.parse(this.itemJson);
        }
    },
    computed: {
        render: function () {
            return '';
        }
    },
    methods: {
        updateAttach: function (data) {
            this.item = data.item;
            if (data.item) {
                this.value = data.item.uid;
            } else {
                this.value = null;
            }
            this.save();
        },
        actionProcessingSave: function (data) {
            if (this.value != data.value) {
                if (data.hasOwnProperty('item')) {
                    this.item = data.value ? data.item : null;
                }
                this.value = data.value;
                this.valueBeforeChange = this.value;
                this.showAlertOtherUserSave(data.user);
            }
        }
    }
});

$(function ($) {
    if ($('.js-app-plugin-stashable-field-attach').length) {
        $('.js-app-plugin-stashable-field-attach').each(function () {
            var el = $(this).get(0);
            new Vue({
                el: el,
                data: {}
            });
        });
    }
});
