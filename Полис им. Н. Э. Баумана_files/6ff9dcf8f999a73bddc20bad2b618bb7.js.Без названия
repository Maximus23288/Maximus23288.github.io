Vue.component('truncate', {
    template: '<p><span v-if="!show">{{ truncate(text) }} <div class="content-text-more-toggle"><a v-if="text.length >= length" @click="toggle()"><i class="fa fa-angle-down"></i> {{ clamp || "Показать полностью" }}</a></div></span><span v-if="show">{{ text }} <div class="content-text-more-toggle"><a @click="toggle()" v-if="text.length >= length"><i class="fa fa-angle-up"></i> {{ less || "Скрыть" }}</a></div></span></p>',
    props: [ 'text', 'clamp', 'length', 'less' ],
    data: function () {
        return {
            show: false,
            counter: this.length
        };
    },
    methods: {
        truncate: function(string) {
            if (string) {
                return string.toString().substring(0, this.length || 100);
            }
            return '';
        },
        toggle: function() {
            this.show = !this.show;
        }
    }
});
