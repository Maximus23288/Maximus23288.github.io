        },
        methods: {
            save: function () {
                return {
                    text: this.content
                }
            },
            validate: function (data) {
                return true;
            },
            settings: function () {

            },
            button: function () {
                return {
                    class: 'btn-create-block btn-create-block_page',
                    title: t('Разрыв страницы')
                }
            },
            onSettingsItem: function (data) {

