Vue.component('app-plugin-stashable-field-textarea', {
    template: '#app-template-plugin-stashable-field-textarea',
    mixins: [stashableFieldVueMixin],
    props: {
        placeholder: {
            default: null
        },
        rows: {
            default: 20
        }
    },
    data: function () {
        return {}
    },
    watch: {
        stashed: function () {
            if (!this.stashed) {
                this.$nextTick(function () {
                    this.resize();
                });
            }
        }
    },
    mounted: function () {
        $(this.$refs.field).on('input', function () {
            this.resize($(this));
        }.bind(this));
    },
    created: function () {

    },
    computed: {
        text: function () {
            return this.escapeHtml(this.value).replace(/\n/g, '<br>');
        }
    },
    methods: {
        resize: function () {
            var input = $(this.$refs.field);
            input.css('height', 'auto');
            input.css('height', input[0].scrollHeight + 'px');
        }
    }
});

