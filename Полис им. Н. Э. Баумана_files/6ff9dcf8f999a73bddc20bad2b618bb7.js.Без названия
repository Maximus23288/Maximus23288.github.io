                }
            });
        }.bind(this),
        afterhide: function (event, data) {

        }.bind(this)
    });
};
var ls = ls || {};
ls.plugin = ls.plugin || {};

ls.plugin.sso = (function ($) {

    var that = this;

    this.goServiceLogin = function (service, referal) {
        referal = referal || window.location.href;
        ls.ajax.load('sso/login-oauth', {service: service, referal: referal}, function (res) {
            if (res.sUrl) {
                window.location.href = res.sUrl;
            }
        }.bind(this), {
            onError: function () {
                ls.msg.error(null, 'Регистрация через ' + service + ' временно не доступна.');
            }
        });
    };

    this.findGetParameter = function (parameterName) {
        var result = null,
            tmp = [];
        location.search
            .substr(1)
            .split("&")
            .forEach(function (item) {
                tmp = item.split("=");
                if (tmp[0] === parameterName) result = decodeURIComponent(tmp[1]);
            });
        return result;
    };

    $(function () {
        $(document).on('click', '.js-sso-auth, .js-bmstuid-auth', function (e) {
            that.goServiceLogin($(this).data('service'), $(this).data('referal'));
            return false;
        });
        if (this.findGetParameter('withauth')) {
            this.goServiceLogin('bmstu');
        }
    }.bind(this));

    return this;
}).call(ls.plugin.sso || {}, jQuery);
var ls = ls || {};

ls.plugin_album = (function ($) {

    this.options = {};

    this.init = function () {
        $(document).on('click', '.js-album-remove', function () {
            if (!confirm('Действительно удалить?')) {
                return false;
            }

            var url = $(this).data('url') || (aRouter.ajax + 'plugin-album/album/remove');
            var params = $(this).data('params') || {};
            params = $.extend({}, params, {
                id: $(this).data('id')
            });
            ls.ajax.load(url, params, function () {

            }.bind(this));
            return false;
        });

        this.initModals();
    };

    this.initModals = function () {
        $('.js-album-create-modal, .js-album-update-modal').each(function (k, v) {
            v = $(v);
            var id = v.data('id');
            var ownerKey = v.data('ownerKey');
            var url = v.data('url') || (aRouter.ajax + 'plugin-album/album/modal/create');
            v.lsModalToggle({
                url: url,
                modalOptions: {
                    center: false
                },
                params: {
                    id: id,
                    owner_key: ownerKey
                }
            });
            v.bind('lsmodaltoggleshow', function () {
                this.initForm();
            }.bind(this));
        }.bind(this));
    };

    this.initForm = function (name, params) {
        var $this = this;
        params = $.extend({}, {urls: {}}, params || {});


        ls.ajax.form(params.urls.create || (aRouter.ajax + 'plugin-album/album/create'), '.js-album-create-form', function (result) {
            $('#modal-album-create').lsModal('hide');
        });
        ls.ajax.form(params.urls.update || (aRouter.ajax + 'plugin-album/album/update'), '.js-album-update-form', function (result) {
            $('#modal-album-create').lsModal('hide');
        });

        $('.js-album-datetime-picker').datetimepicker({
            format: 'Y-m-d H:i:00',
            lang: 'ru',
            scrollMonth: false,
            scrollTime: false,
            scrollInput: false,
            dayOfWeekStart: 1
        });
    };

    $(function () {
        ls.plugin_album.init();
    });

    return this;
}).call(ls.plugin_album || {}, jQuery);
