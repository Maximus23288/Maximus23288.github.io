Vue.component('app-branch-event-create-modal', {
    template: '#app-template-branch-event-create-modal',
    props: ['branch'],
    data: function () {
        return {
            id: null,
            copy_id: null,
            form: {}
        };
    },
    watch: {},
    created: function () {
        $bus.$on('branch-event-create-modal:show', function (data) {
            this.id = null;
            this.copy_id = null;
            if (data) {
                if (data.item) {
                    this.id = data.item.id
                }
                if (data.copy_id) {
                    this.copy_id = data.copy_id
                }
                if (data.date_begin) {
                    this.form.date_begin = data.date_begin;
                }
            }
            this.showModal();
        }.bind(this));
    },
    mounted: function () {

    },
    methods: {
        showModal: function () {
            var data = {
                branch: this.branch,
                id: this.id,
                copy_id: this.copy_id
            };
            var categoryItems = [];
            var addressItems = [];
            var teacherItems = [];
            var options = {
                url: aRouter.branch + 'ajax/event/create-modal/',
                center: false,
                aftershow: function () {
                    new Vue({
                        el: $('#branch-event-create-modal-form-app').get(0),
                        data: {
                            form: this.form,
                            categoryItems: categoryItems,
                            addressItems: addressItems,
                            teacherItems: teacherItems
                        }
                    });
                }.bind(this),
                afterload: function (res) {
                    categoryItems = res.categoryItems;
                    addressItems = res.addressItems;
                    teacherItems = res.teacherItems;
                }
            };
            ls.modal.load(options.url, data, options);
        }
    }
});

$(function ($) {
    if ($('.js-app-branch-event-create-modal').length) {
        $('.js-app-branch-event-create-modal').each(function (k, v) {
            new Vue({
                el: v,
                data: {}
            });
        });
    }
});
Vue.component('app-branch-event-create-modal-form', {
    template: '#app-template-branch-event-create-modal-form',
    props: ['branch', 'enableRequest', 'id', 'copy_id', 'initForm', 'initCategoryItems', 'initAddressItems', 'initTeacherItems'],
    data: function () {
        return {
            form: {
                address_id: '',
                teacher_id: '',
                category_id: ''
            },
            categoryItems: [],
            addressItems: [],
            teacherItems: [],
            isProcessingSubmit: false,
            isProcessingLoad: false
        };
    },
    watch: {
        'form.teacher_id': function (valueNew, valueOld) {
            if (this.form.teacher_id && !this.form.title) {
                let teacher = _.find(this.teacherItems, {id: this.form.teacher_id});
                if (teacher) {
                    this.form.title = teacher.title;
                }
            }
        }
    },
    created: function () {
        if (this.initCategoryItems) {
            this.categoryItems = this.initCategoryItems;
        }
        if (this.initAddressItems) {
            this.addressItems = this.initAddressItems;
        }
        if (this.initTeacherItems) {
            this.teacherItems = this.initTeacherItems;
        }
        this.form.branch_id = this.branch;
        if (this.id || this.copy_id) {
            this.load();
        } else if (this.initForm) {
            _.forEach(this.initForm, function (value, field) {
                this.$set(this.form, field, value);
            }.bind(this));
        }
    },
    mounted: function () {
        var calendarParams = {
            format: 'Y-m-d H:i',
            timepicker: false,
            theme: 'bmstu-calendar',
            dayOfWeekStart: 1,
            scrollInput: false,
            onChangeDateTime: function (ct, $i) {
                $i.get(0).dispatchEvent(new Event('input'));
            }
        };
        $(this.$refs.date_begin).datetimepicker(calendarParams);
        $(this.$refs.date_end).datetimepicker(calendarParams);
        $(this.$refs.date_registration_end).datetimepicker(calendarParams);
        $(this.$refs.clone_date_end).datetimepicker(calendarParams);
        $(this.$refs.address_text).suggestions({
            token: ls.registry.get('dadata.token'),
            type: "ADDRESS",
            count: 7,
            hint: false,
            constraints: {
                label: "",
                locations: {
                    "country": "*"
                }
            },
            onSelect: function (suggestion) {
                this.form.address_text = suggestion.value;
            }.bind(this)
        });
    },
    computed: {
        showAddressText: function () {
            if (!this.form.address_id || !this.addressItems.length) {
                return true;
            }
            return false;
        }
    },
    methods: {
        submit: function () {
            if (this.isProcessingSubmit) {
                return;
            }
            this.isProcessingSubmit = true;
            var params = {
                item: this.form
            };
            if (this.id) {
                params['id'] = this.id;
            }
            ls.ajax.load(aRouter.branch + 'ajax/event/' + (this.id ? 'update/' : 'create/'), params, function (res) {
                $('#modal-branch-event-create').lsModal('hide');
                $bus.$emit('branch-event-create:completed', {
                    item: res.item
                });
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingSubmit = false;
                }.bind(this),
                onError: function (res) {
                    if (res.errors) {
                        _.forEach(res.errors, function (errors, fieldName) {
                            if (this.$refs[fieldName]) {
                                var field = $(this.$refs[fieldName]);
                                field.lsValidationField();
                                if (field.lsValidationField('isInit')) {
                                    field.lsValidationField('showError', errors);
                                }
                            }
                        }.bind(this));
                    }
                }.bind(this)
            });
        },
        load: function () {
            if (this.isProcessingLoad) {
                return;
            }
            this.isProcessingLoad = true;
            var params = {
                id: this.id,
                for_update: 1
            };
            if (this.copy_id) {
                params['id'] = this.copy_id;
            }
            ls.ajax.load(aRouter.branch + 'ajax/event/get/', params, function (res) {
                this.form = res.item;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoad = false;
                }.bind(this)
            });
        }
    }
});
Vue.use(window["vue-js-modal"].default);
Vue.component('app-editor-block-branch-event', {
    template: '#app-template-editor-block-branch-event',
    props: ['initData', 'initParams', 'api', 'uuid'],
    components: {},
    data: function () {
        return {
            params: {
                branch_id: null
            },
            modalName: 'modal-event',
            isProcessingLoad: false,
            isProcessingItemsLoad: false,
            isVisible: false,
            isShowModal: false,
            id: null,
            item: null,
            items: [],
            isShowDescription: true
        };
    },
    watch: {},
    created: function () {
        this.modalName = this.generateUuid();
    },
    mounted: function () {
        if (this.initParams) {
            _.forEach(this.initParams, function (value, name) {
                this.$set(this.params, name, value);
            }.bind(this));
        }

        this.processingData(this.initData);

        if (this.id) {
            this.isVisible = true;
            this.load();
        } else {
            this.showModal();
        }
    },
    computed: {
        isHavePeriod: function () {
            if (this.item.date_end) {
                var dateBegin = moment(this.item.date_begin).format('YYYY-MM-DD');
                var dateEnd = moment(this.item.date_end).format('YYYY-MM-DD');
                if (dateBegin != dateEnd) {
                    return true;
                }
            }
            return false;
        },
        dateBeginTime: function () {
            return moment(this.item.date_begin).format('HH:mm');
        },
        dateRegistrationEnd: function () {
            return this.item.date_registration_end ? moment(this.item.date_registration_end).format('LL HH:mm') : null;
        },
        dateBeginDay: function () {
            return moment(this.item.date_begin).format('LL');
        },
        dateEndTime: function () {
            return this.item.date_end ? moment(this.item.date_end).format('HH:mm') : null;
        },
        dateEndDay: function () {
            return this.item.date_end ? moment(this.item.date_end).format('LL') : null;
        },
        address: function () {
            if (this.item.address) {
                if (this.item.address.address) {
                    return this.item.address.address;
                }
                if (this.item.address.title) {
                    return this.item.address.title;
                }
                if (this.item.address_text) {
                    return this.item.address_text;
                }
            }
            return null;
        }
    },
    methods: {
        save: function () {
            return {
                id: this.id
            }
        },
        validate: function (data) {
            if (data.id) {
                return true;
            }
            return false;
        },
        settings: function () {

        },
        button: function () {
            return {
                class: 'btn-create-block btn-create-block_event',
                title: 'Добавить мероприятие'
            }
        },
        onSettingsItem: function (data) {

        },
        processingData: function (data) {
            if (typeof data !== 'object') {
                return;
            }
            this.id = data.id || null;
        },

        load: function () {
            this.isProcessingLoad = true;
            ls.ajax.load(aRouter.branch + 'ajax/event/get/', {
                id: this.id
            }, function (res) {
                this.item = res.item;
                if (this.isShowModal) {
                    this.closeModal();
                }
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoad = false;
                }.bind(this)
            });
        },
        loadItems: function () {
            this.isProcessingItemsLoad = true;
            ls.ajax.load(aRouter.branch + 'ajax/event/get-future/', {
                id: this.params.branch_id
            }, function (res) {
                this.items = res.items;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingItemsLoad = false;
                }.bind(this)
            });
        },
        showModal: function () {
            this.loadItems();
            this.$modal.show(this.modalName);
        },
        closeModal: function () {
            this.$modal.hide(this.modalName);
        },
        onModalOpened: function () {
            this.isShowModal = true;
        },
        onModalClosed: function () {
            if (this.item) {
                this.isVisible = true;
            } else {
                this.api.remove(this.uuid);
            }
            this.isShowModal = false;
        },
        generateUuid: function uuidv4() {
            return ([1e7] + -1e3 + -4e3 + -8e3 + -1e11).replace(/[018]/g, function (c) {
                return (c ^ crypto.getRandomValues(new Uint8Array(1))[0] & 15 >> c / 4).toString(16);
            });
        },
        addEvent: function () {
            this.load();
        }
    }
});
Vue.component('app-branch-distribution', {
    template: '#app-template-branch-distribution',
    props: {
        id: {
            default: null
        },
        branch_id: {
            default: null
        }
    },
    data: function () {
        return {
            isProcessingLoad: false,
            isProcessingScheduleCreate: false,
            isProcessingScheduleRemove: false,
            isScheduleUpdateOpen: false,
            group: null,
            userItems: [],
            addressItems: [],
            teacherItems: [],
            scheduleItems: [],
            scheduleItemUpdate: null,
            scheduleForm: {
                time_id: null,
                day_of_week: null,
                week_type: null,
                is_denominator: 0,
                is_numerator: 0
            },
            timeItems: [
                {
                    id: 1,
                    title: '1 пара'
                },
                {
                    id: 2,
                    title: '2 пара'
                },
                {
                    id: 3,
                    title: '3 пара'
                },
                {
                    id: 4,
                    title: '4 пара'
                },
                {
                    id: 5,
                    title: '5 пара'
                },
                {
                    id: 6,
                    title: '6 пара'
                },
                {
                    id: 7,
                    title: '7 пара'
                }
            ],
            dayItems: [
                {
                    id: 1,
                    title: 'Понедельник'
                },
                {
                    id: 2,
                    title: 'Вторник'
                },
                {
                    id: 3,
                    title: 'Среда'
                },
                {
                    id: 4,
                    title: 'Четверг'
                },
                {
                    id: 5,
                    title: 'Пятница'
                },
                {
                    id: 6,
                    title: 'Суббота'
                },
                {
                    id: 7,
                    title: 'Воскресенье'
                }
            ]
        };
    },
    watch: {
        'scheduleForm.week_type': function () {
            if (this.scheduleForm.week_type == 'denominator') {
                this.scheduleForm.is_denominator = 1;
                this.scheduleForm.is_numerator = 0;
            }
            if (this.scheduleForm.week_type == 'numerator') {
                this.scheduleForm.is_numerator = 1;
                this.scheduleForm.is_denominator = 0;
            }
            if (!this.scheduleForm.week_type) {
                this.scheduleForm.is_numerator = 0;
                this.scheduleForm.is_denominator = 0;
            }
        }
    },
    created: function () {
        this.loadData();
        this.loadSchedule();
    },
    computed: {
        isReady: function () {
            return !!this.group;
        }
    },
    mounted: function () {

    },
    methods: {
        loadData: function () {
            this.isProcessingLoad = true;
            ls.ajax.load(aRouter.admin + 'ajax/distribution/group-data/', {id: this.id}, function (res) {
                this.group = res.group;
                this.userItems = res.users;
                this.addressItems = res.addresses;
                this.teacherItems = res.teachers;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoad = false;
                }.bind(this)
            });
        },
        loadSchedule: function () {
            if (this.isProcessingScheduleLoad) {
                return;
            }
            this.isProcessingScheduleLoad = true;
            ls.ajax.load(aRouter.admin + 'ajax/distribution/schedule-load/', {id: this.id}, function (res) {
                this.scheduleItems = res.items;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingScheduleLoad = false;
                }.bind(this)
            });
        },
        createSchedule: function () {
            if (this.isProcessingScheduleCreate) {
                return;
            }
            this.isProcessingScheduleCreate = true;
            ls.ajax.load(aRouter.admin + 'ajax/distribution/schedule-create/', {
                group_id: this.id,
                item: this.scheduleForm
            }, function (res) {
                this.loadSchedule();
                this.clearScheduleForm();
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingScheduleCreate = false;
                }.bind(this)
            });
        },
        updateSchedule: function () {
            if (this.isProcessingScheduleUpdate) {
                return;
            }
            this.isProcessingScheduleUpdate = true;
            ls.ajax.load(aRouter.admin + 'ajax/distribution/schedule-update/', {
                id: this.scheduleItemUpdate.id,
                item: this.scheduleForm
            }, function (res) {
                this.loadSchedule();
                this.closeUpdateSchedule();
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingScheduleUpdate = false;
                }.bind(this)
            });
        },
        clearScheduleForm: function () {
            this.scheduleForm.time_id = null;
            this.scheduleForm.day_of_week = null;
            this.scheduleForm.week_type = null;
        },
        removeSchedule: function (item) {
            if (this.isProcessingScheduleRemove) {
                return;
            }
            this.isProcessingScheduleRemove = true;
            ls.ajax.load(aRouter.admin + 'ajax/distribution/schedule-remove/', {
                id: item.id
            }, function (res) {
                this.scheduleItems = res.items;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingScheduleRemove = false;
                }.bind(this)
            });
        },
        openUpdateSchedule: function (item) {
            this.scheduleItemUpdate = item;
            this.isScheduleUpdateOpen = true;
            this.scheduleForm.time_id = item.time_id;
            this.scheduleForm.day_of_week = item.day_of_week;
            this.scheduleForm.week_type = null;
            if (item.is_denominator) {
                this.scheduleForm.week_type = 'denominator';
            }
            if (item.is_numerator) {
                this.scheduleForm.week_type = 'numerator';
            }
        },
        closeUpdateSchedule: function () {
            this.isScheduleUpdateOpen = false;
            this.scheduleItemUpdate = null;
            this.clearScheduleForm();
        },
        dayOfWeek: function (num) {
            var result = null;
            _.forEach(this.dayItems, function (item, name) {
                if (item.id == num) {
                    result = item.title;
                }
            }.bind(this));
            return result;
        },
        addressTitle: function (address) {
            if (address.title) {
                return address.title + ' (' + address.address + ')';
            } else {
                return address.address;
            }
        },
        valueDistributionTeacher: function (user, schedule) {
            if (user.distribution[schedule.id]) {
                return user.distribution[schedule.id].teacher_id;
            }
            return null;
        },
        valueDistributionAddress: function (user, schedule) {
            if (user.distribution[schedule.id]) {
                return user.distribution[schedule.id].address_id;
            }
            return null;
        },
        isShowDistributionAddress: function (user, schedule) {
            if (user.distribution[schedule.id]) {
                return !!user.distribution[schedule.id].teacher_id;
            }
            return false;
        },
        changeDistributionTeacher: function (userIndex, schedule, value) {
            var dist = this.userItems[userIndex].distribution[schedule.id] || {teacher_id: '', schedule_id: schedule.id, address_id: ''};

            if (dist.teacher_id != value) {
                dist.teacher_id = value;
                if (!dist.teacher_id) {
                    dist.address_id = '';
                }
                this.$set(this.userItems[userIndex].distribution, schedule.id, dist);
                this.updateDistribution(userIndex, schedule);
            }
        },
        changeDistributionAddress: function (userIndex, schedule, value) {
            var dist = this.userItems[userIndex].distribution[schedule.id] || {teacher_id: '', schedule_id: schedule.id, address_id: ''};

            if (dist.address_id != value) {
                dist.address_id = value;
                this.$set(this.userItems[userIndex].distribution, schedule.id, dist);
                this.updateDistribution(userIndex, schedule);
            }
        },
        updateDistribution: function (userIndex, schedule) {
            if (this.userItems[userIndex].isProcessingSave) {
                return;
            }
            this.$set(this.userItems[userIndex], 'isProcessingSave', true);
            ls.ajax.load(aRouter.admin + 'ajax/distribution/update/', {
                id: this.id,
                user_uid: this.userItems[userIndex].uid,
                schedule_id: schedule.id,
                distribution: this.userItems[userIndex].distribution[schedule.id]
            }, function (res) {
                if (res.teacherUsedOld) {
                    var index = this.getTeacherIndexById(res.teacherUsedOld.teacher_id);
                    this.$set(this.teacherItems[index], 'used', res.teacherUsedOld.used);
                }
                if (res.teacherUsedNew) {
                    var index = this.getTeacherIndexById(res.teacherUsedNew.teacher_id);
                    this.$set(this.teacherItems[index], 'used', res.teacherUsedNew.used);
                }
                if (res.address_id) {
                    this.$set(this.userItems[userIndex].distribution[schedule.id], 'address_id', res.address_id);
                    this.$set(this.userItems[userIndex], 'isProcessingSave', false);
                    this.updateDistribution(userIndex, schedule);
                }
            }.bind(this), {
                onComplete: function () {
                    this.$set(this.userItems[userIndex], 'isProcessingSave', false);
                }.bind(this)
            });
        },
        countTeacherUsed: function (teacher, schedule) {
            var key = schedule.day_of_week + '_' + schedule.time_id;
            if (teacher.used[key]) {
                return teacher.used[key];
            }
            return 0;
        },
        getTeacherIndexById: function (id) {
            var index = null;
            _.forEach(this.teacherItems, function (item, i) {
                if (item.id == id) {
                    index = i;
                }
            }.bind(this));
            return index;
        },
        getTeachersForSelect: function (schedule) {
            var data = [{id: '', text: ''}];
            _.forEach(this.teacherItems, function (item, i) {
                data.push({id: item.id, text: item.title + ' (' + this.countTeacherUsed(item, schedule) + ')'});
            }.bind(this));
            return data;
        }
    }
});

$(function ($) {
    if ($('.js-app-branch-distribution').length) {
        $('.js-app-branch-distribution').each(function (k, v) {
            new Vue({
                el: v,
                data: {}
            });
        });
    }
});
Vue.component('app-branch-distribution-users', {
    template: '#app-template-branch-distribution-users',
    props: {
        branch_id: {
            default: null
        }
    },
    data: function () {
        return {
            isProcessingLoad: false,
            isProcessingLoadUsers: false,
            userItems: [],
            addressItems: [],
            teacherItems: [],
            sectionItems: [],
            facultyItems: [],
            medicalGroupItems: [],
            page: 1,
            pageCount: 0,
            pageCurrent: 1,
            count: 0,
            filter: {
                only_undistribution: true,
                time_id: null,
                day_of_week: null,
                medical_group_id: null,
                section_id: null,
                faculty_id: null,
                fio: null,
                checkbox: {}
            },
            timeItems: [
                {
                    id: 1,
                    title: '1 пара'
                },
                {
                    id: 2,
                    title: '2 пара'
                },
                {
                    id: 3,
                    title: '3 пара'
                },
                {
                    id: 4,
                    title: '4 пара'
                },
                {
                    id: 5,
                    title: '5 пара'
                },
                {
                    id: 6,
                    title: '6 пара'
                },
                {
                    id: 7,
                    title: '7 пара'
                }
            ],
            dayItems: [
                {
                    id: 1,
                    title: 'Понедельник'
                },
                {
                    id: 2,
                    title: 'Вторник'
                },
                {
                    id: 3,
                    title: 'Среда'
                },
                {
                    id: 4,
                    title: 'Четверг'
                },
                {
                    id: 5,
                    title: 'Пятница'
                },
                {
                    id: 6,
                    title: 'Суббота'
                },
                {
                    id: 7,
                    title: 'Воскресенье'
                }
            ]
        };
    },
    watch: {
        'filter.only_undistribution': function (val, oldVal) {
            this.makeFilter();
        },
        'filter.time_id': function (val, oldVal) {
            this.makeFilter();
        },
        'filter.day_of_week': function (val, oldVal) {
            this.makeFilter();
        },
        'filter.medical_group_id': function (val, oldVal) {
            this.makeFilter();
        },
        'filter.section_id': function (val, oldVal) {
            this.makeFilter();
        },
        'filter.faculty_id': function (val, oldVal) {
            this.makeFilter();
        },
        'filter.checkbox': function (val, oldVal) {
            this.makeFilter();
        }
    },
    created: function () {
        this.loadData();
        this.loadUsers();
    },
    computed: {
        isReady: function () {
            return !this.isProcessingLoad;
        }
    },
    mounted: function () {

    },
    methods: {
        loadData: function () {
            this.isProcessingLoad = true;
            ls.ajax.load(aRouter.admin + 'ajax/distribution/data/', {}, function (res) {
                this.addressItems = res.addresses;
                this.teacherItems = res.teachers;
                this.sectionItems = res.sections;
                this.facultyItems = res.faculties;
                this.medicalGroupItems = res.medicalGroups;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoad = false;
                }.bind(this)
            });
        },
        makeFilter: _.debounce(
            function () {
                this.newSearch();
            },
            500
        ),
        newSearch: function () {
            this.page = 1;
            this.loadUsers();
        },
        loadUsers: function () {
            if (this.isProcessingLoadUsers) {
                return;
            }
            this.isProcessingLoadUsers = true;
            this.userItems = [];
            var params = $.extend({}, this.filter, {
                page: this.page
            });
            ls.ajax.load(aRouter.admin + 'ajax/distribution/users/', params, function (res) {
                this.userItems = res.users;
                if (res.paging) {
                    this.pageCount = res.paging.iCountPage;
                    this.pageCurrent = res.paging.iCurrentPage;
                }
                this.count = res.count;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoadUsers = false;
                }.bind(this)
            });
        },
        dayOfWeek: function (num) {
            var result = null;
            _.forEach(this.dayItems, function (item, name) {
                if (item.id == num) {
                    result = item.title;
                }
            }.bind(this));
            return result;
        },
        addressTitle: function (address) {
            if (address.title) {
                return address.title + ' (' + address.address + ')';
            } else {
                return address.address;
            }
        },
        valueDistributionTeacher: function (user, schedule) {
            if (user.distribution[schedule.id]) {
                return user.distribution[schedule.id].teacher_id;
            }
            return null;
        },
        valueDistributionAddress: function (user, schedule) {
            if (user.distribution[schedule.id]) {
                return user.distribution[schedule.id].address_id;
            }
            return null;
        },
        isShowDistributionAddress: function (user, schedule) {
            if (user.distribution[schedule.id]) {
                return !!user.distribution[schedule.id].teacher_id;
            }
            return false;
        },
        changeDistributionTeacher: function (userIndex, schedule, value) {
            var dist = this.userItems[userIndex].distribution[schedule.id] || {teacher_id: '', schedule_id: schedule.id, address_id: ''};

            if (dist.teacher_id != value) {
                dist.teacher_id = value;
                if (!dist.teacher_id) {
                    dist.address_id = '';
                }
                this.$set(this.userItems[userIndex].distribution, schedule.id, dist);
                this.updateDistribution(userIndex, schedule);
            }
        },
        changeDistributionAddress: function (userIndex, schedule, value) {
            var dist = this.userItems[userIndex].distribution[schedule.id] || {teacher_id: '', schedule_id: schedule.id, address_id: ''};

            if (dist.address_id != value) {
                dist.address_id = value;
                this.$set(this.userItems[userIndex].distribution, schedule.id, dist);
                this.updateDistribution(userIndex, schedule);
            }
        },
        removeDistribution: function (userIndex) {
            if (!confirm('Действительно удалить запись?')) {
                return false;
            }
            ls.ajax.load(aRouter.admin + 'ajax/distribution/remove/', {
                user_uid: this.userItems[userIndex].uid
            }, function (res) {
                this.$delete(this.userItems, userIndex);
            }.bind(this), {
                onComplete: function () {

                }.bind(this)
            });
        },
        updateDistribution: function (userIndex, schedule) {
            if (this.userItems[userIndex].isProcessingSave) {
                return;
            }
            this.$set(this.userItems[userIndex], 'isProcessingSave', true);
            ls.ajax.load(aRouter.admin + 'ajax/distribution/update/', {
                user_uid: this.userItems[userIndex].uid,
                schedule_id: schedule.id,
                distribution: this.userItems[userIndex].distribution[schedule.id]
            }, function (res) {
                if (res.teacherUsedOld) {
                    var index = this.getTeacherIndexById(res.teacherUsedOld.teacher_id);
                    this.$set(this.teacherItems[index], 'used', res.teacherUsedOld.used);
                }
                if (res.teacherUsedNew) {
                    var index = this.getTeacherIndexById(res.teacherUsedNew.teacher_id);
                    this.$set(this.teacherItems[index], 'used', res.teacherUsedNew.used);
                }
                if (res.address_id) {
                    this.$set(this.userItems[userIndex].distribution[schedule.id], 'address_id', res.address_id);
                    this.$set(this.userItems[userIndex], 'isProcessingSave', false);
                    this.updateDistribution(userIndex, schedule);
                }
            }.bind(this), {
                onComplete: function () {
                    this.$set(this.userItems[userIndex], 'isProcessingSave', false);
                }.bind(this)
            });
        },
        countTeacherUsed: function (teacher, schedule) {
            var key = schedule.day_of_week + '_' + schedule.time_id;
            if (teacher.used[key]) {
                return teacher.used[key];
            }
            return 0;
        },
        getTeacherIndexById: function (id) {
            var index = null;
            _.forEach(this.teacherItems, function (item, i) {
                if (item.id == id) {
                    index = i;
                }
            }.bind(this));
            return index;
        },
        getTeachersForSelect: function (schedule) {
            var data = [{id: '', text: ''}];
            _.forEach(this.teacherItems, function (item, i) {
                data.push({id: item.id, text: item.title + ' (' + this.countTeacherUsed(item, schedule) + ')'});
            }.bind(this));
            return data;
        },
        selectPage: function (data) {
            this.page = data.page;
            this.loadUsers();
        }
    }
});

$(function ($) {
    if ($('.js-app-branch-distribution-users').length) {
        $('.js-app-branch-distribution-users').each(function (k, v) {
            new Vue({
                el: v,
                data: {}
            });
        });
    }
});
Vue.component('app-branch-distribution-check', {
    template: '#app-template-branch-distribution-check',
    props: {
        branch_id: {
            default: null
        }
    },
    data: function () {
        return {
            isProcessingSubmit: false,
            isProcessingLoad: false,
            facultyItems: [],
            medicalGroupItems: [],
            resultHtml: null,
            filter: {
                medical_group_id: null,
                faculty_id: null,
                group: null
            },
            timeItems: [
                {
                    id: 1,
                    title: '1 пара'
                },
                {
                    id: 2,
                    title: '2 пара'
                },
                {
                    id: 3,
                    title: '3 пара'
                },
                {
                    id: 4,
                    title: '4 пара'
                },
                {
                    id: 5,
                    title: '5 пара'
                },
                {
                    id: 6,
                    title: '6 пара'
                },
                {
                    id: 7,
                    title: '7 пара'
                }
            ],
            dayItems: [
                {
                    id: 1,
                    title: 'Понедельник'
                },
                {
                    id: 2,
                    title: 'Вторник'
                },
                {
                    id: 3,
                    title: 'Среда'
                },
                {
                    id: 4,
                    title: 'Четверг'
                },
                {
                    id: 5,
                    title: 'Пятница'
                },
                {
                    id: 6,
                    title: 'Суббота'
                },
                {
                    id: 7,
                    title: 'Воскресенье'
                }
            ]
        };
    },
    watch: {},
    created: function () {
        this.loadData();
    },
    computed: {
        isReady: function () {
            return !this.isProcessingLoad;
        }
    },
    mounted: function () {

    },
    methods: {
        loadData: function () {
            this.isProcessingLoad = true;
            ls.ajax.load(aRouter.admin + 'ajax/distribution/data/', {}, function (res) {
                this.facultyItems = res.faculties;
                this.medicalGroupItems = res.medicalGroups;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoad = false;
                }.bind(this)
            });
        },
        dayOfWeek: function (num) {
            var result = null;
            _.forEach(this.dayItems, function (item, name) {
                if (item.id == num) {
                    result = item.title;
                }
            }.bind(this));
            return result;
        },
        submit: function () {
            this.isProcessingSubmit = true;
            ls.ajax.load(aRouter.admin + 'ajax/distribution/check/', this.filter, function (res) {
                this.resultHtml = res.html;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingSubmit = false;
                }.bind(this)
            });
        }
    }
});

$(function ($) {
    if ($('.js-app-branch-distribution-check').length) {
        $('.js-app-branch-distribution-check').each(function (k, v) {
            new Vue({
                el: v,
                data: {}
            });
        });
    }
});
Vue.component('app-branch-distribution-teacher', {
    template: '#app-template-branch-distribution-teacher',
    props: {},
    data: function () {
        return {
            isProcessingLoad: false,
            isProcessingLoadUsers: false,
            isProcessingLoadCalendar: false,
            isProcessingScheduleCreate: false,
            isProcessingScheduleRemove: false,
            isScheduleUpdateOpen: false,
            userItems: [],
            calendarItems: [],
            calendarItem: null,
            teacherItems: [],
            scheduleItems: [],
            addressesItems: [],
            sectionItems: [],
            facultyItems: [],
            medicalGroupItems: [],
            scheduleItemUpdate: null,
            scheduleForm: {
                time_id: null,
                section_id: null,
                faculty_id: null,
                medical_group_id: null,
                address_id: null,
                count_user_max: null,
                day_of_week: null,
                week_type: null,
                is_denominator: 0,
                is_numerator: 0,
                priority: null
            },
            teacher: null,
            countDenominator: 0,
            countNumerator: 0,
            currentDay: null,
            currentTime: null,
            filter: {
                teacher_id: null
            },
            timeItems: [
                {
                    id: 1,
                    title: '1 пара'
                },
                {
                    id: 2,
                    title: '2 пара'
                },
                {
                    id: 3,
                    title: '3 пара'
                },
                {
                    id: 4,
                    title: '4 пара'
                },
                {
                    id: 5,
                    title: '5 пара'
                },
                {
                    id: 6,
                    title: '6 пара'
                },
                {
                    id: 7,
                    title: '7 пара'
                }
            ],
            dayItems: [
                {
                    id: 1,
                    title: 'понедельник'
                },
                {
                    id: 2,
                    title: 'вторник'
                },
                {
                    id: 3,
                    title: 'среда'
                },
                {
                    id: 4,
                    title: 'четверг'
                },
                {
                    id: 5,
                    title: 'пятница'
                },
                {
                    id: 6,
                    title: 'суббота'
                },
                {
                    id: 7,
                    title: 'воскресенье'
                }
            ]
        };
    },
    watch: {
        filter: {
            handler: function (val, oldVal) {
                this.loadCalendar();
            },
            deep: true
        },
        'scheduleForm.week_type': function () {
            if (this.scheduleForm.week_type == 'denominator') {
                this.scheduleForm.is_denominator = 1;
                this.scheduleForm.is_numerator = 0;
            }
            if (this.scheduleForm.week_type == 'numerator') {
                this.scheduleForm.is_numerator = 1;
                this.scheduleForm.is_denominator = 0;
            }
            if (!this.scheduleForm.week_type) {
                this.scheduleForm.is_numerator = 0;
                this.scheduleForm.is_denominator = 0;
            }
        }
    },
    created: function () {
        this.loadData();
    },
    computed: {
        isReady: function () {
            return !this.isProcessingLoad;
        },
        countAll: function () {
            return this.countNumerator + this.countDenominator;
        },
        pathAssets: function () {
            return PATH_SKIN + '/assets';
        },
        getStaticPath: function () {
            return ls.registry.get('branch.url');
        },
        scheduleItemsFiltered: function () {
            if (this.currentDay && this.currentTime) {
                var result = [];
                _.forEach(this.scheduleItems, function (scheduleItem, name) {
                    if (scheduleItem.time_id == this.currentTime && scheduleItem.day_of_week == this.currentDay) {
                        result.push(scheduleItem);
                    }
                }.bind(this));
                return result;
            }
            return [];
        }
    },
    mounted: function () {

    },
    methods: {
        loadData: function () {
            this.isProcessingLoad = true;
            ls.ajax.load(aRouter.admin + 'ajax/distribution/data/', {}, function (res) {
                this.teacherItems = res.teachers;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoad = false;
                }.bind(this)
            });
        },
        loadUsers: function () {
            if (this.isProcessingLoadUsers || !this.calendarItem) {
                return;
            }
            this.isProcessingLoadUsers = true;
            this.userItems = [];
            var params = $.extend({}, this.filter, this.calendarItem);
            ls.ajax.load(aRouter.admin + 'ajax/distribution/teacher-users/', params, function (res) {
                this.userItems = res.users;
                this.count = res.count;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoadUsers = false;
                }.bind(this)
            });
        },
        loadCalendar: function () {
            if (this.isProcessingLoadCalendar) {
                return;
            }
            this.isProcessingLoadCalendar = true;
            this.userItems = [];
            this.calendarItems = [];
            this.teacher = null;
            this.calendarItem = null;
            var params = $.extend({}, this.filter, {});
            ls.ajax.load(aRouter.admin + 'ajax/distribution/teacher-calendar/', params, function (res) {
                this.calendarItems = res.calendarItems;
                this.teacher = res.teacher;
                this.countDenominator = res.countDenominator;
                this.countNumerator = res.countNumerator;
                this.scheduleItems = res.scheduleItems;
                this.addressesItems = res.addressesItems;
                this.sectionItems = res.sectionItems;
                this.facultyItems = res.facultyItems;
                this.medicalGroupItems = res.medicalGroupItems;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoadCalendar = false;
                }.bind(this)
            });
        },
        setCurrentTimeDay: function (time, day) {
            this.currentDay = day;
            this.currentTime = time;
        },
        dayOfWeek: function (num) {
            var result = null;
            _.forEach(this.dayItems, function (item, name) {
                if (item.id == num) {
                    result = item.title;
                }
            }.bind(this));
            return result;
        },
        showCount: function (count) {
            count = parseInt(count);
            if (count) {
                return count;
            }
            return 0;
        },
        countSlots: function (day, time, sectionId) {
            var count = 0;

            _.forEach(this.scheduleItems, function (scheduleItem, name) {
                if ((scheduleItem.time_id == time) && (scheduleItem.day_of_week == day)) {
                    if (!sectionId || (scheduleItem.section && sectionId == scheduleItem.section.id)) {
                        count++;
                    }
                }
            }.bind(this));

            return count;
        },
        countUserMax: function (day, time) {
            let count = 0;
            let unlimit = false;
            _.forEach(this.scheduleItems, function (scheduleItem, name) {
                if ((scheduleItem.time_id == time) && (scheduleItem.day_of_week == day)) {
                    if (scheduleItem.count_user_max) {
                        count = count + parseInt(scheduleItem.count_user_max);
                    } else {
                        unlimit = true;
                    }
                }
            }.bind(this));

            return unlimit ? false : count;
        },
        selectItem: function (time, day, type, count) {
            count = count || 0;
            if (!count) {
                this.calendarItem = null;
                this.userItems = [];
            } else {
                this.calendarItem = {
                    'time_id': time,
                    'day_id': day,
                    'type': type || '',
                    'count': count
                };
                this.loadUsers();
            }
        },
        addressTitle: function (address) {
            if (address.title) {
                return address.title + ' (' + address.address + ')';
            } else {
                return address.address;
            }
        },
        removeSchedule: function (item) {
            if (this.isProcessingScheduleRemove) {
                return;
            }
            this.isProcessingScheduleRemove = true;
            ls.ajax.load(aRouter.admin + 'ajax/distribution/teacher-schedule-remove/', {
                id: item.id
            }, function (res) {
                this.scheduleItems = res.items;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingScheduleRemove = false;
                }.bind(this)
            });
        },
        updateSchedule: function () {
            if (this.isProcessingScheduleUpdate) {
                return;
            }
            this.isProcessingScheduleUpdate = true;
            ls.ajax.load(aRouter.admin + 'ajax/distribution/teacher-schedule-update/', {
                id: this.scheduleItemUpdate.id,
                item: this.scheduleForm
            }, function (res) {
                this.loadCalendar();
                this.closeUpdateSchedule();
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingScheduleUpdate = false;
                }.bind(this)
            });
        },
        createSchedule: function () {
            if (this.isProcessingScheduleCreate) {
                return;
            }
            this.isProcessingScheduleCreate = true;
            ls.ajax.load(aRouter.admin + 'ajax/distribution/teacher-schedule-create/', {
                teacher_id: this.teacher.id,
                item: this.scheduleForm
            }, function (res) {
                this.loadCalendar();
                this.clearScheduleForm();
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingScheduleCreate = false;
                }.bind(this)
            });
        },
        openUpdateSchedule: function (item) {
            this.scheduleItemUpdate = item;
            this.isScheduleUpdateOpen = true;
            this.scheduleForm.time_id = item.time_id;
            this.scheduleForm.section_id = item.section ? item.section.id : null;
            this.scheduleForm.faculty_id = item.faculty ? item.faculty.id : null;
            this.scheduleForm.medical_group_id = item.medical_group ? item.medical_group.id : null;
            this.scheduleForm.address_id = item.address ? item.address.id : null;
            this.scheduleForm.count_user_max = item.count_user_max;
            this.scheduleForm.priority = item.priority;
            this.scheduleForm.day_of_week = item.day_of_week;
            this.scheduleForm.week_type = null;
            if (item.is_denominator) {
                this.scheduleForm.week_type = 'denominator';
            }
            if (item.is_numerator) {
                this.scheduleForm.week_type = 'numerator';
            }
        },
        clearScheduleForm: function () {
            this.scheduleForm.time_id = null;
            this.scheduleForm.day_of_week = null;
            this.scheduleForm.week_type = null;
            this.scheduleForm.section_id = null;
            this.scheduleForm.faculty_id = null;
            this.scheduleForm.medical_group_id = null;
            this.scheduleForm.address_id = null;
            this.scheduleForm.count_user_max = null;
            this.scheduleForm.priority = null;
        },
        closeUpdateSchedule: function () {
            this.isScheduleUpdateOpen = false;
            this.scheduleItemUpdate = null;
            this.clearScheduleForm();
        }
    }
});

$(function ($) {
    if ($('.js-app-branch-distribution-teacher').length) {
        $('.js-app-branch-distribution-teacher').each(function (k, v) {
            new Vue({
                el: v,
                data: {}
            });
        });
    }
});
Vue.component('app-branch-distribution-teacher-view', {
    template: '#app-template-branch-distribution-teacher-view',
    props: ['teacherId', 'branchId'],
    data: function () {
        return {
            isProcessingLoadUsers: false,
            isProcessingLoadCalendar: false,
            userItems: [],
            calendarItems: [],
            calendarItem: null,
            scheduleItems: [],
            teacher: null,
            countDenominator: 0,
            countNumerator: 0,
            currentDay: null,
            currentTime: null,
            timeItems: [
                {
                    id: 1,
                    title: '1 пара'
                },
                {
                    id: 2,
                    title: '2 пара'
                },
                {
                    id: 3,
                    title: '3 пара'
                },
                {
                    id: 4,
                    title: '4 пара'
                },
                {
                    id: 5,
                    title: '5 пара'
                },
                {
                    id: 6,
                    title: '6 пара'
                },
                {
                    id: 7,
                    title: '7 пара'
                }
            ],
            dayItems: [
                {
                    id: 1,
                    title: 'понедельник'
                },
                {
                    id: 2,
                    title: 'вторник'
                },
                {
                    id: 3,
                    title: 'среда'
                },
                {
                    id: 4,
                    title: 'четверг'
                },
                {
                    id: 5,
                    title: 'пятница'
                },
                {
                    id: 6,
                    title: 'суббота'
                },
                {
                    id: 7,
                    title: 'воскресенье'
                }
            ]
        };
    },
    watch: {
        'scheduleForm.week_type': function () {
            if (this.scheduleForm.week_type == 'denominator') {
                this.scheduleForm.is_denominator = 1;
                this.scheduleForm.is_numerator = 0;
            }
            if (this.scheduleForm.week_type == 'numerator') {
                this.scheduleForm.is_numerator = 1;
                this.scheduleForm.is_denominator = 0;
            }
            if (!this.scheduleForm.week_type) {
                this.scheduleForm.is_numerator = 0;
                this.scheduleForm.is_denominator = 0;
            }
        }
    },
    created: function () {
        this.loadCalendar();
    },
    computed: {
        countAll: function () {
            return this.countNumerator + this.countDenominator;
        },
        pathAssets: function () {
            return PATH_SKIN + '/assets';
        },
        getStaticPath: function () {
            return ls.registry.get('branch.url');
        },
        scheduleItemsFiltered: function () {
            if (this.currentDay && this.currentTime) {
                var result = [];
                _.forEach(this.scheduleItems, function (scheduleItem, name) {
                    if (scheduleItem.time_id == this.currentTime && scheduleItem.day_of_week == this.currentDay) {
                        result.push(scheduleItem);
                    }
                }.bind(this));
                return result;
            }
            return [];
        }
    },
    mounted: function () {

    },
    methods: {
        loadUsers: function () {
            if (this.isProcessingLoadUsers || !this.calendarItem) {
                return;
            }
            this.isProcessingLoadUsers = true;
            this.userItems = [];
            var params = $.extend({}, {
                teacher_id: this.teacherId,
                branch_id: this.branchId
            }, this.calendarItem);
            ls.ajax.load(aRouter.branch + 'ajax/teacher/users/', params, function (res) {
                this.userItems = res.users;
                this.count = res.count;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoadUsers = false;
                }.bind(this)
            });
        },
        loadCalendar: function () {
            if (this.isProcessingLoadCalendar) {
                return;
            }
            this.isProcessingLoadCalendar = true;
            this.userItems = [];
            this.calendarItems = [];
            this.calendarItem = null;
            ls.ajax.load(aRouter.branch + 'ajax/teacher/calendar/', {
                teacher_id: this.teacherId,
                branch_id: this.branchId
            }, function (res) {
                this.calendarItems = res.calendarItems;
                this.teacher = res.teacher;
                this.countDenominator = res.countDenominator;
                this.countNumerator = res.countNumerator;
                this.scheduleItems = res.scheduleItems;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoadCalendar = false;
                }.bind(this)
            });
        },
        setCurrentTimeDay: function (time, day) {
            this.currentDay = day;
            this.currentTime = time;
        },
        dayOfWeek: function (num) {
            var result = null;
            _.forEach(this.dayItems, function (item, name) {
                if (item.id == num) {
                    result = item.title;
                }
            }.bind(this));
            return result;
        },
        showCount: function (count) {
            count = parseInt(count);
            if (count) {
                return count;
            }
            return 0;
        },
        countSlots: function (day, time, sectionId) {
            var count = 0;

            _.forEach(this.scheduleItems, function (scheduleItem, name) {
                if ((scheduleItem.time_id == time) && (scheduleItem.day_of_week == day)) {
                    if (!sectionId || (scheduleItem.section && sectionId == scheduleItem.section.id)) {
                        count++;
                    }
                }
            }.bind(this));

            return count;
        },
        countUserMax: function (day, time) {
            let count = 0;
            let unlimit = false;
            _.forEach(this.scheduleItems, function (scheduleItem, name) {
                if ((scheduleItem.time_id == time) && (scheduleItem.day_of_week == day)) {
                    if (scheduleItem.count_user_max) {
                        count = count + parseInt(scheduleItem.count_user_max);
                    } else {
                        unlimit = true;
                    }
                }
            }.bind(this));

            return unlimit ? false : count;
        },
        selectItem: function (time, day, type, count) {
            count = count || 0;
            if (!count) {
                this.calendarItem = null;
                this.userItems = [];
            } else {
                this.calendarItem = {
                    'time_id': time,
                    'day_id': day,
                    'type': type || '',
                    'count': count
                };
                this.loadUsers();
            }
        },
        addressTitle: function (address) {
            if (address.title) {
                return address.title + ' (' + address.address + ')';
            } else {
                return address.address;
            }
        }
    }
});

$(function ($) {
    if ($('.js-app-branch-distribution-teacher-view').length) {
        $('.js-app-branch-distribution-teacher-view').each(function (k, v) {
            new Vue({
                el: v,
                data: {}
            });
        });
    }
});
Vue.component('app-branch-distribution-registration', {
    template: '#app-template-branch-distribution-registration',
    props: {
        id: null
    },
    data: function () {
        return {
            isProcessingLoad: false,
            isProcessingScheduleLoad: false,
            isProcessingSubmit: false,
            isCompleted: false,
            medicalGroupItems: [],
            selectMedicalGroup: null,
            selectSection: null,
            selectSectionMedical: null,
            sectionItems: [],
            sectionBaseItems: [],
            sportSections: [],
            manualOnly: false,
            section: null,
            sectionMedical: null,
            courseNumber: null,
            semesterNumber: null,
            isDebtor: false,
            medicalGroup: null,
            isScheduleSelect: false,
            scheduleItems: [],
            hasSectionFree: false,
            hasFreePlaceForSportSection: false,
            dayItems: [
                {
                    id: 1,
                    title: 'Понедельник'
                },
                {
                    id: 2,
                    title: 'Вторник'
                },
                {
                    id: 3,
                    title: 'Среда'
                },
                {
                    id: 4,
                    title: 'Четверг'
                },
                {
                    id: 5,
                    title: 'Пятница'
                },
                {
                    id: 6,
                    title: 'Суббота'
                },
                {
                    id: 7,
                    title: 'Воскресенье'
                }
            ]
        };
    },
    watch: {
        selectMedicalGroup: function () {
            if (this.selectMedicalGroup && this.selectSection) {
                this.loadSchedule();
            }
        },
        selectSection: function () {
            if (this.selectMedicalGroup && this.selectSection) {
                this.loadSchedule();
            }
        }
    },
    created: function () {
        this.loadData();
    },
    computed: {
        isReady: function () {
            return !this.isProcessingLoad;
        },
        isNotAllow: function () {
            if (this.courseNumber > 3 || this.courseNumber < 2) {
                return true;
            }
            return false;
        },
        isShowSchedule: function () {
            if (this.isNotAllow) {
                return false;
            }
            if (this.courseNumber == 1 && !this.isDebtor) {
                //return false;
            }
            if (this.isCompleted) {
                return false;
            }
            return true;
        },
        isShowScheduleSubmit: function () {
            if (!this.isShowSchedule) {
                return false;
            }
            if (!this.isDebtor) {
                if (this.selectMedicalGroup === null) {
                    return false;
                }
                if (this.manualOnly) {
                    return true;
                }
                if (this.selectSection === null && this.sectionBaseItems.length) {
                    return false;
                }
            }
            return true;
        },
        isShowSubmit: function () {
            if (this.isNotAllow) {
                return false;
            }
            if (this.isCompleted) {
                return false;
            }
            if (this.isDebtor) {
                return true;
            }
            if (this.isScheduleSelect) {
                return true;
            }
            return false;
        },
        sectionMedicalItems: function () {
            return _.filter(this.sectionItems, ['type', 'special_medical']);
        },
        isScheduleFull: function () {
            if (this.scheduleItems.length) {
                let notHave = false;
                _.forEach(this.scheduleItems, function (schedule, k) {
                    if (!schedule.teachers.length) {
                        notHave = true;
                    }
                });
                return !notHave;
            }
            return false;
        },
        isSportsman: function () {
            let isSportsman = false;
            if (this.selectSection && this.sportSections.length) {
                isSportsman = this.sportSections.indexOf(parseInt(this.selectSection)) != -1;
            }
            return isSportsman;
        }
    },
    mounted: function () {

    },
    methods: {
        loadData: function () {
            this.isProcessingLoad = true;
            ls.ajax.load(aRouter.branch + 'ajax/distribution-registration/data/', {id: this.id}, function (res) {
                this.medicalGroupItems = res.medicalGroupItems;
                this.sectionItems = res.sectionItems;
                this.sectionBaseItems = res.sectionBaseItems;
                this.courseNumber = res.courseNumber;
                this.semesterNumber = res.semesterNumber;
                this.sportSections = res.sportSections;
                this.isDebtor = res.isDebtor;
                this.manualOnly = res.manualOnly;
                this.hasFreePlaceForSportSection = res.hasFreePlaceForSportSection;
                if (this.isDebtor) {
                    this.loadSchedule();
                }
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoad = false;
                }.bind(this)
            });
        },
        loadSchedule: function () {
            this.isProcessingScheduleLoad = true;
            this.scheduleItems = [];
            ls.ajax.load(aRouter.branch + 'ajax/distribution-registration/data-schedule/', {
                id: this.id,
                medical_group: this.selectMedicalGroup,
                section: this.selectSection
            }, function (res) {
                this.scheduleItems = res.items;
                this.hasSectionFree = res.has_section_free;
                this.hasFreePlaceForSportSection = res.hasFreePlaceForSportSection;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingScheduleLoad = false;
                }.bind(this)
            });
        },
        setSchedule: function () {
            if (!this.isDebtor) {
                if (this.selectMedicalGroup === null) {
                    return false;
                }
                if (!this.manualOnly && this.selectSection === null && this.sectionBaseItems.length) {
                    return false;
                }
            }

            /**
             * Сразу отправляем на ручное распределение
             */
            if (this.manualOnly) {
                this.medicalGroup = this.selectMedicalGroup ? this.selectMedicalGroup : null;
                this.submit();
                return;
            }

            var needSelectTeacher = false;
            var hasSelectTeacher = false;
            var hasNotSelectTeacher = false;
            var hasTime = false;

            _.forEach(this.scheduleItems, function (schedule, k) {
                hasTime = true;
                if (!schedule.teacher_id) {
                    hasNotSelectTeacher = true;
                    var teachers = _.filter(schedule.teachers, function (teacher) {
                        return !!teacher.has_free_place;
                    });
                    if (teachers.length) {
                        needSelectTeacher = true;
                    }
                } else {
                    hasSelectTeacher = true;
                }
            }.bind(this));

            var isSportsman = false;
            if (this.selectSection && this.sportSections.length) {
                isSportsman = this.sportSections.indexOf(parseInt(this.selectSection)) != -1;
            }

            if (!isSportsman && this.hasFreePlaceForSportSection) {
                ls.msg.error('Вы спортсмен, вам необходимо выбрать свою спортивную секцию.');
                return false;
            }

            if (this.isScheduleFull) {
                if (needSelectTeacher) {
                    if (!isSportsman || !hasSelectTeacher) {
                        ls.msg.error('Необходимо выбрать преподавателя из доступных');
                        return false;
                    }
                }
            }

            if (hasNotSelectTeacher && this.hasSectionFree) {
                if (!(hasSelectTeacher && isSportsman)) {
                    ls.msg.error('Попробуйте выбрать другую секцию, где есть доступные преподаватели');
                    return false;
                }
            }

            if (!hasTime) {
                ls.msg.error('К сожалению, для вашей академической группы не составлено расписание на Кафедре ФВ. Попробуйте зайти позже или обратиться в 111 кабинет Кафедры ФВ. Если вы хотите доверить своё распределение преподавателям, оставьте заявку.');
                return false;
            }

            this.section = this.selectSection ? this.selectSection : null;
            this.medicalGroup = this.selectMedicalGroup ? this.selectMedicalGroup : null;
            this.isScheduleSelect = true;
            this.submit();
        },
        submit: function () {
            if (this.isProcessingSubmit) {
                return;
            }
            this.isProcessingSubmit = true;
            var data = {
                section: this.section,
                medical_group: this.medicalGroup,
                schedules: []
            };
            _.forEach(this.scheduleItems, function (item, name) {
                data.schedules.push({
                    time_id: item.time_id,
                    day_of_week: item.day_of_week,
                    is_denominator: item.is_denominator,
                    is_numerator: item.is_numerator,
                    teacher_id: item.teacher_id
                });
            }.bind(this));
            ls.ajax.load(aRouter.branch + 'ajax/distribution-registration/submit/', {id: this.id, item: data}, function (res) {
                this.isCompleted = true;
                $('.js-branch-registration-modal').hide();
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingSubmit = false;
                }.bind(this)
            });
        },
        dayOfWeek: function (num) {
            var result = null;
            _.forEach(this.dayItems, function (item, name) {
                if (item.id == num) {
                    result = item.title;
                }
            }.bind(this));
            return result;
        },
        cancel: function () {
            $('#modal-branch-registration').lsModal('hide');
        },
        countFreePlaceTeacher: function (teacher) {
            if (teacher.has_free_place) {
                if (teacher.count_max) {
                    var count = teacher.count_max - teacher.count_used;
                    if (count > 0) {
                        return count;
                    }
                    return 0;
                }
                return null;
            }
            return 0;
        }
    }
});

$(function ($) {
    if ($('.js-app-branch-distribution-registration').length) {
        $('.js-app-branch-distribution-registration').each(function (k, v) {
            new Vue({
                el: v,
                data: {}
            });
        });
    }
});
Vue.component('app-branch-user-search', {
    template: '#app-branch-template-user-search',
    data: function () {
        return {
            isProcessingSearch: false,
            inputSearch: '',
            showSearchResult: false,
            itemsSearchResult: []
        }
    },
    watch: {
        inputSearch: function (search) {
            if (search && search.length >= 2) {
                this.makeSearch();
            } else {
                this.showSearchResult = false;
                this.itemsSearchResult = [];
            }
        }
    },
    mounted: function () {

    },
    filters: {},
    methods: {
        makeSearch: _.debounce(
            function () {
                if (!this.inputSearch) {
                    this.showSearchResult = false;
                    this.itemsSearchResult = [];
                    this.isProcessingSearch = false;
                    return;
                }
                this.isProcessingSearch = true;
                ls.ajax.load(aRouter.admin + 'ajax/user/search', {
                    value: this.inputSearch
                }, function (res) {
                    if (this.inputSearch) {
                        this.showSearchResult = true;
                        this.itemsSearchResult = res.items;
                    }
                }.bind(this), {
                    onComplete: function () {
                        this.isProcessingSearch = false;
                    }.bind(this)
                });
            },
            1000
        ),
        closeSearch: function (e) {
            this.inputSearch = '';
            this.$refs.inputSearch.focus();
            if (e) {
                e.preventDefault();
            }
        },
        action: function (item) {
            this.$emit('action', {item: item});
        }
    }
});
Vue.use(window["vue-js-modal"].default);
var stashableFieldVueMixin = {
    props: {
        entity: {
            default: ''
        },
        id: {
            default: null
        },
        field: {
            default: null
        },
        title: {
            default: null
        },
        timeoutOtherSave: {
            default: 5000
        },
        timeoutOtherEdit: {
            default: 0
        },
        timeoutChange: {
            default: 5000
        },
        initValue: {
            default: null
        },
        initValueJson: {
            default: null
        },
        initStashed: {
            default: true
        },
        requestForEdit: {
            default: true
        },
        useWebsocket: {
            default: true
        },
        showIconEdit: {
            default: true
        }
    },
    data: function () {
        return {
            value: '',
            show: true,
            valueBeforeChange: '',
            fieldUid: null,
            isProcessingSave: false,
            isProcessingRequestEdit: false,
            stashed: true,
            channel: null,
            usersEdited: [],
            isOtherSave: false,
            userOtherSave: null,
            userOtherSaveTimeoutId: null,
            endAfterBeginTimeoutId: null,
            changeTimeoutActive: false,
            modalBlockInfoName: 'block-info.modal',
            userBlockInfo: null
        }
    },
    watch: {
        value: {
            handler: function (newValue) {

            },
            deep: true
        }
    },
    mounted: function () {

    },
    created: function () {
        if (this.initValueJson) {
            this.value = JSON.parse(this.initValueJson);
        } else if (this.initValue !== null) {
            this.value = this.initValue;
        } else if (this.id) {
            this.load();
        }
        this.emitFieldChange(true);

        this.modalBlockInfoName = this.modalBlockInfoName + this.generateUuid();
        this.fieldUid = this.generateUuid();
        this.stashed = this.initStashed;
        this.channel = md5(this.entity) + '(' + this.id + ')';
        var subscribeData = ls.registry.get('plugin.stashable.channel.' + this.channel);
        if (this.useWebsocket && ls.websocket) {
            ls.websocket.subscribe('/' + this.channel, function (data) {
                if (data.field == this.field && data.uid != this.fieldUid) {
                    this.actionProcessing(data);
                }
            }.bind(this), subscribeData || {});
        }

        $bus.$on('plugin-stashable:field-set-value', function (data) {
            if (data.entity == this.entity && data.id == this.id && data.field == this.field) {
                this.value = data.value;
                this.save();
            }
        }.bind(this));
        $bus.$on('plugin-stashable:field-hide', function (data) {
            if (data.entity == this.entity && data.id == this.id && data.field == this.field) {
                this.show = false;
            }
        }.bind(this));
        $bus.$on('plugin-stashable:field-show', function (data) {
            if (data.entity == this.entity && data.id == this.id && data.field == this.field) {
                this.show = true;
            }
        }.bind(this));
    },
    computed: {},
    methods: {
        showModalBlockInfo: function (user) {
            this.userBlockInfo = user;
            this.$modal.show(this.modalBlockInfoName);
        },
        closeModalBlockInfo: function () {
            this.$modal.hide(this.modalBlockInfoName);
        },
        onModalBlockInfoClosed: function () {
            this.userBlockInfo = null;
        },
        load: function () {
            this.emitFieldChange(true);
        },
        saveIfOpen: function () {
            if (!this.stashed) {
                this.save();
            }
        },
        save: function () {
            if (this.isProcessingSave) {
                return;
            }
            this.isProcessingSave = true;
            var params = {
                entity: this.entity,
                id: this.id,
                uid: this.fieldUid,
                field: this.field,
                value: this.value
            };
            ls.ajax.load('plugin-stashable/field-update', params, function (res) {
                this.closeEdit();
                this.value = res.value;
                this.callbackFileSave(res);
                this.emitFieldChange();
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingSave = false;
                }.bind(this),
                onError: function (res) {
                    if (res.sMsg) {
                        this.showError(res.sMsg);
                    }
                }.bind(this),
                showNotices: false
            });
        },
        callbackFileSave: function (res) {

        },
        cancel: function () {
            if (this.valueBeforeChange != this.value) {
                this.value = this.valueBeforeChange;
            }
            this.closeEdit();
        },
        changeWithTimeout: function (e) {
            if (e.keyCode == 27) {
                return;
            }
            if (this.changeTimeoutActive) {
                return;
            }
            this.changeTimeoutActive = true;
            this.change();
            setTimeout(function () {
                this.changeTimeoutActive = false;
            }.bind(this), parseInt(this.timeoutChange));
        },
        change: function () {
            this.notifyEditBegin();
        },
        generateUuid: function uuidv4() {
            return ([1e7] + -1e3 + -4e3 + -8e3 + -1e11).replace(/[018]/g, function (c) {
                return (c ^ crypto.getRandomValues(new Uint8Array(1))[0] & 15 >> c / 4).toString(16);
            });
        },
        requestEdit: function (callback) {
            if (this.isProcessingRequestEdit) {
                return;
            }
            this.isProcessingRequestEdit = true;
            var params = {
                entity: this.entity,
                id: this.id,
                uid: this.fieldUid,
                field: this.field
            };
            ls.ajax.load('plugin-stashable/field-request-edit', params, function (res) {
                if (res.data) {
                    $bus.$emit('plugin-stashable:modal-show', {
                        data: res.data, callbackOk: callback, callbackCancel: function () {
                            this.isProcessingRequestEdit = false;
                        }.bind(this)
                    });
                } else {
                    callback();
                }
            }.bind(this), {
                onError: function () {
                    this.isProcessingRequestEdit = false;
                }.bind(this),
                showProgress: false
            });
        },
        openEdit: function () {
            if (!this.stashed) {
                return;
            }
            var callbackRequestEdit = function () {
                this.isProcessingRequestEdit = false;
                this.stashed = false;
                this.valueBeforeChange = this.value;
                this.$nextTick(function () {
                    var field = $(this.$refs.field);
                    if (field.length) {
                        field.focus();
                    }
                }.bind(this));
                this.notifyEditBegin();
            }.bind(this);
            if (this.requestForEdit) {
                this.requestEdit(callbackRequestEdit);
            } else {
                callbackRequestEdit();
            }
        },
        closeEdit: function () {
            this.stashed = true;
            this.clearError();
            this.notifyEditEnd();
        },
        showError: function (msg) {
            var field = $(this.$refs.field);
            if (field.length) {
                field.lsValidationField();
                if (field.lsValidationField('isInit')) {
                    field.lsValidationField('showError', [msg]);
                }
            }
        },
        clearError: function (msg) {
            var field = $(this.$refs.field);
            if (field.length) {
                field.lsValidationField();
                if (field.lsValidationField('isInit')) {
                    field.lsValidationField('clearError');
                }
            }
        },
        notifyEditAction: function (action) {
            if (!this.id) {
                return;
            }
            ls.ajax.load('plugin-stashable/notify-edit-action', {
                entity: this.entity,
                id: this.id,
                field: this.field,
                uid: this.fieldUid,
                action: action,
                requestForEdit: this.requestForEdit ? 1 : 0
            }, function (res) {

            }.bind(this), {
                showProgress: false
            });
        },
        notifyEditBegin: function () {
            this.notifyEditAction('begin');
        },
        notifyEditEnd: function () {
            this.notifyEditAction('end');
        },
        actionProcessing: function (data) {
            if (data.action == 'edit_begin') {
                this.actionProcessingEditBegin(data);
            }
            if (data.action == 'edit_end') {
                this.actionProcessingEditEnd(data);
            }
            if (data.action == 'save') {
                this.actionProcessingSave(data);
            }
        },
        actionProcessingEditBegin: function (data) {
            if (this.requestForEdit && !this.stashed) {
                this.cancel();
                this.showModalBlockInfo(data.user);
            }
            var user = this.getUserEditedById(data.user.id);
            if (!user) {
                var timeout = parseInt(this.timeoutOtherEdit);
                this.usersEdited.push(data.user);
                if (timeout) {
                    this.endAfterBeginTimeoutId = setTimeout(function () {
                        this.actionProcessingEditEnd(data);
                    }.bind(this), timeout);
                }
            }
            if (user && this.endAfterBeginTimeoutId) {
                clearTimeout(this.endAfterBeginTimeoutId);
            }
        },
        actionProcessingEditEnd: function (data) {
            var index = this.getUserEditedIndexById(data.user.id);
            if (index > -1) {
                this.usersEdited.splice(index, 1);
            }
        },
        actionProcessingSave: function (data) {
            if (this.value != data.value) {
                this.value = data.value;
                this.valueBeforeChange = this.value;
                this.showAlertOtherUserSave(data.user);
            }
        },
        showAlertOtherUserSave: function (user) {
            this.userOtherSave = user;
            this.isOtherSave = true;
            if (this.userOtherSaveTimeoutId) {
                clearTimeout(this.userOtherSaveTimeoutId);
            }
            this.userOtherSaveTimeoutId = setTimeout(function () {
                this.userOtherSave = null;
                this.isOtherSave = false;
            }.bind(this), parseInt(this.timeoutOtherSave));
        },
        getUserEditedById: function (id) {
            var index = this.getUserEditedIndexById(id);
            if (index > -1) {
                return this.usersEdited[index];
            }
            return null;
        },
        getUserEditedIndexById: function (id) {
            var result = -1;
            _.forEach(this.usersEdited, function (user, index) {
                if (user.id == id) {
                    result = index;
                    return false;
                }
            });
            return result;
        },
        inArray: function (needle, haystack, strict) {
            var found = false, key, strict = !!strict;
            for (key in haystack) {
                if ((strict && haystack[key] === needle) || (!strict && haystack[key] == needle)) {
                    found = true;
                    break;
                }
            }
            return found;
        },
        emitFieldChange: function (init) {
            init = !!init;
            $bus.$emit('plugin-stashable:field-change', {init: init, entity: this.entity, id: this.id, field: this.field, value: this.value});
            this.$emit('field-change', {init: init, entity: this.entity, id: this.id, field: this.field, value: this.value});
        },
        escapeHtml: function (text) {
            var map = {
                '&': '&amp;',
                '<': '&lt;',
                '>': '&gt;',
                '"': '&quot;',
                "'": '&#039;'
            };
            return text.replace(/[&<>"']/g, function (m) {
                return map[m];
            });
        }
    }
};
Vue.component('app-plugin-stashable-field-text', {
    template: '#app-template-plugin-stashable-field-text',
    mixins: [stashableFieldVueMixin],
    props: {
        placeholder: {
            default: null
        },
        link: {
            default: false
        },
        renderText: {
            default: false
        }
    },
    data: function () {
        return {}
    },
    watch: {},
    mounted: function () {

    },
    created: function () {

    },
    computed: {
        render: function () {
            if (this.renderText) {
                return this.renderText;
            }
            return this.value;
        }
    },
    methods: {

    }
});

