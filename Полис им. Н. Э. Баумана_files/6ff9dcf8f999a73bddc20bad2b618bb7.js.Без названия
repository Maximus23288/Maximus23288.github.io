(function ($) {
    "use strict";

    $.widget("livestreet.lsBranchEventItemData", $.livestreet.lsComponent, {
        /**
         * Дефолтные опции
         */
        options: {
            // Ссылки
            urls: {
                // Добавить
                item: aRouter.branch + 'ajax/event-item/'
            },

            // Селекторы
            selectors: {},

            // Классы
            classes: {},

            // Параметры отправляемые при каждом аякс запросе
            params: {}
        },

        /**
         * Конструктор
         *
         * @constructor
         * @private
         */
        _create: function () {
            this._super();

            this.element.addClass('branch-event-item-data');
            this.element.addClass('branch-event-item-data-loaded');
            this._load('item', this.onLoadSuccess);
        },

        /**
         *
         */
        onLoadSuccess: function (response) {
            this.element.removeClass('branch-event-item-data-loaded');
            if (response.html) {
                this.element.html(response.html);
                $('.js-event-join-action').lsBranchEventJoin();
            } else {
                this.element.removeClass('branch-event-item-data');
            }
        }
    });
})(jQuery);
(function ($) {
    "use strict";

    $.widget("livestreet.lsBranchEventJoin", $.livestreet.lsComponent, {
        /**
         * Дефолтные опции
         */
        options: {
            // Ссылки
            urls: {
                // Добавить
                join: aRouter.branch + 'ajax/event-join/',
                modal: aRouter.branch + 'ajax/event-join-modal/'
            },

            // Селекторы
            selectors: {
                users: '@.js-event-join-users'
            },

            // Классы
            classes: {},

            // Параметры отправляемые при каждом аякс запросе
            params: {}

            // Коллбэки

            // После успешного изменения состояния
            // afterjoinsuccess: null
        },

        /**
         * Конструктор
         *
         * @constructor
         * @private
         */
        _create: function () {
            this._super();

            // Обработка кликов по кнопкам голосования
            this._on({click: '_join'});
        },

        _join: function () {
            if (this._getParam('request') && !parseInt(this._getParam('join'))) {
                var params = this.option('params');
                ls.modal.load(
                    this.options.urls.modal,
                    params,
                    {
                        aftershow: function (event, data) {
                            this._initModalForm();
                        }.bind(this),
                        afterhide: function (event, data) {

                        }.bind(this)
                    }
                );
            } else {
                this._load('join', this.onJoinSuccess);
            }
        },

        _initModalForm: function () {
            $('.js-event-join-request-submit').click(function () {
                var data = $('.js-event-join-request-form').serializeJSON();
                this._load('join', data, this.onJoinSuccess);
                $('#modal-event-join-request').lsModal('hide');
                return false;
            }.bind(this));
            $('.js-branch-datetime-picker').datetimepicker({
                format: 'Y-m-d'
            });
        },

        /**
         *
         */
        onJoinSuccess: function (response) {
            // Обновляем счетчик
            if (this.elements.users) {
                var text = '';
                if (response.count_user_max) {
                    if (response.count_users_available) {
                        text = 'Осталось мест ' + response.count_users_available + ' из ' + response.count_user_max;
                    } else {
                        text = 'Мест не осталось';
                    }
                } else {
                    if (response.count_users) {
                        text = 'Идёт ' + response.count_users;
                    } else {
                        text = 'Пока никто не идёт';
                    }
                }
                this.elements.users.text(text);
            }
            this.element.text(response.button_text);

            this._setParam('join', response.join);
            this._trigger('afterjoinsuccess', null, {context: this, response: response});
        }
    });
})(jQuery);
