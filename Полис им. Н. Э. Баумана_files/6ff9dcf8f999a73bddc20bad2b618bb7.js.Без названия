Vue.component('app-branch-event-user-schedule', {
    template: '#app-template-branch-event-user-schedule',
    props: ['branch'],
    data: function () {
        return {
            date: null,
            isProcessingLoad: false,
            items: []
        };
    },
    watch: {},
    created: function () {
        this.load();

        $bus.$on('branch-event-item:update', function (data) {
            this.load(true);
        }.bind(this));

        if (!this.date) {
            this.date = moment().format('YYYY-MM-DD')
        }
    },
    mounted: function () {

    },
    computed: {
        day: function () {
            return moment(this.date).format('D');
        },
        week: function () {
            return moment(this.date).format('dddd');
        },
        month: function () {
            return moment(this.date).format('MMMM');
        },
        year: function () {
            return moment(this.date).format('YYYY');
        },
        staticPath: function () {
            return PATH_SKIN;
        }
    },
    methods: {
        open: function () {
            $(".js-open-event").click(function () {
                $(this).toggleClass('opened');
                $(this).next(".event-item__content").toggle("fast");
            })
        },
        load: function (silent) {
            silent = silent || false;
            if (!silent) {
                this.isProcessingLoad = true;
            }
            ls.ajax.load(aRouter.branch + 'ajax/event/user-schedule/', {
                id: this.branch
            }, function (res) {
                this.items = res.items;
            }.bind(this), {
                onComplete: function () {
                    if (!silent) {
                        this.isProcessingLoad = false;
                    }
                }.bind(this)
            });
        },
        formatDateGroup: function (date) {
            return moment(date).format('D MMMM');
        }
    }
});

$(function ($) {
    if ($('.js-app-branch-event-user-schedule').length) {
        $('.js-app-branch-event-user-schedule').each(function (k, v) {
            new Vue({
                el: v,
                data: {}
            });
        });
    }
});
Vue.component('app-branch-event-calendar', {
    template: '#app-template-branch-event-calendar',
    props: ['branch'],
    data: function () {
        return {
            items: [],
            categories: [],
            today: null,
            month: null,
            monthLoad: null,
            activeItemDate: null,
            defaultDate: null,
            isAllowEventCreate: false,
            isProcessingLoad: false
        };
    },
    watch: {
        monthLoad: function (newVal, oldVal) {
            if (oldVal) {
                this.load(true);
            }
        }
    },
    created: function () {
        var date = this.getHashParam('date');
        if (date) {
            this.defaultDate = date;
        } else {
            this.defaultDate = moment().format('YYYY-MM-DD');
        }
        this.monthLoad = moment(this.defaultDate).format('YYYYMM');

        this.load();
        $bus.$on('branch-event-create:completed', function (data) {
            var date = moment(data.item.date_begin).format('YYYYMM');
            if (this.month == date) {
                this.load(true);
            }
        }.bind(this));
        $bus.$on('branch-event-item:remove', function (data) {
            this.load(true);
        }.bind(this));
    },
    mounted: function () {
        if (this.getHashParam('create')) {
            setTimeout(function () {
                this.showModalCreate();
            }.bind(this), 1000);
        }
    },
    methods: {
        load: function (silent) {
            silent = silent || false;
            if (!silent) {
                this.isProcessingLoad = true;
            }
            ls.ajax.load(aRouter.branch + 'ajax/event/calendar/', {id: this.branch, date: this.monthLoad}, function (res) {
                this.items = res.items;
                this.today = res.today;
                this.month = res.month;
                this.monthLoad = res.month;
                this.categories = res.categories;
                this.isAllowEventCreate = res.is_allow_event_create;

                if (!this.activeItemDate) {
                    this.setActiveDate(this.defaultDate);
                }
            }.bind(this), {
                onComplete: function () {
                    if (!silent) {
                        this.isProcessingLoad = false;
                    }
                }.bind(this)
            });
        },
        isPrevMonth: function (item) {
            if (item.month < this.month) {
                return true;
            }
            return false;
        },
        isNextMonth: function (item) {
            if (item.month > this.month) {
                return true;
            }
            return false;
        },
        isWeekend: function (item) {
            if (item.week >= 6) {
                return true;
            }
            return false;
        },
        isLastDayOfMonth: function (item) {
            if (item.day == item.month_days) {
                return true;
            }
            return false;
        },
        isFirstDayOfMonth: function (item) {
            if (item.day == 1) {
                return true;
            }
            return false;
        },
        isShowMonth: function (item, index) {
            if (item.day == 1 || index == 0) {
                return true;
            }
            return false;
        },
        isCurrentDay: function (item) {
            if (item.date == this.today) {
                return true;
            }
            return false;
        },
        isActive: function (item) {
            if (item.date == this.activeItemDate) {
                return true;
            }
            return false;
        },
        activeItem: function (item) {
            this.setActiveDate(item.date);
        },
        setActiveDate: function (date) {
            this.activeItemDate = date;
            $bus.$emit('branch-event-calendar:select', {date: this.activeItemDate});
            var dateMonth = moment(this.activeItemDate).format('YYYYMM');
            this.monthLoad = dateMonth;
        },
        showModalCreate: function () {
            $bus.$emit('branch-event-create-modal:show', {date_begin: moment(this.activeItemDate).format('YYYY-MM-DD HH:mm')});
        },
        getHashParams: function (hashString) {
            var hashParams = {};
            var e,
                a = /\+/g, // Regex for replacing addition symbol with a space
                r = /([^&;=]+)=?([^&;]*)/g,
                d = function (s) {
                    return decodeURIComponent(s.replace(a, " "));
                };
            hashString = hashString || window.location.hash.substring(1);

            while (e = r.exec(hashString))
                hashParams[d(e[1])] = d(e[2]);

            return hashParams;
        },
        getHashParam: function (name) {
            var params = this.getHashParams();
            return params[name];
        }
    }
});

$(function ($) {
    if ($('.js-app-branch-event-calendar').length) {
        $('.js-app-branch-event-calendar').each(function (k, v) {
            new Vue({
                el: v,
                data: {}
            });
        });
    }
});
Vue.component('app-branch-event-day', {
    template: '#app-template-branch-event-day',
    props: ['initDate', 'branch'],
    data: function () {
        return {
            date: null,
            items: [],
            isProcessingLoad: false
        };
    },
    watch: {
        date: function () {
            this.load();
        }
    },
    created: function () {
        if (this.initDate) {
            this.date = this.initDate;
        }

        $bus.$on('branch-event-day:update', function (data) {
            this.date = data.date;
        }.bind(this));

        $bus.$on('branch-event-item:update', function (data) {
            this.replaceItem(data.item);
        }.bind(this));

        $bus.$on('branch-event-item:remove', function (data) {
            this.removeItem(data.item);
        }.bind(this));

        $bus.$on('branch-event-create:completed', function (data) {
            if (!this.replaceItem(data.item)) {
                var date = moment(data.item.date_begin).format('YYYY-MM-DD');
                if (this.date == date) {
                    this.load();
                }
            }
        }.bind(this));

        $bus.$on('branch-event-calendar:select', function (data) {
            this.date = data.date;
        }.bind(this));
    },
    computed: {
        dateTitle: function () {
            if (this.date) {
                return (moment(this.date).format('LL'));
            }
            return null;
        }
    },
    mounted: function () {

    },
    methods: {
        load: function () {
            this.items = [];
            this.isProcessingLoad = true;
            ls.ajax.load(aRouter.branch + 'ajax/event/day/', {
                id: this.branch,
                date: this.date
            }, function (res) {
                this.items = res.items;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoad = false;
                }.bind(this)
            });
        },
        replaceItem: function (newItem) {
            var index = -1;
            for (var i = 0; i < this.items.length; i++) {
                if (this.items[i].id === newItem.id) {
                    index = i;
                    break
                }
            }
            if (index !== -1) {
                this.items.splice(index, 1, newItem);
                return true;
            }
            return false;
        },
        removeItem: function (removeItem) {
            var index = -1;
            for (var i = 0; i < this.items.length; i++) {
                if (this.items[i].id === removeItem.id) {
                    index = i;
                    break
                }
            }
            if (index !== -1) {
                this.items.splice(index, 1);
                return true;
            }
            return false;
        }
    }
});

$(function ($) {
    if ($('.js-app-branch-event-day').length) {
        $('.js-app-branch-event-day').each(function (k, v) {
            new Vue({
                el: v,
                data: {}
            });
        });
    }
});
Vue.component('app-branch-event-item', {
    template: '#app-template-branch-event-item',
    props: {
        item: {
            default: null
        },
        itemId: {
            default: null
        },
        viewType: {
            default: 'normal'
        },
        showActions: {
            default: true
        },
        showAdminActions: {
            default: true
        }
    },
    data: function () {
        return {
            isShowDescription: true
        };
    },
    watch: {
        date: function () {

        }
    },
    created: function () {
        if (this.itemId && !this.item) {
            this.loadItem();
        }
        if (this.viewType == 'minimal') {
            this.isShowDescription = false;
        }
        if (this.viewType == 'full') {
            $bus.$on('branch-event-create:completed', function (data) {
                if (data.item.id == this.item.id) {
                    this.item = data.item;
                }
            }.bind(this));
        }
    },
    computed: {
        countUsersMore: function () {
            if (this.item.is_allow_view_users && this.item.users.length) {
                return this.item.users_count_all - this.item.users.length;
            }
            return 0;
        },
        isHavePeriod: function () {
            if (this.item.date_end) {
                var dateBegin = moment(this.item.date_begin).format('YYYY-MM-DD');
                var dateEnd = moment(this.item.date_end).format('YYYY-MM-DD');
                if (dateBegin != dateEnd) {
                    return true;
                }
            }
            return false;
        },
        itemStyle: function () {
            return {
                boxShadow: 'inset 4px 0px 0px 0px ' + this.item.category.color
            }
        },
        dateBeginTime: function () {
            return moment(this.item.date_begin).format('HH:mm');
        },
        dateRegistrationEnd: function () {
            return this.item.date_registration_end ? moment(this.item.date_registration_end).format('LL HH:mm') : null;
        },
        dateBeginDay: function () {
            return moment(this.item.date_begin).format('LL');
        },
        dateEndTime: function () {
            return this.item.date_end ? moment(this.item.date_end).format('HH:mm') : null;
        },
        dateEndDay: function () {
            return this.item.date_end ? moment(this.item.date_end).format('LL') : null;
        },
        address: function () {
            if (this.item.address) {
                if (this.item.address.title && this.item.address.address) {
                    return this.item.address.title + ' (' + this.item.address.address + ')';
                }
                if (this.item.address.address) {
                    return this.item.address.address;
                }
                if (this.item.address.title) {
                    return this.item.address.title;
                }
                if (this.item.address_text) {
                    return this.item.address_text;
                }
            }
            return null;
        },
        isShowActions: function () {
            if (!this.showActions) {
                return false;
            }
            return true;
        },
        isShowAdminActions: function () {
            if (!this.showAdminActions) {
                return false;
            }
            if (this.viewType == 'minimal') {
                return false;
            }
            return true;
        }
    },
    mounted: function () {

    },
    methods: {
        joinToggle: function () {
            if (this.item.join_need_request && !this.item.join_current) {
                ls.modal.load(
                    aRouter.branch + 'ajax/event/join/modal/', {
                        id: this.item.id
                    }, {
                        aftershow: function (event, data) {
                            $('.js-event-join-request-submit').click(function () {
                                var data = $('.js-event-join-request-form').serializeJSON();
                                this._joinToggle(data, function () {
                                    $('#modal-event-join-request').lsModal('hide');
                                });
                                return false;
                            }.bind(this));
                            $('#modal-event-join-request').find('.js-branch-datetime-picker').datetimepicker({
                                format: 'Y-m-d',
                                timepicker: false,
                                dayOfWeekStart: 1,
                                scrollInput: false
                            });
                            $('.js-select2').select2();
                        }.bind(this),
                        afterhide: function (event, data) {

                        }.bind(this)
                    }
                );
            } else {
                this._joinToggle();
            }
        },
        _joinToggle: function (data, callback) {
            var formError = $('#modal-event-join-request').find('.js-form-error');
            formError.lsValidationField();
            formError.lsValidationField('clearError');
            var params = $.extend({}, data || {}, {
                id: this.item.id
            });
            ls.ajax.load(aRouter.branch + 'ajax/event/join/', params, function (res) {
                if ($.isFunction(callback)) callback.apply(this, [res.item]);
                $bus.$emit('branch-event-item:update', {
                    item: res.item
                });
                if (this.viewType == 'full') {
                    this.item = res.item;
                }
            }.bind(this), {
                onComplete: function () {

                }.bind(this),
                onError: function (res) {
                    if (res.errors) {
                        _.forEach(res.errors, function (errors, fieldName) {
                            var input = $('#modal-event-join-request').find('[name="' + fieldName + '"]');
                            if (input.length) {
                                input.lsValidationField();
                                if (input.lsValidationField('isInit')) {
                                    input.lsValidationField('showError', errors);
                                }
                            }
                        }.bind(this));
                    }
                    if (res.sMsg) {
                        if (formError.length) {
                            formError.lsValidationField('showError', [res.sMsg]);
                        } else {
                            ls.msg.error(res.sMsg);
                        }
                    }
                }.bind(this),
                showNotices: false
            });
        },
        showModalUpdate: function () {
            $bus.$emit('branch-event-create-modal:show', {
                item: this.item
            });
        },
        downloadXls: function () {
            ls.ajax.load(aRouter.branch + 'ajax/event/export-xls', {
                id: this.item.id
            }, function (res) {
                if (res.file) {
                    window.location = res.file;
                }
            }.bind(this));
        },
        remove: function () {
            if (!confirm('Действительно удалить?')) {
                return;
            }
            ls.ajax.load(aRouter.branch + 'ajax/event/remove/', {
                id: this.item.id
            }, function (res) {
                $bus.$emit('branch-event-item:remove', {
                    item: this.item
                });
                if (this.viewType == 'full') {
                    window.location = '/events/';
                }
            }.bind(this), {
                onComplete: function () {

                }.bind(this)
            });
        },
        showModalCopy: function () {
            $bus.$emit('branch-event-create-modal:show', {
                copy_id: this.item.id
            });
        },
        loadItem: function () {
            ls.ajax.load(aRouter.branch + 'ajax/event/get', {
                id: this.itemId
            }, function (res) {
                if (res.item) {
                    this.item = res.item;
                }
            }.bind(this));
        }
    }
});

$(function(){
    if ($('.js-app-branch-event-item').length) {
        $('.js-app-branch-event-item').each(function (k, v) {
            new Vue({
                el: v,
                data: {}
            });
        });
    }
});
Vue.component('app-branch-event-list', {
    template: '#app-template-branch-event-list',
    props: ['branch', 'user'],
    data: function () {
        return {
            items: [],
            filter: {
                user: null,
                branch: null
            },
            isProcessingLoad: false,
            page: 1,
            pageCount: 0,
            pageCurrent: 1
        };
    },
    watch: {
        date: function () {
            this.load();
        }
    },
    created: function () {
        if (this.branch) {
            this.filter.branch = this.branch;
        }
        if (this.user) {
            this.filter.user = this.user;
        }
        $bus.$on('branch-event-item:update', function (data) {
            this.replaceItem(data.item);
        }.bind(this));

        $bus.$on('branch-event-item:remove', function (data) {
            this.removeItem(data.item);
        }.bind(this));

        this.load();
    },
    computed: {},
    mounted: function () {

    },
    methods: {
        load: function () {
            this.items = [];
            this.isProcessingLoad = true;
            var params = $.extend({}, this.filter, {
                page: this.page
            });
            ls.ajax.load('event/list', params, function (res) {
                this.items = res.items;
                if (res.paging) {
                    this.pageCount = res.paging.iCountPage;
                    this.pageCurrent = res.paging.iCurrentPage;
                }
                if (this.page > 1) {
                    this.scrollTop();
                }
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoad = false;
                }.bind(this)
            });
        },
        selectPage: function (data) {
            this.page = data.page;
            this.load();
        },
        replaceItem: function (newItem) {
            var index = -1;
            for (var i = 0; i < this.items.length; i++) {
                if (this.items[i].id === newItem.id) {
                    index = i;
                    break
                }
            }
            if (index !== -1) {
                this.items.splice(index, 1, newItem);
                return true;
            }
            return false;
        },
        removeItem: function (removeItem) {
            var index = -1;
            for (var i = 0; i < this.items.length; i++) {
                if (this.items[i].id === removeItem.id) {
                    index = i;
                    break
                }
            }
            if (index !== -1) {
                this.items.splice(index, 1);
                return true;
            }
            return false;
        },
        scrollTop: function () {
            var el = this.$refs['view-top'];
            $.scrollTo(el, 200, {
                offset: {
                    top: -50
                }
            });
        }
    }
});

