Vue.use(window["vue-js-modal"].default);
Vue.component('app-editor-block-video', {
    template: '#app-template-editor-block-video',
    props: ['initData', 'initParams', 'api', 'uuid'],
    components: {},
    data: function () {
        return {
            modalName: null,
            isProcessingPreview: false,
            isProcessingLoad: false,
            isVisible: false,
            isShowModal: false,
            url: null,
            item: null,
            itemPreview: null
        };
    },
    watch: {
        url: function (val) {
            if (this.item && this.item != val) {
                this.itemPreview = null;
            }
        }
    },
    created: function () {
        this.modalName = this.generateUuid();
    },
    mounted: function () {
        this.processingData(this.initData);

        if (this.url) {
            this.isVisible = true;
            this.load();
        } else {
            this.showModal();
        }

        this.$nextTick(function () {
            this.$emit('onRender', {
                isNew: !this.item
            });
        }.bind(this));
    },
    computed: {
        staticPath: function () {
            return PATH_SKIN;
        }
    },
    methods: {
        save: function () {
            return {
                url: this.url
            }
        },
        validate: function (data) {
            if (data.url) {
                return true;
            }
            return false;
        },
        settings: function () {
            return [{
                class: 'component-editor-btn_edit',
                data: {
                    action: 'edit'
                }
            }];
        },
        button: function () {
            return {
                class: 'btn-create-block btn-create-block_video',
                title: t('Добавить видео')
            }
        },
        onSettingsItem: function (data) {
            if (data.action == 'edit') {
                this.showModal();
            }
        },
        processingData: function (data) {
            if (typeof data !== 'object') {
                return;
            }
            this.url = data.url || null;
        },

        load: function () {
            this.isProcessingLoad = true;
            ls.ajax.load('plugin-editor/video/info', {
                url: this.url
            }, function (res) {
                this.item = res.item;
                if (this.isShowModal) {
                    this.closeModal();
                }
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoad = false;
                }.bind(this)
            });
        },
        loadPreview: function () {
            this.itemPreview = null;
            if (this.url) {
                this.isProcessingPreview = true;
                ls.ajax.load('plugin-editor/video/info', {
                    url: this.url
                }, function (res) {
                    this.itemPreview = res.item;
                }.bind(this), {
                    onComplete: function () {
                        this.isProcessingPreview = false;
                    }.bind(this)
                });
            }
        },
        showModal: function () {
            this.itemPreview = null;
            this.$modal.show(this.modalName);
        },
        closeModal: function () {
            this.$modal.hide(this.modalName);
        },
        onModalOpened: function () {
            this.isShowModal = true;
            this.loadPreview();
        },
        onModalClosed: function () {
            if (this.item) {
                this.isVisible = true;
                this.url = this.item.url;
            } else {
                this.api.remove(this.uuid);
            }
            this.isShowModal = false;
        },
        generateUuid: function uuidv4() {
            return ([1e7] + -1e3 + -4e3 + -8e3 + -1e11).replace(/[018]/g, function (c) {
                return (c ^ crypto.getRandomValues(new Uint8Array(1))[0] & 15 >> c / 4).toString(16);
            });
        },
        add: function () {
            this.load();
        }
    }
});
$.widget("livestreet.blockEditor", $.livestreet.lsComponent, {
    /**
     * Дефолтные опции
     */
    options: {
        // Селекторы
        selectors: {},
        data: {},
        field: 'text',
        blocks: {

        }
    },

    editor: null,
    /**
     * Конструктор
     *
     * @constructor
     * @private
     */
    _create: function () {
        this._super();

        var form = this.element.parents('form');
        if (form.length) {
            var blockEditors = form.data('blockEditors') || [];
            blockEditors.push(this);
            form.data('blockEditors', blockEditors);
        }

        this.editor = new Vue({
            el: this.element.get(0),
            data: {
                data: this.option('data'),
                field: this.option('field'),
                blocks: this.option('blocks')
            }
        });
    },

    save: function () {
        return this.editor.$refs.editor.save();
    },
    field: function () {
        return this.option('field');
    }
});
Vue.component('app-media-library', {
    template: '#app-template-media-library',
    props: {
        targetId: {
            default: null
        },
        targetType: {
            default: null
        },
        fieldSaveName: {
            default: null
        },
        ownerKey: {
            default: null
        },
        allowSelectMany: {
            default: true
        },
        allowRemoveItem: {
            default: false
        },
        softSelectMany: {
            default: true
        },
        sortDesc: {
            default: true
        },
        showFilterOwner: {
            default: true
        },
        showFilterDate: {
            default: true
        },
        titleButtonOk: {
            default: 'Вставить'
        },
        initSelectedItems: {
            default: function () {
                return [];
            }
        },
        showSettings: {
            default: true
        },
        showActions: {
            default: true
        },
        showSelected: {
            default: true
        },
        initImageSettings: {
            default: function () {
                return {
                    size: 'original'
                };
            }
        },
        initGallerySettings: {
            default: function () {
                return {
                    showPreview: false
                };
            }
        }
    },
    data: function () {
        return {
            filterLoad: {
                date: '',
                search: '',
                type: 'target'
            },
            imageSettings: null,
            gallerySettings: null,
            dateItems: [],
            imageSizeItems: [],
            activeItem: null,
            selectedItems: [],
            selectedItemsId: [],
            isProcessingLoad: false,
            isShowSelected: false,
            items: [],
            errors: [],
            page: 1,
            pageCount: 0,
            pageCurrent: 1
        }
    },
    watch: {
        filterLoad: {
            handler: function (newValue) {
                this.load();
            },
            deep: true
        },
        selectedItemsId: function (newValue) {
            if (!newValue.length) {
                this.isShowSelected && this.closeSelected();
            }
        },
        items: function () {
            if (this.fieldSaveName) {
                var dataForSave = [];
                _.forEach(this.items, function (item, index) {
                    dataForSave.push(item.id);
                }.bind(this));
                $(this.$refs.fieldSave).val(JSON.stringify(dataForSave));
            }
        }
    },
    created: function () {
        this.imageSettings = _.cloneDeep(this.initImageSettings);
        this.gallerySettings = _.cloneDeep(this.initGallerySettings);

        if (this.initSelectedItems && this.initSelectedItems.length) {
            this.selectedItems = _.cloneDeep(this.initSelectedItems);
            _.forEach(this.selectedItems, function (item, index) {
                this.selectedItemsId.push(item.id);
            }.bind(this));
            this.activeItem = this.selectedItems[0];
            this.makeShowSelected();
        }
        this.load();
    },
    destroyed: function () {

    },
    mounted: function () {
        var fileupload = {
            url: aRouter.ajax + 'plugin-media/upload/',
            sequentialUploads: false,
            singleFileUploads: true,
            limitConcurrentUploads: 2
        };

        var uploader = $(this.$refs['file-input']);
        fileupload.dropZone = $(this.$refs['upload-area']);
        uploader.fileupload(fileupload);
        uploader.fileupload('option', 'formData', {
            security_ls_key: LIVESTREET_SECURITY_KEY,
            target_type: this.targetType,
            target_id: this.targetId,
            owner_key: this.ownerKey
        });
        uploader.on({
            fileuploadadd: this.onUploadAdd.bind(this),
            fileuploadsubmit: function () {

            },
            fileuploadstart: function () {

            },
            fileuploadchange: function () {

            },
            fileuploaddone: function (event, data) {
                this[data.result.bStateError ? 'onUploadError' : 'onUploadDone'](data);
            }.bind(this),
            fileuploadfail: function (event, data) {
                this['onUploadError'](data);
            }.bind(this),
            fileuploadprogress: function (event, data) {
                this.onUploadProgress(data.context, parseInt(data.loaded / data.total * 100, 10));
            }.bind(this)
        });
    },
    filters: {},
    computed: {
        showFilter: function () {
            return !this.isShowSelected;
        },
        showProcessingLoad: function () {
            return this.isProcessingLoad && !this.isShowSelected;
        },
        itemsShow: function () {
            if (this.isShowSelected) {
                return this.selectedItems;
            } else {
                return this.items;
            }
        }
    },
    methods: {
        onUploadAdd: function (event, file) {
            var item = {
                'id': this.generateUid(),
                'uploading': true,
                'progress': 0
            };
            file.context = item.id;
            this.items.unshift(item);
        },
        onUploadError: function (data) {
            var item = this.getItemById(data.context);
            this.removeItemSoft(item);
            if (!data.result) {
                this.addError('Не удалось загрузить файл. Попробуйте позже.');
            } else {
                this.addError(data.result.sMsg || data.result.sMsgTitle);
            }
        },
        onUploadDone: function (data) {
            var index = this.getItemIndexById(data.context);
            this.$set(this.items, index, data.result.item);
        },
        onUploadProgress: function (context, percent) {
            var item = this.getItemById(context);
            item.progress = percent;
        },
        generateUid: function () {
            var text = "";
            var possible = "abcdefghijklmnopqrstuvwxyz0123456789";

            for (var i = 0; i < 10; i++) {
                text += possible.charAt(Math.floor(Math.random() * possible.length));
            }
            return text;
        },
        getItemById: function (id) {
            var item = null;
            _.forEach(this.items, function (v, index) {
                if (v.id == id) {
                    item = v;
                    return false;
                }
            });
            return item;
        },
        getItemIndexById: function (id) {
            var indexReturn = null;
            _.forEach(this.items, function (v, index) {
                if (v.id == id) {
                    indexReturn = index;
                    return false;
                }
            });
            return indexReturn;
        },
        getItemIndexByIdFromItemsShow: function (id) {
            var indexReturn = null;
            _.forEach(this.itemsShow, function (v, index) {
                if (v.id == id) {
                    indexReturn = index;
                    return false;
                }
            });
            return indexReturn;
        },
        addItem: function (data) {
            var item = {
                'id': data.id,
                'preview': data.preview,
                'info': data.info
            };
            this.items.push(item);
        },
        removeItemSoft: function (item) {
            _.forEach(this.items, function (v, index) {
                if (v.id == item.id) {
                    Vue.delete(this.items, index);
                    return false;
                }
            }.bind(this));
        },
        removeItem: function (item) {
            if (!confirm('Действительно удалить?')) {
                return;
            }
            this.removeItemSoft(item);
            ls.ajax.load('plugin-media/remove', {
                id: item.id,
                target_id: this.targetId,
                target_type: this.targetType
            }, function (res) {

            }.bind(this));
        },
        actionCancel: function () {
            this.$emit('action-cancel', {});
        },
        actionOk: function () {
            this.$emit('action-ok', {type: 'media', items: this.selectedItems, settings: this.imageSettings});
        },
        actionOkGallery: function () {
            this.$emit('action-ok', {type: 'gallery', items: this.selectedItems, settings: this.gallerySettings});
        },
        addError: function (msg) {
            this.errors.push({msg: msg});
        },
        isSelectedItem: function (item) {
            if (this.selectedItemsId.indexOf(item.id) > -1) {
                return true;
            }
            return false;
        },
        isActiveItem: function (item) {
            if (this.activeItem && this.activeItem.id == item.id) {
                return true;
            }
            return false;
        },
        setActive: function (item, shiftKey) {
            shiftKey = shiftKey || false;
            var selectModeMany = this.allowSelectMany && (!this.softSelectMany || shiftKey);
            if (this.isActiveItem(item)) {
                if (selectModeMany) {
                    // ничего не делаем
                } else {
                    // снимаем выделение
                    this.removeFromSelected(item);
                }
            } else {
                if (this.isSelectedItem(item)) {
                    this.activeItem = item;
                } else {
                    if (!selectModeMany) {
                        this.selectedItems = [];
                        this.selectedItemsId = [];
                    }
                    /**
                     * Выделение диапазона
                     */
                    if (this.activeItem && selectModeMany && shiftKey) {
                        var indexCurrentActive = this.getItemIndexByIdFromItemsShow(this.activeItem.id);
                        var indexItem = this.getItemIndexByIdFromItemsShow(item.id);
                        if (indexCurrentActive != null) {
                            var range = _.range(Math.min(indexCurrentActive, indexItem) + 1, Math.max(indexCurrentActive, indexItem));
                            if (range && range.length) {
                                for (var i = 0; i < range.length; ++i) {
                                    if (!this.isSelectedItem(this.items[range[i]])) {
                                        this.setSelected(this.items[range[i]]);
                                    }
                                }
                            }
                        }
                    }
                    this.setSelected(item);
                    this.activeItem = item;
                }
            }
        },
        removeFromSelected: function (item) {
            var isActive = this.isActiveItem(item);
            this.selectedItems.splice(this.selectedItems.indexOf(item), 1);
            this.selectedItemsId.splice(this.selectedItemsId.indexOf(item.id), 1);
            if (isActive) {
                // удаляем из активного
                this.activeItem = null;
                // ищет новый для активного
                if (this.selectedItems.length) {
                    this.$nextTick(function () {
                        this.activeItem = this.selectedItems[this.selectedItems.length - 1];
                    }.bind(this));
                }
            }
        },
        setSelected: function (item) {
            this.selectedItems.push(item);
            this.selectedItemsId.push(item.id);
        },
        resetItems: function () {
            this.selectedItems = [];
            this.selectedItemsId = [];
            this.activeItem = null;
        },
        load: function () {
            this.isProcessingLoad = true;
            this.items = [];
            var parts = {};
            if (!this.dateItems.length) {
                parts.datesMonth = 1;
                parts.imageSizes = 1;
            }
            ls.ajax.load('plugin-media/load', {
                filter: this.filterLoad,
                target_type: this.targetType,
                target_id: this.targetId,
                owner_key: this.ownerKey,
                page: this.page,
                sort_way: this.sortDesc ? 'desc' : 'asc',
                parts: parts
            }, function (res) {
                this.items = res.items;
                if (res.datesMonth) {
                    this.dateItems = res.datesMonth;
                }
                if (res.imageSizes) {
                    this.imageSizeItems = res.imageSizes;
                }
                if (res.paging) {
                    this.pageCount = res.paging.iCountPage;
                    this.pageCurrent = res.paging.iCurrentPage;
                }
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoad = false;
                }.bind(this)
            });
        },
        selectPage: function (data) {
            this.page = data.page;
            this.load();
        },
        makeShowSelected: function () {
            this.isShowSelected = true;
        },
        closeSelected: function () {
            this.isShowSelected = false;
        },
        getItemSizes: function (item) {
            var result = [];
            if (item && item.sizes) {
                _.forEach(item.sizes, function (v, code) {
                    result.push(
                        {
                            title: (this.imageSizeItems[code]) ? this.imageSizeItems[code].title : code,
                            size: v.w + 'x' + (v.h ? v.h : ''),
                            code: code
                        }
                    );
                }.bind(this));
            }
            return result;
        },
        getPreview: function (item, size) {
            size = size || 'preview-medium';
            if (item.sizes[size]) {
                var data = item.sizes[size];
                var s = data.w + 'x' + (data.h ? data.h : '');
                if (data.crop) {
                    s = s + 'crop';
                }
                var path = ls.utils.pathinfo(item.image);
                return path.dirname + '/' + path.filename + '_' + s + '.' + path.extension;
            }
            return item.image;
        },
        copyTextToClipboard: function (text) {
            ls.utils.copyTextToClipboard(text);
        },
        updateItemField: function (item, field) {
            ls.ajax.load('plugin-media/update-field', {
                id: item.id,
                field: field,
                value: item[field]
            }, function (res) {

            });
        },
        pluralize: function (count, words, skip) {
            return ls.lang.pluralize(count, words, null, skip);
        }
    }
});
Vue.component('app-media-library-preview', {
    template: '#app-template-media-library-preview',
    props: {
        targetId: {
            default: null
        },
        targetType: {
            default: null
        },
        ownerKey: {
            default: null
        },
        fieldName: {
            default: 'preview_id'
        }
    },
    data: function () {
        return {
            item: null,
            modalName: null,
            isProcessing: false
        };
    },
    created: function () {
        this.modalName = this.generateUuid();
        if (this.targetId) {
            this.load();
        }
    },
    mounted: function () {

    },
    methods: {
        showModal: function () {
            this.$modal.show(this.modalName);
        },
        closeModal: function () {
            this.$modal.hide(this.modalName);
        },
        onModalOpened: function () {
            this.isShowModal = true;
        },
        onModalClosed: function () {
            this.isShowModal = false;
        },
        actionCancel: function () {
            this.closeModal();
        },
        actionOk: function (data) {
            if (data.items.length) {
                var media = data.items[0];
                // запускаем процесс создания превью
                this.createPreview(media);
            }
            this.closeModal();
        },
        createPreview: function (media) {
            this.isProcessing = true;
            ls.ajax.load('plugin-media/create-preview', {
                id: media.id,
                target_id: this.targetId,
                target_type: this.targetType,
                owner_key: this.ownerKey
            }, function (res) {
                this.item = res.item;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessing = false;
                }.bind(this)
            });
        },
        remove: function () {
            if (!this.item) {
                return;
            }
            ls.ajax.load('plugin-media/remove-preview', {
                id: this.item.id,
                target_id: this.targetId,
                target_type: this.targetType
            }, function (res) {
                this.item = null;
            }.bind(this));
        },
        load: function () {
            this.isProcessing = true;
            ls.ajax.load('plugin-media/load-preview', {
                target_type: this.targetType,
                target_id: this.targetId,
                owner_key: this.ownerKey
            }, function (res) {
                this.item = res.item;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessing = false;
                }.bind(this)
            });
        },
        generateUuid: function uuidv4() {
            return ([1e7] + -1e3 + -4e3 + -8e3 + -1e11).replace(/[018]/g, function (c) {
                return (c ^ crypto.getRandomValues(new Uint8Array(1))[0] & 15 >> c / 4).toString(16);
            });
        }
    }
});

$(function ($) {
    if ($('.js-app-media-library-preview').length) {
        $('.js-app-media-library-preview').each(function (k, v) {
            new Vue({
                el: v,
                data: {}
            });
        });
    }
});
jQuery(document).ready(function ($) {
    /**
     * Fotorama
     */
    $( '.js-photoset' ).livequery(function() {
        $( this ).lsSlider();
    });
});
Vue.use(window["vue-js-modal"].default);
Vue.component('app-album-show', {
    template: '#app-template-album-show',
    props: {
        albumId: {
            default: null
        }
    },
    data: function () {
        return {
            modalName: null,
            isShowModal: false,
            isProcessingLoad: false,
            items: [],
            itemForModal: null,
            album: null,
            errors: [],
            allowUpdate: false
        }
    },
    watch: {},
    created: function () {
        this.modalName = this.generateUid();
        this.load();
    },
    destroyed: function () {

    },
    mounted: function () {
        var fileupload = {
            url: aRouter.ajax + 'plugin-album/photo/upload/',
            sequentialUploads: false,
            singleFileUploads: true,
            limitConcurrentUploads: 2
        };

        var uploader = $(this.$refs['file-input']);
        fileupload.dropZone = $(this.$refs['upload-area']);
        uploader.fileupload(fileupload);
        uploader.fileupload('option', 'formData', {
            security_ls_key: LIVESTREET_SECURITY_KEY,
            album_id: this.albumId
        });
        uploader.on({
            fileuploadadd: this.onUploadAdd.bind(this),
            fileuploadsubmit: function () {

            },
            fileuploadstart: function () {

            },
            fileuploadchange: function () {

            },
            fileuploaddone: function (event, data) {
                this[data.result.bStateError ? 'onUploadError' : 'onUploadDone'](data);
            }.bind(this),
            fileuploadfail: function (event, data) {
                this['onUploadError'](data);
            }.bind(this),
            fileuploadprogress: function (event, data) {
                this.onUploadProgress(data.context, parseInt(data.loaded / data.total * 100, 10));
            }.bind(this)
        });
    },
    filters: {},
    computed: {
        itemsShow: function () {
            return this.items;
        },
        showProcessingLoad: function () {
            return this.isProcessingLoad;
        }
    },
    methods: {
        onUploadAdd: function (event, file) {
            var item = {
                'id': this.generateUid(),
                'uploading': true,
                'progress': 0
            };
            file.context = item.id;
            this.items.unshift(item);
        },
        onUploadError: function (data) {
            var item = this.getItemById(data.context);
            this.removeItemSoft(item);
            if (!data.result) {
                this.addError('Не удалось загрузить файл. Попробуйте позже.');
            } else {
                this.addError(data.result.sMsg || data.result.sMsgTitle);
            }
        },
        onUploadDone: function (data) {
            var index = this.getItemIndexById(data.context);
            this.$set(this.items, index, data.result.item);
            this.album = data.result.album;
        },
        onUploadProgress: function (context, percent) {
            var item = this.getItemById(context);
            item.progress = percent;
        },
        generateUid: function () {
            var text = "";
            var possible = "abcdefghijklmnopqrstuvwxyz0123456789";

            for (var i = 0; i < 10; i++) {
                text += possible.charAt(Math.floor(Math.random() * possible.length));
            }
            return text;
        },
        getItemById: function (id) {
            var item = null;
            _.forEach(this.items, function (v, index) {
                if (v.id == id) {
                    item = v;
                    return false;
                }
            });
            return item;
        },
        getItemIndexById: function (id) {
            var indexReturn = null;
            _.forEach(this.items, function (v, index) {
                if (v.id == id) {
                    indexReturn = index;
                    return false;
                }
            });
            return indexReturn;
        },
        getItemIndexByIdFromItemsShow: function (id) {
            var indexReturn = null;
            _.forEach(this.itemsShow, function (v, index) {
                if (v.id == id) {
                    indexReturn = index;
                    return false;
                }
            });
            return indexReturn;
        },
        addItem: function (data) {
            var item = {
                'id': data.id,
                'preview': data.preview,
                'info': data.info
            };
            this.items.push(item);
        },
        removeItemSoft: function (item) {
            _.forEach(this.items, function (v, index) {
                if (v.id == item.id) {
                    Vue.delete(this.items, index);
                    return false;
                }
            }.bind(this));
        },
        removeItem: function (item) {
            if (!confirm('Действительно удалить?')) {
                return;
            }
            this.removeItemSoft(item);
            ls.ajax.load('plugin-album/photo/remove', {
                id: item.id,
                album_id: this.albumId
            }, function (res) {
                this.album = res.album;
                this.closeModal();
            }.bind(this));
        },
        addError: function (msg) {
            this.errors.push({
                msg: msg
            });
        },
        load: function () {
            this.isProcessingLoad = true;
            this.items = [];
            var parts = {};
            ls.ajax.load('plugin-album/photo/load-items', {
                album_id: this.albumId,
                parts: parts
            }, function (res) {
                this.items = res.items || [];
                this.album = res.album;
                this.allowUpdate = !!res.allow_update;
                this.$nextTick(function () {
                    this.initMasonry();
                }.bind(this));
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoad = false;
                }.bind(this)
            });
        },
        getItemSizes: function (item) {
            var result = [];
            if (item && item.sizes) {
                _.forEach(item.sizes, function (v, code) {
                    result.push({
                        title: (this.imageSizeItems[code]) ? this.imageSizeItems[code].title : code,
                        size: v.w + 'x' + (v.h ? v.h : ''),
                        code: code
                    });
                }.bind(this));
            }
            return result;
        },
        isPreview: function (item) {
            if (this.album.preview_id == item.id) {
                return true;
            }
            return false;
        },
        makePreview: function (item) {
            ls.ajax.load('plugin-album/photo/preview', {
                id: item.id,
                album_id: this.albumId
            }, function (res) {
                this.album = res.album;
                this.closeModal();
            }.bind(this));
        },
        getImage: function (item, size) {
            size = size || 'middle';
            if (item.sizes[size]) {
                var data = item.sizes[size];
                var s = data.w + 'x' + (data.h ? data.h : '');
                if (data.crop) {
                    s = s + 'crop';
                }
                var path = ls.utils.pathinfo(item.image);
                return path.dirname + '/' + path.filename + '_' + s + '.' + path.extension;
            }
            return item.image;
        },
        pluralize: function (count, words, skip) {
            return ls.lang.pluralize(count, words, null, skip);
        },
        initMasonry: function () {
            return;
            $(this.$refs['grid']).masonry({
                itemSelector: '.object-list-item',
                columnWidth: 200
            });
        },
        showModal: function (item) {
            this.itemForModal = item;
            this.$modal.show(this.modalName);
        },
        closeModal: function () {
            this.$modal.hide(this.modalName);
        },
        onModalOpened: function () {
            this.isShowModal = true;
        },
        onModalClosed: function () {
            this.isShowModal = false;
            this.itemForModal = null;
        },
        updateItemField: function (item, field) {
            ls.ajax.load('plugin-album/photo/update-field', {
                id: item.id,
                field: field,
                value: item[field]
            }, function (res) {

            });
        }
    }
});

$(function ($) {
    if ($('.js-album-show').length) {
        $('.js-album-show').each(function (k, v) {
            new Vue({
                el: v,
                data: {},
                components: {}
            });
        });
    }
});
Vue.component('app-album-library', {
    template: '#app-template-album-library',
    props: {
        ownerKey: {
            default: null
        },
        titleButtonOk: {
            default: 'Вставить'
        },
        initSelectedItem: {
            default: function () {
                return null;
            }
        },
        initSettings: {
            default: function () {
                return {};
            }
        }
    },
    data: function () {
        return {
            filterLoad: {
                search: ''
            },
            settings: null,
            selectedItem: null,
            isProcessingLoad: false,
            isShowSelected: false,
            items: []
        }
    },
    watch: {
        filterLoad: {
            handler: function (newValue) {
                this.makeLoad();
            },
            deep: true
        }
    },
    created: function () {
        this.settings = _.cloneDeep(this.initSettings);
        if (this.initSelectedItem) {
            this.selectedItem = _.cloneDeep(this.initSelectedItem);
            this.showSelected();
        }
        this.load();
    },
    destroyed: function () {

    },
    mounted: function () {

    },
    filters: {},
    computed: {
        showFilter: function () {
            return !this.isShowSelected;
        },
        showProcessingLoad: function () {
            return this.isProcessingLoad && !this.isShowSelected;
        },
        itemsShow: function () {
            if (this.isShowSelected) {
                return [this.selectedItem];
            } else {
                return this.items;
            }
        }
    },
    methods: {
        getItemById: function (id) {
            var item = null;
            _.forEach(this.items, function (v, index) {
                if (v.id == id) {
                    item = v;
                    return false;
                }
            });
            return item;
        },
        getItemIndexById: function (id) {
            var indexReturn = null;
            _.forEach(this.items, function (v, index) {
                if (v.id == id) {
                    indexReturn = index;
                    return false;
                }
            });
            return indexReturn;
        },
        getItemIndexByIdFromItemsShow: function (id) {
            var indexReturn = null;
            _.forEach(this.itemsShow, function (v, index) {
                if (v.id == id) {
                    indexReturn = index;
                    return false;
                }
            });
            return indexReturn;
        },
        removeItemSoft: function (item) {
            _.forEach(this.items, function (v, index) {
                if (v.id == item.id) {
                    Vue.delete(this.items, index);
                    return false;
                }
            }.bind(this));
        },
        actionCancel: function () {
            this.$emit('action-cancel', {});
        },
        actionOk: function () {
            this.$emit('action-ok', {item: this.selectedItem, settings: this.settings});
        },
        isSelectedItem: function (item) {
            if (this.selectedItem && this.selectedItem.id == item.id) {
                return true;
            }
            return false;
        },
        setActive: function (item) {
            if (this.isSelectedItem(item)) {
                this.removeFromSelected(item);
            } else {
                this.setSelected(item);
            }
        },
        removeFromSelected: function (item) {
            this.selectedItem = null;
        },
        setSelected: function (item) {
            this.selectedItem = item;
        },
        makeLoad: _.debounce(function () {
            this.load();
        }, 300),
        load: function () {
            this.isProcessingLoad = true;
            this.items = [];
            ls.ajax.load('plugin-album/album/load-items', {
                filter: this.filterLoad,
                owner_key: this.ownerKey
            }, function (res) {
                this.items = res.items;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoad = false;
                }.bind(this)
            });
        },
        showSelected: function () {
            this.isShowSelected = true;
        },
        closeSelected: function () {
            this.isShowSelected = false;
        },
        reset: function() {
            this.selectedItem = null;
        }
    }
});
Vue.use(window["vue-js-modal"].default);
Vue.component('app-editor-block-album', {
    template: '#app-template-editor-block-album',
    props: ['initData', 'api', 'uuid', 'initParams'],
    components: {},
    data: function () {
        return {
            item: null,
            items: [],
            params: {
                owner_key: null
            },
            albumSettings: {},
            modalName: null,
            isProcessingLoad: false,
            isVisible: false,
            isShowModal: false
        };
    },
    watch: {},
    created: function () {
        this.modalName = this.generateUuid();
    },
    mounted: function () {
        if (this.initParams) {
            _.forEach(this.initParams, function (value, name) {
                this.$set(this.params, name, value);
            }.bind(this));
        }

        this.processingData(this.initData);

        if (this.id) {
            this.isVisible = true;
            this.load();
        } else {
            this.showModal();
        }

        this.$nextTick(function () {
            this.$emit('onRender', {
                isNew: !this.id
            });
        }.bind(this));
    },
    computed: {},
    methods: {
        save: function () {
            return {
                id: this.item.id,
                settings: this.albumSettings
            }
        },
        validate: function (data) {
            if (data.id) {
                return true;
            }
            return false;
        },
        settings: function () {
            return [{
                class: 'component-editor-btn_edit',
                data: {
                    action: 'edit'
                }
            }];
        },
        onSettingsItem: function (data) {
            if (data.action == 'edit') {
                this.showModal();
            }
        },
        button: function () {
            return {
                class: 'btn-create-block btn-create-block_photoalbum',
                title: 'Добавить альбом'
            }
        },
        processingData: function (data) {
            if (typeof data !== 'object') {
                this.api.remove(this.uuid);
                return;
            }
            this.albumSettings = data.settings || {};
            this.id = data.id || null;
        },
        load: function () {
            this.isProcessingLoad = true;
            ls.ajax.load('plugin-album/album/load', {
                id: this.id,
                owner_key: this.params.owner_key
            }, function (res) {
                this.item = res.item;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoad = false;
                }.bind(this)
            });
        },
        showModal: function () {
            this.$modal.show(this.modalName);
        },
        closeModal: function () {
            this.$modal.hide(this.modalName);
        },
        onModalOpened: function () {
            this.isShowModal = true;
        },
        onModalClosed: function () {
            if (this.item) {
                this.isVisible = true;
            } else {
                this.api.remove(this.uuid);
            }
            this.isShowModal = false;
        },
        generateUuid: function uuidv4() {
            return ([1e7] + -1e3 + -4e3 + -8e3 + -1e11).replace(/[018]/g, function (c) {
                return (c ^ crypto.getRandomValues(new Uint8Array(1))[0] & 15 >> c / 4).toString(16);
            });
        },
        actionCancel: function () {
            this.closeModal();
        },
        actionOk: function (data) {
            this.item = data.item;
            _.forEach(data.settings, function (value, name) {
                this.$set(this.albumSettings, name, value);
            }.bind(this));
            this.closeModal();
        }
    }
});
Vue.component('app-document-create', {
    template: '#app-template-document-create',
    props: ['uid', 'branchId', 'targetId', 'targetType'],
    data: function () {
        return {
            form: {
                title: '',
                type_id: '',
                date_accept: '',
                number: '',
                date_publication: '',
                date_change: '',
                description: '',
                text: '',
                show: 1,
                is_available: 1,
                only_auth: 0,
                files_ids: []
            },
            types: []
        }
    },
    watch: {},
    created: function () {
        if (this.uid) {
            this.loadItem();
        } else {
            this.form.date_publication = moment().format('YYYY-MM-DD HH:mm:ss')
        }
        this.loadTypeItems();
    },
    mounted: function () {
        jQuery.datetimepicker.setLocale('ru');
        $('.js-datetime-picker').datetimepicker({
            format: 'Y-m-d H:i:00',
            scrollMonth: false,
            scrollTime: false,
            scrollInput: false,
            dayOfWeekStart: '1',
            onChangeDateTime: function (DatePicker, $Input) {
                $Input.get(0).dispatchEvent(new Event('input'));
            }
        });
        $('.js-datetime-picker-date').datetimepicker({
            format: 'Y-m-d',
            timepicker: false,
            scrollMonth: false,
            scrollTime: false,
            scrollInput: false,
            dayOfWeekStart: '1',
            onChangeDateTime: function (DatePicker, $Input) {
                $Input.get(0).dispatchEvent(new Event('input'));
            }
        });

        this.initEditor();
    },
    filters: {},
    methods: {
        initEditor: function () {
            var vm = this;
            if (window.tinymce) {
                var defaultSettings = {
                    target: this.$refs.text,
                    height: 500,
                    language: 'ru',
                    menubar: false,
                    extended_valid_elements: 'span[id|class]',
                    content_style: "body { padding: 20px; font-size:.9em; }",
                    statusbar: false,
                    convert_urls: false,
                    relative_urls: false,
                    plugins: [
                        "advlist autolink autosave link image lists charmap print preview hr anchor pagebreak",
                        "searchreplace wordcount visualblocks visualchars code fullscreen insertdatetime media nonbreaking",
                        "table contextmenu directionality emoticons template textcolor paste textcolor colorpicker textpattern"
                    ],
                    toolbar1: "styleselect bold italic subscript superscript | alignleft aligncenter alignright alignjustify | table| hr template bullist numlist table | link unlink anchor lsdocument lsmedia removeformat code ",
                    style_formats: [
                        {title: 'Абзац', format: 'p'},
                        {title: 'Заголовок 2', format: 'h2'},
                        {title: 'Заголовок 3', format: 'h3'},
                        {title: 'Прямая речь', selector: 'p', classes: 'speech'},
                        {title: 'Цитата', format: 'blockquote'},
                        {
                            title: 'Таблица', items: [
                            {
                                title: 'Заголовок', items: [
                                {title: 'Ячейка', selector: 'td', classes: 'table-head'},
                                {title: 'Ряд', selector: 'tr', classes: 'table-head'}
                            ]
                            },
                            {title: 'Базовый', selector: 'table', classes: 'table'},
                            {title: '«Зебра»', selector: 'table', classes: 'table-striped'},
                            {title: 'Границы', selector: 'table', classes: 'table-bordered'},
                            {title: 'Компактная', selector: 'table', classes: 'table-sm'}
                        ]
                        }
                    ],
                    image_advtab: true,
                    table_default_attributes: {
                        class: 'table b-table',
                        border: '0'
                    },
                    templates: [
                        {
                            title: 'Таблица событий',
                            description: '',
                            content: '<table class="table b-table"><tr><td class="b-table__date"><span>1</span>января</td><td><a href="#">Название документа</a></td></tr></table>'
                        }
                    ],
                    table_default_styles: {
                        'border-collapsed': 'inherit', 'width': '100%'
                    },
                    table_responsive_width: true,
                    content_css: [
                        PATH_ROOT + 'framework/frontend/components/css-reset/vendor/normalize/normalize.css?v=0',
                        PATH_SKIN + '/assets/css/style.css?v=' + new Date().getTime(),
                        PATH_SKIN + '/assets/css/custom_tinymce.css?v=' + new Date().getTime()
                    ],
                    style_formats_merge: false,
                    setup: function (editor) {
                        editor.on('change', function () {
                            editor.save();
                        });
                        editor.on('blur', function () {
                            var newContent = editor.getContent();
                            vm.form.text = newContent;
                        });
                    }
                };

                var customSetting = {};
                var settingsFinal = $.extend({}, defaultSettings, customSetting);

                tinymce.remove();
                ls.hook.run('ls_admin_tinymce_init_before', [settingsFinal], window);
                tinymce.init(settingsFinal);
            }
        },
        loadItem: function () {
            ls.ajax.load('document/load-item', {
                uid: this.uid
            }, function (res) {
                res.item.is_available = parseInt(res.item.is_available);
                res.item.only_auth = parseInt(res.item.only_auth);
                this.form = res.item;
                this.form.type_id_init = parseInt(res.item.type_id);
                this.form.files_ids = [];
                if (this.form.files) {
                    _.forEach(this.form.files, function (v, id) {
                        this.form.files_ids.push(id);
                    }.bind(this));
                }
                if (window.tinymce) {
                    var editor = tinymce.editors[0];
                    editor.setContent(this.form.text);
                }
            }.bind(this), {
                onComplete: function () {

                }.bind(this)
            });
        },
        loadTypeItems: function () {
            ls.ajax.load('document/load-types', {
                id: this.id
            }, function (res) {
                this.types = res.items;
                if (this.form.type_id_init) {
                    setTimeout(function () {
                        Vue.set(this.form, 'type_id', this.form.type_id_init);
                        this.form.type_id_init = '';
                    }.bind(this), 1000);
                }
            }.bind(this), {
                onComplete: function () {

                }.bind(this)
            });
        },
        submitCreate: function () {
            ls.ajax.load('document/create', {
                item: this.form,
                branch_id: this.branchId,
                target_id: this.targetId,
                target_type: this.targetType
            }, function (res) {

            }.bind(this), {
                onComplete: function () {

                }.bind(this)
            });
        },
        submitUpdate: function () {
            ls.ajax.load('document/update', {
                item: this.form,
                branch_id: this.branchId,
                target_id: this.targetId,
                target_type: this.targetType,
                uid: this.uid
            }, function (res) {

            }.bind(this), {
                onComplete: function () {

                }.bind(this)
            });
        },
        refreshFiles: function (data) {
            var files = [];
            _.forEach(data.ids, function (v, index) {
                files.push(v.id);
            }.bind(this));
            this.form.files_ids = files;
        }
    }
});

$(function ($) {
    if ($('#app-document-create').length) {
        new Vue({
            el: '#app-document-create',
            data: {},
            components: {}
        });
    }
});
Vue.component('app-document-insert', {
    template: '#app-template-document-insert',
    props: ['branchId'],
    data: function () {
        return {
            items: [],
            itemActive: null,
            itemsSelected: [],
            insertOptions: {
                useList: true,
                useLink: false,
                title: ''
            },
            isLoadingItems: false,
            view: 'search',
            inputSearch: '',
            isProcessingSearch: false,
            isProcessingUpdate: false,
            searchPage: 1,
            searchPageCount: 0,
            searchPageCurrent: 1,
            cache: {
                search: {
                    items: []
                },
                selected: {
                    items: []
                }
            }
        }
    },
    watch: {
        view: function (now, old) {
            this.cache[old] = {
                items: this.items
            };
            this.items = this.cache[now].items;

            if (now == 'search' && !this.items.length) {
                this.makeSearch();
            }
            if (now == 'selected') {
                this.items = this.itemsSelected;
            }
            this.checkItemActive();
            this.checkItemsSelected();
        },
        inputSearch: function (search) {
            this.searchPage = 1;
            this.makeSearch();
        },
        itemsSelected: function (now) {
            if ((this.itemsSelected.length < 1) && (this.view == 'selected')) {
                this.view = 'search';
            }
        }
    },
    mounted: function () {
        $('.js-media-document-insert-submit').click(function () {
            if (this.itemsSelected.length < 1) {
                alert('Для вставки необходимо выбрать изображение');
                return false;
            }
            var ids = [];
            var uids = [];
            _.forEach(this.itemsSelected, function (item, index) {
                uids.push(item.info.uid);
                ids.push(item.id);
            }.bind(this));

            var data = {
                items: uids,
                options: this.insertOptions
            };
            if (this.insertOptions.useLink) {
                var item = this.getItemById(ids[0]);
                data['item_url'] = item.info.url;
                data['item_title'] = item.info.title;
            }
            $bus.$emit('app-document-insert:submit', data);
        }.bind(this));
        this.makeSearch();
    },
    filters: {},
    methods: {
        getItemById: function (id) {
            var item = null;
            _.forEach(this.items, function (v, index) {
                if (v.id == id) {
                    item = v;
                    return false;
                }
            });
            return item;
        },
        getItemSelectedById: function (id) {
            var item = null;
            _.forEach(this.itemsSelected, function (v, index) {
                if (v.id == id) {
                    item = v;
                    return false;
                }
            });
            return item;
        },
        clickItem: function (item) {
            if (item.state == 'uploading' || item.state == 'error') {
                return;
            }

            if (item.active) {
                Vue.set(item, 'selected', false);
                Vue.set(item, 'active', false);
                this.removeItemFromSelected(item);
                this.setItemActive();
            } else {
                Vue.set(item, 'selected', true);
                this.addItemToSelected(item);
                this.setItemActive(item);
            }
        },
        addItemToSelected: function (item) {
            var itemOld = this.getItemSelectedById(item.id);
            if (!itemOld) {
                this.itemsSelected.push(item);
            }
        },
        removeItemFromSelected: function (item) {
            this.itemsSelected = _.remove(this.itemsSelected, function (i) {
                return i.id !== item.id;
            });
            if (this.view == 'selected') {
                this.items = this.itemsSelected;
            }
        },
        setItemActive: function (item) {
            this.itemActive = null;
            _.forEach(this.items, function (v, index) {
                Vue.set(v, 'active', false);
            });
            if (item) {
                Vue.set(item, 'active', true);
                this.itemActive = item;
            } else {
                var itemLast = null;
                _.forEach(this.items, function (v, index) {
                    if (v.selected) {
                        itemLast = v;
                    }
                });
                if (itemLast) {
                    Vue.set(itemLast, 'active', true);
                    this.itemActive = itemLast;
                }
            }
        },
        checkItemActive: function () {
            this.itemActive = null;
            _.forEach(this.items, function (v, index) {
                if (v.active && !this.itemActive) {
                    Vue.set(v, 'active', true);
                    this.itemActive = v;
                } else {
                    Vue.set(v, 'active', false);
                }
            }.bind(this));
        },
        checkItemsSelected: function () {
            _.forEach(this.items, function (v, index) {
                if (this.getItemSelectedById(v.id)) {
                    Vue.set(v, 'selected', true);
                } else {
                    Vue.set(v, 'selected', false);
                }
            }.bind(this));
        },
        updateItemInfo: function (item, field) {
            this.isProcessingUpdate = true;
            var params;
            if (field) {
                var itemCurr = this.getItemById(item.id);
                if (itemCurr) {
                    Vue.set(itemCurr.info, field, item.info[field]);
                }
                params = {
                    id: item.id,
                    field: field,
                    value: item.info[field]
                }
            } else {
                params = {
                    id: item.id,
                    data: item.info
                }
            }

            ls.ajax.load(aRouter.admin + 'plugin/documents/ajax/update-info/', params, function (res) {

            }, {
                onComplete: function () {
                    this.isProcessingUpdate = false;
                }.bind(this)
            });
        },
        addItem: function (data) {
            var item = {
                'id': data.id,
                'preview': data.preview,
                'info': data.info
            };
            this.items.push(item);
            return item;
        },
        removeItem: function (item) {
            if (!confirm('Действительно удалить?')) {
                return;
            }
            ls.ajax.load(aRouter.admin + 'plugin/documents/ajax/remove/', {
                id: item.id,
                target_id: this.targetId,
                target_type: this.targetType
            }, function (res) {
                _.forEach(this.items, function (v, index) {
                    if (v.id == item.id) {
                        this.removeItemFromSelected(item);
                        Vue.delete(this.items, index);
                        this.setItemActive();
                        return false;
                    }
                }.bind(this));
            }.bind(this));
        },
        makeSearch: _.debounce(
            function () {
                this.isProcessingSearch = true;
                ls.ajax.load('document/search', {
                    search: this.inputSearch,
                    branch_id: this.branchId,
                    page: this.searchPage,
                    per_page: 20
                }, function (res) {

                    this.items = [];
                    this.itemActive = null;

                    _.forEach(res.items, function (item, index) {
                        this.addItem(item);
                    }.bind(this));
                    this.checkItemsSelected();

                    if (res.paging) {
                        this.searchPageCount = res.paging.iCountPage;
                        this.searchPageCurrent = res.paging.iCurrentPage;
                    }

                }.bind(this), {
                    onComplete: function () {
                        this.isProcessingSearch = false;
                    }.bind(this)
                });
            },
            300
        ),
        searchSelectPage: function (data) {
            this.searchPage = data.page;
            this.makeSearch();
        },
        isAllowRemove: function (item) {
            return true;
        },
        resetSelected: function (e) {
            _.forEach(this.items, function (v, index) {
                if (v.selected) {
                    Vue.set(v, 'selected', false);
                }
            }.bind(this));
            this.itemsSelected = [];
            if (e) {
                e.preventDefault();
            }
        }
    }
});
Vue.component('app-plugin-document-search', {
    template: '#app-template-plugin-document-search',
    props: {
        url: {
            default: aRouter.docs + 'ajax/search/'
        }
    },
    data: function () {
        return {
            isProcessingSearch: false,
            firstSearchCompleted: false,
            isNotFound: false,
            filter: {
                search: ''
            },
            page: 1,
            pageCount: 0,
            pageCurrent: 1,
            count: 0,
            items: []
        }
    },
    watch: {

    },
    created: function () {
        this.search();
    },
    mounted: function () {
        var vm = this;
        $(window).on('hashchange', function () {
            this.initFilterFromHash();
        }.bind(this));

        this.hideGlobalSpinner();
    },
    computed: {
    },
    methods: {
        makeSearch: _.debounce(
            function () {
                this.newSearch();
            },
            500
        ),
        newSearch: function () {
            this.page = 1;
            this.search();
        },
        initFilterFromHash: function () {
            var search = ls.main.getHashParam('search');
            if (search && this.filter.search != search) {
                this.filter.search = search;
                this.makeSearch();
            }
        },
        updateFilterHash: function () {
            var params = ls.main.getHashParams();
            if (params['search'] != this.filter.search) {
                params['search'] = this.filter.search;
                var hash = '#';
                _.forEach(params, function (v, k) {
                    hash = hash + k + '=' + v + '&';
                }.bind(this));
                window.location.hash = hash;
            }
        },
        search: function () {
            this.isNotFound = false;
            this.items = [];
            this.isProcessingSearch = true;
            this.updateFilterHash();

            var params = $.extend({}, this.filter, {
                page: this.page
            });
            ls.ajax.load(this.url, params, function (res) {
                this.items = [];
                if (res.items && res.items.length) {
                    _.forEach(res.items, function (item, index) {
                        this.items.push(item);
                    }.bind(this));
                } else {
                    this.isNotFound = true;
                }
                if (res.paging) {
                    this.pageCount = res.paging.iCountPage;
                    this.pageCurrent = res.paging.iCurrentPage;
                }
                this.count = res.count;
                this.firstSearchCompleted = true;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingSearch = false;
                }.bind(this),
                onError: function (res) {
                    if (res.errors) {
                        _.forEach(res.errors, function (errors, fieldName) {
                            if (this.$refs[fieldName]) {
                                var field = $(this.$refs[fieldName]);
                                field.lsValidationField();
                                if (field.lsValidationField('isInit')) {
                                    field.lsValidationField('showError', errors);
                                }
                            }
                        }.bind(this));
                    }
                }.bind(this)
            });
        },
        selectPage: function (data) {
            this.page = data.page;
            this.search();
        },
        clearFilterField: function (field) {
            this.filter[field] = '';
            this.newSearch();
        },
        hideGlobalSpinner: function () {
            $('.js-spinner-global-vue').hide();
        }
    }
});

$(function ($) {
    if ($('#app-plugin-document-search').length) {
        new Vue({
            el: '#app-plugin-document-search',
            data: {},
            components: {}
        });
    }
});
Vue.component('app-attachment-uploader', {
    template: '#app-template-attachment-uploader',
    props: {
        maxItems: {
            default: 500
        },
        uid: {
            default: null
        },
        title: {
            default: 'Загрузить файл(ы)'
        },
    },
    data: function () {
        return {
            uploadItems: []
        }
    },
    watch: {},
    created: function () {
        this.idInput = this.generateIdInput();
        if (this.uid) {
            this.loadItems();
        }
    },
    mounted: function () {
        var fileupload = {
            url: aRouter.ajax + 'document/upload-file',
            sequentialUploads: false,
            singleFileUploads: true,
            limitConcurrentUploads: 1
        };

        var uploader = $(this.$refs.uploadInput);
        fileupload.dropZone = $(this.$refs.uploadArea);
        uploader.fileupload(fileupload);
        uploader.on({
            fileuploadadd: this.onUploadAdd.bind(this),
            fileuploaddone: function (event, data) {
                this[data.result.bStateError ? 'onUploadError' : 'onUploadDone'](data.context, data.result)
            }.bind(this),
            fileuploadfail: function (event, data) {
                this['onUploadError'](data.context, data.result)
            }.bind(this),
            fileuploadprogress: function (event, data) {
                this.onUploadProgress(data.context, parseInt(data.loaded / data.total * 100, 10))
            }.bind(this)
        })
    },
    filters: {},
    methods: {
        refreshEvent: function () {
            var ids = this.getUploads();
            this.$emit('refresh', {
                ids: ids
            });
        },
        generateIdInput: function () {
            return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function (c) {
                var r = Math.random() * 16 | 0,
                    v = c == 'x' ? r : (r & 0x3 | 0x8);
                return v.toString(16);
            });
        },
        loadItems: function () {
            ls.ajax.load('document/load-files', {
                uid: this.uid
            }, function (res) {
                this.uploadItems = res.items;
            }.bind(this), {
                onComplete: function () {

                }.bind(this)
            });
        },
        onUploadAdd: function (event, file) {
            if (this.uploadItems.length >= this.maxItems) {
                return false
            }
            var item = {
                'id': this.generateUid(),
                'idInput': this.generateIdInput(),
                'state': 'uploading',
                'progress': 0,
                'info': {
                    name: file.files[0].name
                }
            };
            file.context = item.id;
            this.uploadItems.unshift(item);

            $(event.target).fileupload('option', 'formData', {
                security_ls_key: LIVESTREET_SECURITY_KEY,
                uid: this.uid
            })
        },
        onUploadError: function (context, result) {
            var item = this.getItemById(context);
            item.state = 'error';

            if (result.sMsg) {
                item.msgError = result.sMsg;
            }

            setTimeout(function () {
                _.forEach(this.uploadItems, function (v, index) {
                    if (v.id == item.id) {
                        this.$delete(this.uploadItems, index);
                        return false
                    }
                }.bind(this))
            }.bind(this), 1000000)
        },
        onUploadDone: function (context, result) {
            var item = this.getItemById(context);

            item.state = 'completed';
            item.id = result.id;
            item.info = result.info;
            item.group = result.group;
            this.refreshEvent();
        },
        onUploadProgress: function (context, percent) {
            var item = this.getItemById(context);
            item.progress = percent
        },
        generateUid: function () {
            var text = '';
            var possible = 'abcdefghijklmnopqrstuvwxyz0123456789';

            for (var i = 0; i < 10; i++) {
                text += possible.charAt(Math.floor(Math.random() * possible.length))
            }
            return text
        },
        getItemById: function (id) {
            var item = null;
            _.forEach(this.uploadItems, function (v, index) {
                if (v.id == id) {
                    item = v;
                    return false
                }
            });
            return item
        },
        removeItem: function (item, noConfirm) {
            if (!noConfirm && !confirm('Действительно удалить?')) {
                return
            }

            _.forEach(this.uploadItems, function (v, index) {
                if (v.id == item.id) {
                    this.$delete(this.uploadItems, index);
                    return false
                }
            }.bind(this));

            this.refreshEvent();
        },
        getUploads: function () {
            var items = [];
            _.forEach(this.uploadItems, function (v, index) {
                if (v.state == 'completed') {
                    items.push({
                        'id': v.id
                    })
                }
            }.bind(this));
            return items
        }
    }
});

$(function ($) {
    if ($('#app-attachment-uploader').length) {
        new Vue({
            el: '#app-attachment-uploader',
            data: {},
            components: {}
        });
    }
});
Vue.component('app-document-library', {
    template: '#app-template-document-library',
    props: {
        ownerKey: {
            default: null
        },
        titleButtonOk: {
            default: 'Вставить'
        },
        initSelectedItems: {
            default: function () {
                return [];
            }
        },
        initSettings: {
            default: function () {
                return {
                    group: '',
                    asLink: false
                };
            }
        }
    },
    data: function () {
        return {
            filterLoad: {
                date: '',
                search: ''
            },
            settings: null,
            activeItem: null,
            selectedItems: [],
            selectedItemsId: [],
            isProcessingLoad: false,
            isShowSelected: false,
            items: [],
            errors: []
        }
    },
    watch: {
        filterLoad: {
            handler: function (newValue) {
                this.makeLoad();
            },
            deep: true
        },
        selectedItemsId: function (newValue) {
            if (!newValue.length) {
                this.isShowSelected && this.closeSelected();
            }
        }
    },
    created: function () {
        this.settings = _.cloneDeep(this.initSettings);
        if (this.initSelectedItems && this.initSelectedItems.length) {
            this.selectedItems = _.cloneDeep(this.initSelectedItems);
            _.forEach(this.selectedItems, function (item, index) {
                this.selectedItemsId.push(item.uid);
            }.bind(this));
            this.activeItem = this.selectedItems[0];
            this.showSelected();
        }
        this.load();
    },
    destroyed: function () {

    },
    mounted: function () {

    },
    filters: {},
    computed: {
        showFilter: function () {
            return !this.isShowSelected;
        },
        showProcessingLoad: function () {
            return this.isProcessingLoad && !this.isShowSelected;
        },
        itemsShow: function () {
            if (this.isShowSelected) {
                return this.selectedItems;
            } else {
                return this.items;
            }
        }
    },
    methods: {
        getItemById: function (id) {
            var item = null;
            _.forEach(this.items, function (v, index) {
                if (v.uid == id) {
                    item = v;
                    return false;
                }
            });
            return item;
        },
        getItemIndexById: function (id) {
            var indexReturn = null;
            _.forEach(this.items, function (v, index) {
                if (v.uid == id) {
                    indexReturn = index;
                    return false;
                }
            });
            return indexReturn;
        },
        getItemIndexByIdFromItemsShow: function (id) {
            var indexReturn = null;
            _.forEach(this.itemsShow, function (v, index) {
                if (v.uid == id) {
                    indexReturn = index;
                    return false;
                }
            });
            return indexReturn;
        },
        removeItemSoft: function (item) {
            _.forEach(this.items, function (v, index) {
                if (v.uid == item.uid) {
                    Vue.delete(this.items, index);
                    return false;
                }
            }.bind(this));
        },
        actionCancel: function () {
            this.$emit('action-cancel', {});
        },
        actionOk: function () {
            this.settings.asLink = false;
            if (this.selectedItems < 2) {
                this.settings.group = '';
            }
            this.$emit('action-ok', {items: this.selectedItems, settings: this.settings});
        },
        actionOkLink: function () {
            this.settings.asLink = true;
            this.settings.group = '';
            this.$emit('action-ok', {items: this.selectedItems, settings: this.settings});
        },
        addError: function (msg) {
            this.errors.push({msg: msg});
        },
        isSelectedItem: function (item) {
            if (this.selectedItemsId.indexOf(item.uid) > -1) {
                return true;
            }
            return false;
        },
        isActiveItem: function (item) {
            if (this.activeItem && this.activeItem.uid == item.uid) {
                return true;
            }
            return false;
        },
        setActive: function (item, shiftKey) {
            shiftKey = shiftKey || false;
            if (this.isActiveItem(item)) {
                this.removeFromSelected(item);
            } else {
                if (this.isSelectedItem(item)) {
                    this.activeItem = item;
                } else {
                    this.setSelected(item);
                    this.activeItem = item;
                }
            }
        },
        removeFromSelected: function (item) {
            var isActive = this.isActiveItem(item);
            this.selectedItems.splice(this.selectedItems.indexOf(item), 1);
            this.selectedItemsId.splice(this.selectedItemsId.indexOf(item.uid), 1);
            if (isActive) {
                // удаляем из активного
                this.activeItem = null;
                // ищет новый для активного
                if (this.selectedItems.length) {
                    this.$nextTick(function () {
                        this.activeItem = this.selectedItems[this.selectedItems.length - 1];
                    }.bind(this));
                }
            }
        },
        setSelected: function (item) {
            this.selectedItems.push(item);
            this.selectedItemsId.push(item.uid);
        },
        resetItems: function () {
            this.selectedItems = [];
            this.selectedItemsId = [];
            this.activeItem = null;
        },
        makeLoad: _.debounce(function () {
            this.load();
        }, 300),
        load: function () {
            this.isProcessingLoad = true;
            this.items = [];
            ls.ajax.load('document/load-items', {
                filter: this.filterLoad,
                owner_key: this.ownerKey
            }, function (res) {
                this.items = res.items;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoad = false;
                }.bind(this)
            });
        },
        showSelected: function () {
            this.isShowSelected = true;
        },
        closeSelected: function () {
            this.isShowSelected = false;
        },
        copyTextToClipboard: function (text) {
            ls.utils.copyTextToClipboard(text);
        },
        pluralize: function (count, words, skip) {
            return ls.lang.pluralize(count, words, null, skip);
        }
    }
});
Vue.use(window["vue-js-modal"].default);
Vue.component('app-editor-block-document', {
    template: '#app-template-editor-block-document',
    props: ['initData', 'api', 'uuid', 'initParams'],
    components: {},
    data: function () {
        return {
            items: [],
            params: {
                owner_key: null
            },
            type: null,
            documentSettings: {
                group: '',
                asLink: false
            },
            modalName: null,
            isProcessingLoad: false,
            isVisible: false,
            isShowModal: false
        };
    },
    watch: {},
    created: function () {
        this.modalName = this.generateUuid();
    },
    mounted: function () {
        if (this.initParams) {
            _.forEach(this.initParams, function (value, name) {
                this.$set(this.params, name, value);
            }.bind(this));
        }

        if (typeof this.initData == 'object' && this.initData.items) {
            this.processingData(this.initData);
        } else {
            this.showModal();
        }

        this.$nextTick(function () {
            this.$emit('onRender', {
                isNew: !this.items.length
            });
        }.bind(this));
    },
    computed: {
        itemsId: function () {
            var result = [];
            _.forEach(this.items, function (item, index) {
                result.push(item.uid);
            });
            return result;
        }
    },
    methods: {
        save: function () {
            return {
                items: this.itemsId,
                settings: this.documentSettings
            }
        },
        validate: function (data) {
            if (data.items && data.items.length) {
                return true;
            }
            return false;
        },
        settings: function () {
            return [{
                class: 'component-editor-btn_edit',
                data: {
                    action: 'edit'
                }
            }];
        },
        onSettingsItem: function (data) {
            if (data.action == 'edit') {
                this.showModal();
            }
        },
        button: function () {
            return {
                class: 'btn-create-block btn-create-block_document',
                title: 'Добавить документ'
            }
        },
        processingData: function (data) {
            if (typeof data !== 'object') {
                this.api.remove(this.uuid);
                return;
            }
            this.isVisible = true;
            this.documentSettings = data.settings;
            this.load();
        },
        load: function () {
            this.isProcessingLoad = true;
            ls.ajax.load('document/load-items', {
                filter: {
                    uids: this.initData.items
                },
                owner_key: this.params.owner_key
            }, function (res) {
                this.items = res.items;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoad = false;
                }.bind(this)
            });
        },
        showModal: function () {
            this.$modal.show(this.modalName);
        },
        closeModal: function () {
            this.$modal.hide(this.modalName);
        },
        onModalOpened: function () {
            this.isShowModal = true;
        },
        onModalClosed: function () {
            if (this.items.length) {
                this.isVisible = true;
            } else {
                this.api.remove(this.uuid);
            }
            this.isShowModal = false;
        },
        generateUuid: function uuidv4() {
            return ([1e7] + -1e3 + -4e3 + -8e3 + -1e11).replace(/[018]/g, function (c) {
                return (c ^ crypto.getRandomValues(new Uint8Array(1))[0] & 15 >> c / 4).toString(16);
            });
        },
        actionCancel: function () {
            this.closeModal();
        },
        actionOk: function (data) {
            this.items = data.items;
            _.forEach(data.settings, function (value, name) {
                this.$set(this.documentSettings, name, value);
            }.bind(this));
            this.closeModal();
        },
        pluralize: function (count, words, skip) {
            return ls.lang.pluralize(count, words, null, skip);
        }
    }
});
jQuery(function ($) {
    // Форма добавления
    $('#form-branch-news-create').lsBranchNewsAdd();
    $('#form-branch-event-create').lsBranchEventAdd();
    $('.js-event-join-action').lsBranchEventJoin();
    $('.js-event-user-approve').lsBranchEventApprove();
    $('.js-branch-event-item-data').lsBranchEventItemData();


    $(document).on('click', '.js-cross-register-submit', function () {
        ls.ajax.load(aRouter.branch + 'ajax/cross/register', {
            id: $(this).data('id')
        }, function (res) {
            $('#modal-cross-register').lsModal('hide');
            $('.js-cross-count-user').text(res.count);
            $('.js-cross-button-modal-register').hide();
            $('.js-cross-user-time-value').text(res.time);
            $('.js-cross-user-time-area').show();
        }.bind(this));
        return false;
    });

    $(document).on('click', '.js-cross-button-export-xls', function () {
        ls.ajax.load(aRouter.branch + 'ajax/cross/export-xls', {
            id: $(this).data('id')
        }, function (res) {
            if (res.file) {
                window.location = res.file;
            }
        }.bind(this));
        return false;
    });
    $(document).on('click', '.js-event-button-export-xls', function () {
        ls.ajax.load(aRouter.branch + 'ajax/event/export-xls', {
            id: $(this).data('id')
        }, function (res) {
            if (res.file) {
                window.location = res.file;
            }
        }.bind(this));
        return false;
    });

    $(document).on('click', '.js-branch-event-editor-popup-choice', function () {
        if (currentEditorMarkitup) {
            $(currentEditorMarkitup.textarea).lsEditor('insert', '<event id="' + $(this).data('id') + '" />');
            $('#modal-event-editor-popup').lsModal('hide');
        }
        return false;
    });

    jQuery.datetimepicker.setLocale('ru');
    $('.js-branch-datetime-picker').datetimepicker({
        format: 'Y-m-d H:i:00'
    });

    $('.js-branch-event-calendar').datetimepicker({
        format: 'Y-m-d',
        formatDate: 'Y-m-d',
        inline: true,
        timepicker: false,
        lang: 'ru',
        allowDates: window.branchEventDates || [],
        yearStart: 2018,
        todayButton: false,
        scrollMonth: false,
        scrollTime: false,
        scrollInput: false,
        dayOfWeekStart: 1,
        defaultSelect: false,
        yearEnd: 2019,
        onSelectDate: function (ct, $i) {
            window.location.href = branchUrl + 'events/#date=' + dateToYMD(ct);
        }
    });

    function dateToYMD(date) {
        var d = date.getDate();
        var m = date.getMonth() + 1;
        var y = date.getFullYear();
        return '' + y + '-' + (m <= 9 ? '0' + m : m) + '-' + (d <= 9 ? '0' + d : d);
    }

    $bus.$on('app-document-insert:submit', function (data) {
        if (currentEditorMarkitup) {
            if (data.options.useLink) {
                $(currentEditorMarkitup.textarea).lsEditor('insert', '<a href="' + data.item_url + '" class="document-link">' + data.item_title + '</a>');
            } else {
                $(currentEditorMarkitup.textarea).lsEditor('insert', '<lsdocs items="' + data.items.join(',') + '" list="' + (data.options.useList ? 1 : 0) + '" title="' + data.options.title + '" >');
            }
            $('#modal-media-document-insert').lsModal('hide');
        }
    });

    $('.js-branch-news-repost').each(function (k, v) {
        v = $(v);
        var id = v.data('id');
        var url = aRouter.branch + 'ajax/news-repost/modal';
        v.lsModalToggle({
            url: url,
            modalOptions: {
                center: false
            },
            params: {
                id: id
            }
        });
        v.bind('lsmodaltoggleshow', function () {
            $('.js-select2').select2();
            ls.ajax.form(aRouter.branch + 'ajax/news-repost', '.js-branch-news-repost-form', function (result) {
                $('#modal-branch-news-repost').lsModal('hide');
            });
        }.bind(this));
    }.bind(this));

    $('.js-event-user-add').each(function (k, v) {
        v = $(v);
        var id = v.data('id');
        var url = aRouter.branch + 'ajax/event/add-user/modal';
        v.lsModalToggle({
            url: url,
            modalOptions: {
                center: false
            },
            params: {
                id: id
            }
        });
        v.bind('lsmodaltoggleshow', function () {
            ls.ajax.form(aRouter.branch + 'ajax/event/add-user', '.js-event-user-add-form', function (result) {
                $('#modal-branch-event-user-add').lsModal('hide');
            });

            new Vue({
                el: $('#js-vue-event-user-add').get(0),
                data: {
                    user: null
                },
                methods: {
                    action: function(data) {
                        if (data.item) {
                            $('#user-add-user-id-field').val(data.item.id);
                            this.$refs['search'].closeSearch();
                        }
                        this.user=data.item;
                    }
                }
            });
        }.bind(this));
    }.bind(this));

    if ($('.js-input-color').length) {
        var picker = new Pickr({
            el: '.js-color-picker',
            default: '',
            swatches: [
                '#F44336',
                '#E91E63',
                '#9C27B0',
                '#673AB7',
                '#3F51B5',
                '#2196F3',
                '#03A9F4',
                '#00BCD4',
                '#009688',
                '#4CAF50',
                '#8BC34A',
                '#CDDC39',
                '#FFEB3B',
                '#FFC107'
            ],
            components: {
                preview: true,
                opacity: false,
                hue: true,
                interaction: {
                    hex: false,
                    rgba: false,
                    hsva: false,
                    input: true,
                    clear: true,
                    save: true
                }
            },
            onSave: function (hsva, instance) {
                var field = $(instance.options.el).data('field');
                if (hsva) {
                    var hex = hsva.toHEX();
                    $(field).val('#' + hex[0] + hex[1] + hex[2]);
                } else {
                    $(field).val('');
                }
            }
        });
        if ($('.js-input-color').val()) {
            picker.setColor($('.js-input-color').val());
        }
    }

    $('.js-dadata-address').suggestions({
        token: ls.registry.get('dadata.token'),
        type: "ADDRESS",
        count: 7,
        hint: false,
        constraints: {
            label: "",
            locations: {"country": "*"}
        },
        onSelect: function (suggestion) {
            $('.js-dadata-address-lat').val(suggestion.data.geo_lat).change();
            $('.js-dadata-address-lon').val(suggestion.data.geo_lon).change();
        }.bind(this)
    });


    $('.js-content-search-form').keyup(function (e) {
        if (e.keyCode == 13) {
            $('.js-content-search-form-submit').trigger('click');
        }
    });
    $('.js-content-search-form-submit').click(function () {
        var type = $(this).data('type');
        var form = $('.js-content-search-form[data-type="' + type + '"]');
        var search = JSON.stringify(form.serializeJSON());
        if ($('.js-content-' + type + '-table-list').length) {
            $('.js-content-' + type + '-table-list').DataTable().search(search, false, false).draw();
        }
    });
    $('.js-content-search-form').submit(function () {
        return false;
    });
});

