/**
 * Аякс поиск
 *
 * @license   GNU General Public License, version 2
 * @copyright 2013 OOO "ЛС-СОФТ" {@link http://livestreetcms.com}
 * @author    Denis Shakhov <denis.shakhov@gmail.com>
 */

(function($) {
    "use strict";

    $.widget( "livestreet.lsSearchAjax", $.livestreet.lsComponent, {
        /**
         * Дефолтные опции
         */
        options: {
            // Ссылки
            urls: {
                search: null
            },

            // Селекторы
            selectors: {
                list: '.js-search-ajax-list',
                more: '.js-search-ajax-more',
                title: null
            },

            // Локализация
            i18n: {
                title: null
            },

            // Фильтры
            filters : [],

            // Парметры передаваемый при аякс запросе
            params : {},

            updatelist: null
        },

        /**
         * Конструктор
         *
         * @constructor
         * @private
         */
        _create: function () {
            this._super();

            this._currentXHR = null;

            this._filterTypes = {
                text: function (element, name, set, update) {
                    element.on('keyup', function (event) {
                        set(name, event.currentTarget.value);
                        ls.timer.run( this, update.bind(this, name), null, null, 300 );
                    });
                },
                select: function (element, name, set, update) {
                    element.on('change', function (event) {
                        set(name, event.currentTarget.value);
                        update(name);
                    });
                },
                radio: function (element, name, set, update) {
                    element.on('change', function (event) {
                        if ( ! event.currentTarget.checked) return;

                        set(name, event.currentTarget.value);
                        update(name);
                    });
                },
                checkbox: function (element, name, set, update) {
                    element.on('change', function (event) {
                        var value = $(event.currentTarget).is( ':checked' ) ? 1 : 0;
                        set(name, value);
                        update(name);
                    });
                },
                company: function (element, name, set, update) {
                    element.on('bnkfieldcompanyselect', function (event, suggestion) {
                        set(name, suggestion.data);
                        update(name);
                    });
                }
            };

            // Иниц-ия фильтров
            $.each( this.option( 'filters' ), function ( index, value ) {
                this._initFilter( value );
            }.bind(this));

            // Кнопка подгрузки
            this.elements.more.lsMore({
                target: this.elements.list,
                urls: {
                    load: this.option( 'urls.search' )
                },
                beforeload: function ( event, context ) {
                    $.extend( context.option( 'params' ), this.option( 'params' ) );
                }.bind(this)
            });
        },

        /**
         * Добавление фильтра
         */
        addFilter: function( filter ) {
            this.option('filters').push(filter);
            this._initFilter( filter );
        },

        /**
         *
         */
        addFilterType: function( filterType ) {

        },

        /**
         * Иниц-ия фильтра
         */
        _initFilter: function( filter ) {
            this._filterTypes[filter.type].call(
                this,
                $.type(filter.selector) === 'string' ? this.element.find(filter.selector) : filter.selector,
                filter.name,
                this.setParam.bind(this),
                this.update.bind(this)
            );
        },

        /**
         * Установка параметра
         */
        setParam: function( name, value ) {
            this.option( 'params.' + name, value );
            // this.updateUrl();
        },

        /**
         * Получение параметра
         */
        getParam: function( name ) {
            return this.option( 'params.' + name );
        },

        /**
         * Обновление поиска
         */
        update: function() {
            if (this._currentXHR) this._currentXHR.abort();

            this._trigger('beforeupdate');

            this._currentXHR = this._load( 'search', 'onUpdate', { showProgress: false });
        },

        /**
         *
         */
        onUpdate: function ( response ) {
            if ($.isFunction(this.options.updatelist)) {
                this.options.updatelist(response);
            } else {
                this.elements.list.show().html(response.html.trim());
            }

            this._currentXHR = null;

            this._trigger( 'afterupdate', null, { element: this.element, response: response } );
        },

        /**
         * Обновляет ссылку на основе параметров
         */
        updateUrl: function () {
            window.history.pushState( {}, 'Search', window.location.origin + window.location.pathname + '?' + $.param( this.option( 'params' ) ) );
        }
    });
})( jQuery );
;
Vue.component('app-bookmark', {
    template: '#app-template-bookmark',
    props: ['targetType', 'targetId', 'initState'],
    data: function () {
        return {
            state: false,
            count: 0,
            isProcessing: false
        }
    },
    watch: {},
    created: function () {
        this.state = this.initState ? true : false;
    },
    mounted: function () {

    },
    computed: {
        title: function () {
            return this.state ? 'Удалить из избранного' : 'Добавить в избранное';
        }
    },
    methods: {
        toggle: function (event) {
            event.preventDefault();
            this.isProcessing = true;
            ls.ajax.load('bookmark/toggle', {'target_type': this.targetType, 'target_id': this.targetId}, function (res) {
                this.state = res.state ? true : false;
                this.count = res.count;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessing = false;
                }.bind(this)
            });
        }
    }
});

