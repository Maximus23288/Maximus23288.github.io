//# sourceMappingURL=index.js.map;
Vue.component('select2', {
    props: ['options', 'value', 'multiple', 'paramsInit'],
    template: '<select :multiple="multipleSelect"><slot></slot></select>',
    data: function () {
        return {
            multipleSelect: false
        }
    },
    mounted: function () {
        this.params = this.paramsInit || {};
        var vm = this;

        var params = $.extend({}, this.params, {data: this.options});
        $(this.$el)
        // init select2
            .select2(params)
            .val(this.value)
            .trigger('change')
            // emit event on change.
            .on('change', function () {
                vm.$emit('input', $(this).val())
            });
    },
    created: function () {
        this.multipleSelect = !!(this.multiple);
    },
    watch: {
        value: function (value) {
            // update value
            if (!this.multipleSelect) {
                $(this.$el).val(value).trigger('change');
            }
        },
        options: function (options) {
            // update options
            var params = $.extend({}, this.params, {data: this.options});
            $(this.$el).empty().select2(params);
            if (!this.multipleSelect) {
                $(this.$el).val(this.value).trigger('change');
            }
        }
    },
    methods: {
        triggerChange: function () {
            $(this.$el).select2();
            $(this.$el).val(this.value).trigger('change');
        },
        addOption: function (text, id, selected) {
            var newOption = new Option(text, id, selected, true);
            $(this.$el).append(newOption).trigger('change');
        }
    },
    destroyed: function () {
        $(this.$el).off().select2('destroy')
    }
});
Vue.prototype.$t = function (name, replaceStrings) {
    return ls.lang.get(name, replaceStrings);
};
Vue.filter('t', function (value) {
    if (!value) {
        return '';
    }
    return ls.lang.get(value);
});

var VueLangPlugin = {
    install: function (Vue, options) {
        Vue.mixin({
            created: function () {
                _.forEach(this.$attrs, function (value, name) {
                    var nameParts = name.split('.');
                    if (nameParts.length > 1) {
                        var nameProp = nameParts.shift();
                        if (nameParts.indexOf("t") != -1) {
                            if (typeof this.$props[nameProp] !== 'undefined') {
                                this.$props[nameProp] = this.$t(value);
                            }
                        }
                    }
                }.bind(this));
            }
        });
    }
};
Vue.use(VueLangPlugin);
Vue.component('v-clappr', {
    props: ['options', 'src', 'poster'],
    template: '<div class=""></div>',
    data: function () {
        return {}
    },
    mounted: function () {
        this.player = new Clappr.Player({
            source: this.src,
            poster: this.poster,
            parent: this.$el,
            width: '100%',
            //height: '100%',
            events: {
                onReady: function () {
                    this.core.$el.addClass('js-clappr-wrapper')
                }
            }
        });
    },
    created: function () {

    },
    watch: {},
    methods: {},
    destroyed: function () {
        this.player.destroy();
    }
});
