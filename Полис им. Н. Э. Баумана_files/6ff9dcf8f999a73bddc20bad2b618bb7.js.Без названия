Vue.component('app-subscribe', {
    template: '#app-template-subscribe',
    props: ['targetType', 'targetId', 'initState', 'userId'],
    data: function () {
        return {
            state: false,
            count: 0,
            isProcessing: false
        }
    },
    watch: {},
    created: function () {
        this.state = this.initState ? true : false;
    },
    mounted: function () {

    },
    computed: {
        title: function () {
            return this.state ? "Убрать новости филиала<br />из моей ленты" : "Добавить новости филиала<br />в мою ленту";
        }
    },
    methods: {
        toggle: function (event) {
            event.preventDefault();

            if (!this.userId) {
                $('.js-sso-auth').trigger('click');
                return;
            }

            this.isProcessing = true;
            ls.ajax.load('subscribe/toggle', {
                'target_type': this.targetType,
                'target_id': this.targetId
            }, function (res) {
                this.state = res.state ? true : false;
                this.count = res.count;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessing = false;
                }.bind(this)
            });
        }
    }
});

$(function ($) {
    if ($('.js-app-subscribe').length) {
        $('.js-app-subscribe').each(function (k, v) {
            new Vue({
                el: v,
                data: {},
                components: {}
            });
        });
    }
});
Vue.component('app-like', {
    template: '#app-template-like',
    props: ['initIsLiked', 'initUsers', 'isBig', 'showUsers', 'showLastUser', 'initCount', 'targetType', 'targetId'],
    data: function () {
        return {
            count: 0,
            users: [],
            isLiked: false,
            isProcessing: false,
            titleAction: 'Поставить лайк'
        }
    },
    created: function () {
        if (this.initCount) {
            this.count = this.initCount;
        }
        if (this.initIsLiked) {
            this.isLiked = this.initIsLiked;
            this.titleAction = 'Вы уже поставили лайк';
        }
        if (this.initUsers) {
            this.users = this.initUsers;
        }
    },
    computed: {
        iconColor: function () {
            if (this.isLiked) {
                return '#8DBF59';
            } else {
                return '';
            }
        },
        moreUsers: function () {
            var count = this.count - this.users.length;
            return count > 0 ? count : 0;
        },

        lastUser: function () {
            if (this.users.length) {
                if (this.showLastUser) {
                    return this.users[this.users.length - 1];
                }
            }
            return null;
        }
    },
    methods: {
        create: function () {
            if (this.isProcessing || this.isLiked) {
                return;
            }
            this.isProcessing = true;
            ls.ajax.load('like/create', {
                target_type: this.targetType,
                target_id: this.targetId
            }, function (response) {
                this.count = response.count;
                this.users = response.users;
                this.isLiked = true;
                this.isProcessing = false;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessing = false;
                }.bind(this)
            });
        }
    }
});

$(function ($) {
    if ($('.js-app-like').length) {
        $('.js-app-like').each(function (k, v) {
            new Vue({
                el: v,
                data: {},
                components: {}
            });
        });
    }
});
Vue.use(window["vue-js-modal"].default);
Vue.component('app-map-point', {
    template: '#app-template-map-point',
    props: ['initLat', 'initLon'],
    data: function () {
        return {
            modalName: 'modal-map',
            useModal: false,
            map: null,
            marker: null,
            lat: null,
            lon: null
        }
    },
    watch: {},
    created: function () {
        if (this.initLat) {
            this.lat = this.initLat;
        } else {
            this.lat = '55.7535748'; // Кремль
        }
        if (this.initLon) {
            this.lon = this.initLon;
        } else {
            this.lon = '37.6211235';
        }
    },
    mounted: function () {
        this.initMap();
    },
    computed: {},
    methods: {
        show: function () {
            this.$modal.show(this.modalName);
        },
        hide: function () {
            this.$modal.hide(this.modalName);
        },
        initMap: function () {
            this.map = L.map(this.$refs.map).setView([Number(this.lat), Number(this.lon)], 15);
            this.map.scrollWheelZoom.disable();
            L.tileLayer('http://{s}.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png', {
                maxZoom: 18,
                attribution: '&copy; <a href="http://www.openstreetmap.org/copyright">OpenStreetMap</a> contributors, &copy; <a href="https://carto.com/attributions">CARTO</a>'
            }).addTo(this.map);

            this.marker = L.marker([Number(this.lat), Number(this.lon)], {draggable: true}).addTo(this.map);
            this.marker.on('moveend', function () {
                this.lat = this.marker.getLatLng().lat;
                this.lon = this.marker.getLatLng().lng;
                this.save();
            }.bind(this));
        },
        opened: function () {
            if (!this.map) {
                this.initMap();
            }
        },
        closed: function () {
            if (this.map) {
                this.map.remove();
                this.map = null;
                this.marker = null;
            }
        },
        save: function () {
            this.$emit('save', {lat: this.lat, lon: this.lon});
            if (this.useModal) {
                this.hide();
            }
        },
        update: function (lat, lon) {
            var latlon = [Number(lat), Number(lon)];
            this.marker.setLatLng(latlon);
            this.map.flyTo(latlon);
        }
    }
});
Vue.component('app-profile-sport', {
    template: '#app-template-profile-sport',
    props: {
        id: {
            default: ''
        }
    },
    data: function () {
        return {
            items: [],
            isProcessingSave: false,
            isProcessingLoad: false
        }
    },
    watch: {},
    created: function () {
        this.loadData();
    },
    mounted: function () {

    },
    methods: {
        loadData: function () {
            this.isProcessingLoad = true;
            ls.ajax.load(aRouter.my + 'ajax/sport-data-load', {
                id: this.id
            }, function (res) {
                this.items = res.items;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoad = false;
                }.bind(this)
            });
        },
        save: function (item) {
            if (this.isProcessingSave) {
                return;
            }
            if (item.use) {
                if (this.countUsed > 2) {
                    var index = this.getItemIndexById(item.id);
                    this.$nextTick(function(){
                        this.$set(this.items[index], 'use', false);
                    });
                    ls.msg.error('Выбрать можно не больше двух вариантов');
                    return;
                }
            }
            this.isProcessingSave = true;
            ls.ajax.load(aRouter.my + 'ajax/sport-save', {
                user_id: this.id,
                id: item.id,
                use: item.use ? 1 : 0
            }, function (res) {

            }.bind(this), {
                onComplete: function () {
                    this.isProcessingSave = false;
                }.bind(this)
            });
        },
        getItemIndexById: function (id) {
            var indexReturn = null;
            _.forEach(this.items, function (v, index) {
                if (v.id == id) {
                    indexReturn = index;
                    return false;
                }
            });
            return indexReturn;
        }
    },
    computed: {
        countUsed: function () {
            var count = 0;
            _.forEach(this.items, function (item) {
                if (item.use) {
                    count++;
                }
            });
            return count;
        }
    }
});

$(function ($) {
    if ($('#app-profile-sport').length) {
        new Vue({
            el: '#app-profile-sport',
            data: {},
            components: {}
        });
    }
});
Vue.component('app-paging-item', {
    template: '<li :class="classObject"><span v-if="isActive || !page" class="ls-pagination-item-inner">{{ textPage }}</span><a v-else class="ls-pagination-item-inner ls-pagination-item-link" href="#" @click.stop.prevent="click">{{ textPage }}</a></li>',
    props: ['isActive', 'page', 'text'],
    data: function () {
        return {};
    },
    computed: {
        classObject: function () {
            return {
                active: this.isActive,
                'ls-pagination-item': true
            }
        },
        textPage: function () {
            return this.text || this.page;
        }
    },
    methods: {
        click: function () {
            this.$emit('selectPage', {page: this.page})
        }
    }
});
Vue.component('app-paging', {
    template: '#app-template-paging',
    props: {
        total: {
            default: 0
        },
        current: {
            default: 1
        },
        padding: {
            default: 2
        }
    },
    data: function () {
        return {}
    },
    computed: {
        start: function () {
            var start = 1;
            if (this.total > this.padding * 2 + 1) {
                start = ( (this.current - this.padding) < 4 ) ? 1 : this.current - this.padding;
            }
            return start;
        },
        end: function () {
            var end = this.total;
            if (this.total > this.padding * 2 + 1) {
                end = ( (this.current + this.padding) > (this.total - 3) ) ? this.total : this.current + this.padding;
            }
            return end;
        },
        textPage: function () {
            return this.text || this.page;
        },
        arrayPages: function () {
            var items = [];
            for (var i = this.start; i < this.end + 1; i++) {
                items.push(i);
            }
            return items;
        }
    },
    methods: {
        selectPage: function (data) {
            this.$emit('selectPage', {page: data.page})
        }
    }
});
jQuery(function($){
    if ($('.js-toolbar').length && !!$.trim($('.js-toolbar').html()).length) {
        $('.js-toolbar').show();
    }
});
(function($) {
    "use strict";

    $.widget( "livestreet.lsBranchNewsAdd", {
        /**
         * Дефолтные опции
         */
        options: {
            // Ссылки
            urls: {
                add: aRouter.branch + 'ajax/news-create/',
                edit: aRouter.branch + 'ajax/news-update/'
                //preview: aRouter[ 'content' ] + 'ajax/preview/'
            },

            // Селекторы
            selectors: {
                image_preview: '.js-branch-news-add-field-image-preview'
            }
        },

        /**
         * Конструктор
         *
         * @constructor
         * @private
         */
        _create: function () {
            var _this = this;

            this.elements = {
                image_preview: this.element.find( this.option( 'selectors.image_preview' ) ),
                blogs: this.element.find( this.option( 'selectors.blogs' ) ),
                buttons: {
                    submit: this.element.find( this.option( 'selectors.buttons.submit' ) )
                }
            };

            // Иниц-ия формы
            this.element.lsContent({
                urls: {
                    add: this.option( 'urls.add' ),
                    edit: this.option( 'urls.edit' )
                },
                callbacks: {
                    beforeSubmit: function(){}
                }
            });

            // Превью (изображение)
            this.elements.image_preview.lsFieldImageAjax({
                urls: {
                    add: this.option( 'urls.add' ),
                    edit: this.option( 'urls.edit' )
                }
            });
        }
    });
})(jQuery);
