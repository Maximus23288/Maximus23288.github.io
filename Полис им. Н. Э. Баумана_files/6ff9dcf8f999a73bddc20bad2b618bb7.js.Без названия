Vue.use(window["vue-js-modal"].default);
Vue.component('app-plugin-stashable-modal-request', {
    template: '#app-template-plugin-stashable-modal-request',
    props: {},
    data: function () {
        return {
            modalName: 'request.modal',
            isShowModal: false,
            callbackOk: null,
            callbackCancel: null,
            data: null,
            stateOk: false
        }
    },
    watch: {},
    mounted: function () {

    },
    created: function () {
        $bus.$on('plugin-stashable:modal-show', function (data) {
            this.data = data.data;
            this.callbackOk = data.callbackOk || null;
            this.callbackCancel = data.callbackCancel || null;
            this.showModal();
        }.bind(this));
    },
    computed: {
        time: function () {
            if (this.data && this.data.time) {
                return moment(this.data.time * 1000).format('DD MMMM HH:mm:ss');
            }
            return null;
        }
    },
    methods: {
        ok: function () {
            this.stateOk = true;
            this.closeModal();
        },
        showModal: function () {
            this.stateOk = false;
            this.$modal.show(this.modalName);
        },
        closeModal: function () {
            this.$modal.hide(this.modalName);
        },
        onModalOpened: function () {
            this.isShowModal = true;
        },
        onModalClosed: function () {
            this.isShowModal = false;
            if (this.stateOk) {
                if (this.callbackOk) {
                    this.callbackOk();
                }
            } else {
                if (this.callbackCancel) {
                    this.callbackCancel();
                }
            }
        }
    }
});

$(function ($) {
    if ($('.js-app-plugin-stashable-modal-request').length) {
        $('.js-app-plugin-stashable-modal-request').each(function () {
            var el = $(this).get(0);
            new Vue({
                el: el,
                data: {}
            });
        });
    }
});
Vue.component('app-plugin-attach', {
    template: '#app-template-plugin-attach',
    props: {
        targetType: {
            default: null
        },
        targetId: {
            default: null
        },
        title: {
            default: 'Загрузить изображение'
        },
        initItem: {
            default: null
        },
        itemJson: {
            default: null
        },
        fieldSave: {
            default: null
        },
        showName: {
            default: false
        },
        withLink: {
            default: true
        },
        iconPath: {
            default: null
        },
        allowChange: {
            default: true
        },
        allowTypes: {
            default: function () {
                return ['file']
            }
        }

    },
    data: function () {
        return {
            item: null,
            uid: null,
            errorMsg: null
        }
    },
    watch: {
        item: {
            handler: function (newValue) {
                if (!newValue || (!newValue.uploading)) {

                }
            },
            deep: true
        },
        initItem: function (newVal) {
            this.item = newVal || null;
        }
    },
    computed: {
        getIconPath: function () {
            if (this.iconPath) {
                return this.iconPath;
            } else {
                return PATH_SKIN + '/assets/images/icons/epub.svg';
            }
        }
    },
    mounted: function () {
        var fileupload = {
            url: aRouter.ajax + 'plugin-attach/upload',
            sequentialUploads: false,
            singleFileUploads: true,
            limitConcurrentUploads: 1
        };

        var uploader = $(this.$refs.uploadInput);
        fileupload.dropZone = $(this.$refs.uploadArea);
        uploader.fileupload(fileupload);
        uploader.fileupload('option', 'formData', {
            security_ls_key: LIVESTREET_SECURITY_KEY,
            target_type: this.targetType,
            target_id: this.targetId
        });
        uploader.on({
            fileuploadadd: this.onUploadAdd.bind(this),
            fileuploaddone: function (event, data) {
                this[data.result.bStateError ? 'onUploadError' : 'onUploadDone'](data.context, data.result);
            }.bind(this),
            fileuploadfail: function (event, data) {
                this['onUploadError'](data.context, data.result);
            }.bind(this),
            fileuploadprogress: function (event, data) {
                this.onUploadProgress(data.context, parseInt(data.loaded / data.total * 100, 10));
            }.bind(this)
        });

        if (this.fieldSave) {
            $(this.$refs.fieldSave).val(this.item ? this.item.uid : '');
        }
    },
    created: function () {
        this.uid = this.generateUid();
        if (this.initItem) {
            this.item = this.initItem;
        } else {
            if (this.itemJson) {
                this.item = JSON.parse(this.itemJson);
            }
        }

        this.$on('clear', function () {
            this.item = null;
        });
    },
    filters: {},
    methods: {
        isAllowType: function (type) {
            return this.allowTypes.indexOf(type) != -1;
        },
        onUploadAdd: function (event, file) {
            var item = {
                'uid': this.generateUid(),
                'uploading': true,
                'progress': 0
            };
            file.context = item.uid;
            this.item = item;
            this.errorMsg = null;
        },
        onUploadError: function (context, result) {
            this.item.uploading = false;
            this.errorMsg = (result && result.sMsg) ? result.sMsg : 'Ошибка загрузки';
            this.item = null;
        },
        onUploadDone: function (context, result) {
            if (result.item) {
                this.item = result.item;
                this.emitUpdate();
            } else {
                this.onUploadError(context, result);
            }
        },
        onUploadProgress: function (context, percent) {
            this.item.progress = percent;
        },
        generateUid: function () {
            return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function (c) {
                var r = Math.random() * 16 | 0,
                    v = c == 'x' ? r : (r & 0x3 | 0x8);
                return v.toString(16);
            });
        },
        removeItem: function (noConfirm) {
            if (!noConfirm && !confirm('Действительно удалить?')) {
                return;
            }
            var uid = this.item.uid;
            ls.ajax.load('plugin-attach/remove', {
                uid: uid
            }, function (res) {
                this.item = null;
                this.emitUpdate();
            }.bind(this), {
                showNotices: true
            });
        },
        getItem: function () {
            return this.item;
        },
        emitUpdate: function () {
            this.$emit('update', {
                item: this.item
            });
            if (this.fieldSave) {
                $(this.$refs.fieldSave).val(this.item ? this.item.uid : '');
            }
        },
        showRequestUrl: function () {
            var url = prompt('Введите URL');
            if (!url) {
                return;
            }
            this.createByUrl(url);
        },
        createByUrl: function (url) {
            var item = {
                'uid': this.generateUid(),
                'uploading': true,
                'progress': 0
            };
            this.item = item;
            this.errorMsg = null;

            ls.ajax.load('plugin-attach/create-url', {
                target_type: this.targetType,
                target_id: this.targetId,
                url: url
            }, function (res) {
                this.item = res.item;
                this.emitUpdate();
            }.bind(this), {
                onError: function (result) {
                    this.errorMsg = (result && result.sMsg) ? result.sMsg : 'Ошибка загрузки';
                    this.item = null;
                }.bind(this)
            });
        }
    }
});

$(function ($) {
    if ($('.js-app-plugin-attach').length) {
        $('.js-app-plugin-attach').each(function () {
            var el = $(this).get(0);
            new Vue({
                el: el,
                data: {}
            });
        });
    }
});
Vue.component('app-plugin-attach-many', {
    template: '#app-template-plugin-attach-many',
    props: {
        targetType: {
            default: null
        },
        targetId: {
            default: null
        },
        title: {
            default: 'Загрузить изображение'
        },
        fieldSave: {
            default: null
        },
        iconPath: {
            default: null
        },
        showName: {
            default: false
        },
        withLink: {
            default: true
        },
        initItems: {
            default: function () {
                return []
            }
        },
        countMax: {
            default: null
        },
        allowChange: {
            default: true
        },
        allowTypes: {
            default: function () {
                return ['file']
            }
        }
    },
    data: function () {
        return {
            items: [],
            initCompleted: false
        }
    },
    watch: {
        items: {
            handler: function (newVal, oldVal) {
                if (newVal.length) {
                    var needNew = true;
                    _.forEach(newVal, function (v, k) {
                        if (!v.uid) {
                            needNew = false;
                        }
                    });
                    if (needNew) {
                        this.addNew();
                    }
                }
                this.emitUpdate();

            },
            deep: true
        },
        allowChange: function () {
            if (!this.allowChange) {
                _.forEach(this.items, function (item, k) {
                    if (!item.uid) {
                        this.items.splice(k, 1);
                    }
                }.bind(this));
            }
        }
    },
    computed: {
        itemsShow: function () {
            if (this.countMax) {
                return this.items.slice(0, this.countMax);
            }
            return this.items;
        }
    },
    mounted: function () {

    },
    created: function () {
        if (this.initItems && this.initItems.length) {
            _.forEach(this.initItems, function (item) {
                this.addNew(item);
            }.bind(this));
        }
        if (!this.items.length) {
            this.addNew();
        }
        this.initCompleted = true;
    },
    filters: {},
    methods: {
        addNew: function (initItem) {
            var item = $.extend({}, {uid: null, guid: this.generateUid()}, {});
            if (initItem) {
                item.init = initItem;
                item.uid = initItem.uid;
                this.items.push(item);
            } else {
                if (this.allowChange) {
                    this.items.push(item);
                }
            }
        },
        updateItem: function (data, index) {
            var uid = data.item ? data.item.uid : null;

            if (uid) {
                this.$set(this.items[index], 'uid', uid);
            } else {
                this.$delete(this.items, index);
            }
        },
        generateUid: function () {
            return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function (c) {
                var r = Math.random() * 16 | 0,
                    v = c == 'x' ? r : (r & 0x3 | 0x8);
                return v.toString(16);
            });
        },
        emitUpdate: function () {
            if (!this.initCompleted) {
                return;
            }
            var uids = [];
            if (this.items.length) {
                _.forEach(this.items, function (item) {
                    if (item.uid) {
                        uids.push(item.uid);
                    }
                });
            }
            this.$emit('update', {
                items: uids
            });
            if (this.fieldSave) {
                $(this.$refs.fieldSave).val(JSON.stringify(uids));
            }
        }
    }
});

$(function ($) {
    if ($('.js-app-plugin-attach-many').length) {
        $('.js-app-plugin-attach-many').each(function () {
            var el = $(this).get(0);
            new Vue({
                el: el,
                data: {}
            });
        });
    }
});
Vue.component('app-editor', {
    template: '#app-template-editor',
    props: {
        initData: {
            default: null
        },
        debug: {
            default: false
        },
        fieldName: {
            default: null
        },
        defaultBlocks: {
            default: function () {
                return [{
                    type: 'text'
                }]
            }
        },
        blocks: {
            default: function () {
                return {
                    'header': {
                        type: 'header',
                        params: {}
                    },
                    'text': {
                        type: 'text',
                        params: {}
                    }
                }
            }
        }
    },
    data: function () {
        return {
            contents: [],
            activeContent: null,
            showControlItems: false,
            dataForSave: [],
            version: "1.0.0",
            buttons: []
        };
    },
    watch: {
        showControlItems: function (newV, oldV) {

        },
        activeContent: function (content) {
            this.showControlItems = false;
            var block = this.getBlock(content);
            block.$emit('onActive');
        },
        dataForSave: function () {
            if (this.fieldName) {
                //$(this.$refs.fieldSave).val(JSON.stringify(this.dataForSave));
            }
        },
        contents: function () {
            $bus.$emit('app-editor-contents-update', {
                contents: this.contents
            });
        }
    },
    created: function () {
        _.forEach(this.blocks, function (block, name) {
            if (!block.type) {
                block.type = name;
                this.$set(this.blocks, name, block);
            }
            var cmp = Vue.component('app-editor-block-' + block.type);
            var cmpClass = Vue.extend(cmp);
            var cmpObject = new cmpClass();

            var button = cmpObject.button();
            if (button) {
                button.name = name;
                this.buttons.push(button);
            }
        }.bind(this));


        var initBlocks = [];
        if (this.initData && this.initData.blocks) {
            if (this.initData.blocks) {
                initBlocks = this.initData.blocks;
            }
        } else {
            initBlocks = this.defaultBlocks;
        }
        _.forEach(initBlocks, function (block, index) {
            var name = block.name || block.type;
            if (this.isAllowType(name)) {
                this.addContentBlock(name, block.data);
            }
        }.bind(this));
    },
    mounted: function () {

    },
    filters: {},
    computed: {
        pathAssets: function () {
            return PATH_SKIN + '/assets';
        }
    },
    methods: {
        isAllowType: function (name) {
            if (name && (typeof (this.blocks[name]) != 'undefined')) {
                return true;
            }
            return false;
        },
        addContentBlock: function (name, data) {
            var block = this.blocks[name];
            if (!block || (typeof (block) == 'undefined')) {
                return;
            }
            var content = {
                uuid: this.generateUuid(),
                type: block.type,
                settings: [],
                data: data || {},
                params: block.params || {}
            };
            this.contents.push(content);
        },
        generateUuid: function () {
            return ([1e7] + -1e3 + -4e3 + -8e3 + -1e11).replace(/[018]/g, function (c) {
                return (c ^ crypto.getRandomValues(new Uint8Array(1))[0] & 15 >> c / 4).toString(16);
            });
        },
        getBlock: function (content) {
            return this.$refs[content.uuid][0];
        },
        save: function () {
            var blocks = [];
            _.forEach(this.contents, function (content, index) {
                var data = this.getBlock(content).save();
                if (!this.getBlock(content).validate(data)) {
                    return;
                }
                blocks.push({
                    type: content.type,
                    data: data
                });
            }.bind(this));
            this.dataForSave = {
                time: Date.now(),
                version: this.version,
                blocks: blocks
            };
            return this.dataForSave;
        },
        move: function (indexFrom, indexTo) {
            this.contents.splice(indexTo, 0, this.contents.splice(indexFrom, 1)[0]);
        },
        findContent: function (content) {
            var indexFind = -1;
            _.forEach(this.contents, function (item, index) {
                if (item.uuid == content.uuid) {
                    indexFind = index;
                    return;
                }
            });
            return indexFind;
        },
        isAllowMoveUp: function (content) {
            var indexFrom = this.findContent(content);
            var indexTo = indexFrom - 1;
            if (indexTo >= 0) {
                return true;
            }
            return false;
        },
        isAllowMoveDown: function (content) {
            var indexFrom = this.findContent(content);
            var indexTo = indexFrom + 1;
            if (indexTo > 0 && indexTo < this.contents.length) {
                return true;
            }
            return false;
        },
        moveUp: function (content) {
            var indexFrom = this.findContent(content);
            if (indexFrom > -1) {
                var indexTo = indexFrom - 1;
                if (indexTo >= 0) {
                    this.move(indexFrom, indexTo);
                    this.scrollToContent(content);
                }
            }
        },
        moveDown: function (content) {
            var indexFrom = this.findContent(content);
            if (indexFrom > -1) {
                var indexTo = indexFrom + 1;
                if (indexTo < this.contents.length) {
                    this.move(indexFrom, indexTo);
                    this.scrollToContent(content);
                }
            }
        },
        scrollToContent: function (content) {
            var el = this.$refs[content.uuid][0].$el;
            var offset = el.getBoundingClientRect().top;
            this.$nextTick(function () {
                $.scrollTo(el, 200, {
                    offset: {
                        top: -1 * offset
                    }
                });
            }.bind(this));
        },
        remove: function (content) {
            var indexFrom = -1;
            _.forEach(this.contents, function (item, index) {
                if (item.uuid == content.uuid) {
                    indexFrom = index;
                    return;
                }
            });
            if (indexFrom > -1) {
                this.contents.splice(indexFrom, 1);
            }
        },
        setActive: function (content) {
            this.activeContent = content;
        },
        isActiveContent: function (content) {
            if (this.activeContent == content) {
                return true;
            }
            return false;
        },
        isShowContentControl: function (content) {
            return this.isActiveContent(content);
        },
        toggleControlItems: function () {
            this.showControlItems = !this.showControlItems;
        },
        isShowSelected: function (content) {
            return this.isShowContentControl(content);
        },
        isShowControlItems: function () {
            return this.showControlItems;
        },
        onRenderContent: function (content, event) {
            this.renderSettings(content);
            var makeActive = true;
            if (event) {
                if (!event.isNew) {
                    makeActive = false;
                }
            }
            if (makeActive) {
                this.setActive(content);
            }
        },
        renderSettings: function (content) {
            var settings = this.getBlock(content).settings();
            this.$set(content, 'settings', settings || []);
        },
        getSettingsItemData: function (data) {
            data = data || {};
            var result = {};
            _.forEach(data, function (value, key) {
                var k = 'data-' + key;
                result[k] = value;
            });
            return result;
        },
        runContentSettingsItem: function (content, data) {
            data = data || {};
            this.getBlock(content).onSettingsItem(data);
        },
        getApi: function () {
            return {
                remove: function (uuid) {
                    var indexFrom = -1;
                    _.forEach(this.contents, function (item, index) {
                        if (item.uuid == uuid) {
                            indexFrom = index;
                            return;
                        }
                    });
                    if (indexFrom > -1) {
                        this.contents.splice(indexFrom, 1);
                    }
                }.bind(this),
                setActive: function (uuid) {
                    var index = this.findContent({
                        uuid: uuid
                    });
                    if (index > -1) {
                        this.setActive(this.contents[index]);
                    }
                }.bind(this),
                generateUuid: function () {
                    return this.generateUuid();
                }.bind(this)
            };
        }
    }
});
Vue.component('app-editor-block-header', {
    template: '#app-template-editor-block-header',
    props: ['initData', 'api', 'uuid', 'initParams'],
    components: {},
    data: function () {
        return {
            initText: '',
            textSave: '',
            text: '',
            numbers: [2, 3],
            numberDefault: 2,
            numberCurrent: null,
            tagName: null
        };
    },
    watch: {
        numberCurrent: function (val) {
            if (val) {
                this.tagName = 'h' + val;
            }
        }
    },
    created: function () {
        if (this.initText) {
            this.text = this.initText;
        }
        this.$on('onActive', function () {
            this.onActive();
        }.bind(this));

        this.numberCurrent = this.numberDefault;
        this.processingData(this.initData);
    },
    mounted: function () {
        this.$nextTick(function () {
            this.$emit('onRender', {
                isNew: !this.initData.text
            });
        }.bind(this));
    },
    methods: {
        save: function () {
            return {
                text: this.trim(this.textSave),
                number: this.numberCurrent
            }
        },
        validate: function (data) {
            if (data.text) {
                return true;
            }
            return false;
        },
        settings: function () {
            return [{
                    class: 'btn-h2',
                    active: this.numberCurrent == 2,
                    data: {
                        number: 2
                    }
                },
                {
                    class: 'btn-h3',
                    active: this.numberCurrent == 3,
                    data: {
                        number: 3
                    }
                }
            ];
        },
        button: function () {
            return {
                class: 'btn-create-block btn-create-block_header',
                title: t('Добавить заголовок')
            }
        },
        processingData: function (data) {
            if (typeof data !== 'object') {
                return;
            }
            this.text = data.text || '';
            this.textSave = this.text;
            if (this.isAllowNumber(data.number)) {
                this.numberCurrent = data.number;
            }
        },
        isAllowNumber: function (number) {
            if (number && this.numbers.indexOf(number) != -1) {
                return true;
            }
            return false;
        },
        onSettingsItem: function (data) {
            this.text = this.textSave;
            this.numberCurrent = data.number;
            this.$emit('onRenderSettings');
        },
        trim: function (text) {
            text = text.trim();
            if (text != '<br>' && text != '<br/>') {
                return text;
            }
            return '';
        },
        onInput: function (e) {
            this.textSave = e.target.innerHTML;
            if (this.textSave.trim() == '<br>') {
                this.text = '___clear___'; // hack for trigger change variable at next tick
                this.$nextTick(function () {
                    this.text = '';
                }.bind(this));
            }
        },
        onActive: function () {
            this.$refs.header.focus();
        },
        onPaste: function (e) {
            var text = e.clipboardData.getData('Text');
            e.preventDefault();
            e.stopPropagation();
            if (text) {
                this.$refs.header.innerHTML = this.$refs.header.innerText + text;
                this.$refs.header.dispatchEvent(new Event('input'));
            }
            return false;
        }
    }
});
$(function ($) {
    if (typeof (Editor) == 'undefined') {
        Editor = {};
    }
    Vue.component('app-editor-block-text', {
        template: '#app-template-editor-block-text',
        props: ['initData', 'api', 'uuid', 'initParams'],
        components: {
            'editor': Editor
        },
        data: function () {
            return {
                content: '',
                params: {
                    target_id: null,
                    target_type: null,
                    owner_key: null
                },
                paramsEditor: {
                    plugins: 'quickbars, link, image, lists, code, table, powerpaste, lsmath',
                    table_appearance_options: false,
                    contextmenu: false,
                    // table_resize_bars: false,
                    table_responsive_width: true,
                    relative_urls: false,
                    document_base_url: PATH_ROOT,
                    remove_script_host: true,
                    menubar: false,
                    powerpaste_word_import: 'clean',
                    powerpaste_html_import: 'clean',
                    powerpaste_allow_local_images: false,
                    table_default_attributes: {
                        class: 'table'
                    },
                    min_height: 500,
                    extended_valid_elements: 'table[class=table]',
                    invalid_elements: 'colgroup,col',
                    table_default_styles: {},
                    toolbar_location: 'top',
                    fixed_toolbar_container: '#mytoolbar',
                    inline_boundaries_selector: 'ins',
                    language: LANGUAGE == 'ru' ? 'ru' : 'en',
                    toolbar: 'important blockquote | alignleft aligncenter alignright alignjustify | outdent indent | link  | numlist bullist | table lsmath | code',
                    quickbars_selection_toolbar: 'bold italic strikethrough superscript subscript',
                    quickbars_insert_toolbar: '',
                    quickbars_image_toolbar: '',
                    lsmath: {

                    },
                    formats: {
                        important: { block: 'p', classes: 'important' }
                    },
                    setup: function (editor) {
                        editor.ui.registry.addToggleButton('important', {
                            text: t('Важно'),
                            onAction: function (_) {
                                editor.execCommand('mceToggleFormat', false, 'important');
                            },
                            onSetup: function (api) {
                                editor.formatter.formatChanged('important', function (state) {
                                    api.setActive(state);
                                });
                            }
                        });
                    }
                }
            };
        },
        watch: {},
        created: function () {
            this.paramsEditor.fixed_toolbar_container = '#' + this.editorToolbarId;
        },
        mounted: function () {
            if (this.initParams) {
                _.forEach(this.initParams, function (value, name) {
                    this.$set(this.params, name, value);
                }.bind(this));
            }

            this.processingData(this.initData);
        },
        methods: {
            save: function () {
                return {
                    text: this.content
                }
            },
            validate: function (data) {
                return true;
            },
            settings: function () {

            },
            button: function () {
                return {
                    class: 'btn-create-block btn-create-block_text',
                    title: t('Добавить текстовый блок')
                }
            },
            onSettingsItem: function (data) {

            },
            processingData: function (data) {
                if (typeof data !== 'object') {
                    return;
                }
                this.content = data.text || '';
            },
            onEditorFocusIn: function () {
                this.api.setActive(this.uuid);
            }
        },
        computed: {
            editorToolbarId: function () {
                return 'editor-toolbar-' + this.uuid;
            }
        }
    });
});
Vue.use(window["vue-js-modal"].default);
Vue.component('app-editor-block-image', {
    template: '#app-template-editor-block-image',
    props: ['initData', 'api', 'uuid', 'initParams'],
    components: {},
    data: function () {
        return {
            items: [],
            params: {
                target_id: null,
                target_type: null,
                owner_key: null,
                media: null
            },
            paramsMedia: {
                allowSelectMany: true,
                softSelectMany: true
            },
            type: null,
            imageSettings: {
                size: 'original'
            },
            gallerySettings: {
                showPreview: false
            },
            modalName: null,
            isProcessingLoad: false,
            isVisible: false,
            isShowModal: false
        };
    },
    watch: {},
    created: function () {
        this.modalName = this.generateUuid();
    },
    mounted: function () {
        if (this.initParams) {
            _.forEach(this.initParams, function (value, name) {
                this.$set(this.params, name, value);
            }.bind(this));
            if (this.params.media) {
                _.forEach(this.params.media, function (value, name) {
                    this.$set(this.paramsMedia, name, value);
                }.bind(this));
            }
        }

        if (typeof this.initData == 'object' && this.initData.items) {
            this.processingData(this.initData);
        } else {
            this.showModal();
        }

        this.$nextTick(function () {
            this.$emit('onRender', {
                isNew: !this.items.length
            });
        }.bind(this));
    },
    computed: {
        mediaItem: function () {
            if (this.items.length) {
                return this.items[0];
            }
            return null;
        },
        mediaItemImage: function () {
            var item = this.mediaItem;
            if (item) {
                return this.getPreview(item, this.imageSettings.size);
            }
            return null;
        },
        itemsId: function () {
            var result = [];
            _.forEach(this.items, function (item, index) {
                result.push(item.id);
            });
            return result;
        }
    },
    methods: {
        save: function () {
            return {
                type: this.type,
                items: this.itemsId,
                settings: this.type == 'gallery' ? this.gallerySettings : this.imageSettings
            }
        },
        validate: function (data) {
            if (data.items && data.items.length) {
                return true;
            }
            return false;
        },
        settings: function () {
            return [{
                class: 'component-editor-btn_edit',
                data: {
                    action: 'edit'
                }
            }];
        },
        onSettingsItem: function (data) {
            if (data.action == 'edit') {
                this.showModal();
            }
        },
        button: function () {
            return {
                class: 'btn-create-block btn-create-block_image',
                title: t('Добавить фотогалерею')
            }
        },
        processingData: function (data) {
            if (typeof data !== 'object') {
                this.api.remove(this.uuid);
                return;
            }
            this.isVisible = true;
            this.type = data.type;
            if (this.type == 'gallery') {
                this.gallerySettings = data.settings
            } else {
                this.imageSettings = data.settings
            }
            this.load();
        },
        load: function () {
            this.isProcessingLoad = true;
            ls.ajax.load('plugin-media/load', {
                filter: {
                    ids: this.initData.items
                },
                target_type: this.params.target_type,
                target_id: this.params.target_id,
                owner_key: this.params.owner_key
            }, function (res) {
                this.items = res.items;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoad = false;
                }.bind(this)
            });
        },
        showModal: function () {
            this.$modal.show(this.modalName);
        },
        closeModal: function () {
            this.$modal.hide(this.modalName);
        },
        onModalOpened: function () {
            this.isShowModal = true;
        },
        onModalClosed: function () {
            if (this.items.length) {
                this.isVisible = true;
            } else {
                this.api.remove(this.uuid);
            }
            this.isShowModal = false;
        },
        generateUuid: function uuidv4() {
            return ([1e7] + -1e3 + -4e3 + -8e3 + -1e11).replace(/[018]/g, function (c) {
                return (c ^ crypto.getRandomValues(new Uint8Array(1))[0] & 15 >> c / 4).toString(16);
            });
        },
        actionCancel: function () {
            this.closeModal();
        },
        actionOk: function (data) {
            if (data.type == 'gallery') {
                this.type = data.type;
                this.items = data.items;
                _.forEach(data.settings, function (value, name) {
                    this.$set(this.gallerySettings, name, value);
                }.bind(this));
            }
            if (data.type == 'media') {
                this.type = data.type;
                this.items = data.items;
                _.forEach(data.settings, function (value, name) {
                    this.$set(this.imageSettings, name, value);
                }.bind(this));
            }
            this.closeModal();
        },
        getPreview: function (item, size) {
            size = size || 'preview-medium';
            if (item.sizes[size]) {
                var data = item.sizes[size];
                var s = data.w + 'x' + (data.h ? data.h : '');
                if (data.crop) {
                    s = s + 'crop';
                }
                var path = ls.utils.pathinfo(item.image);
                return path.dirname + '/' + path.filename + '_' + s + '.' + path.extension;
            }
            return item.image;
        },
        pluralize: function (count, words, skip) {
            return ls.lang.pluralize(count, words, null, skip);
        }
    }
});
$(function ($) {
    if (typeof (Editor) == 'undefined') {
        Editor = {};
    }
