    if ($('.js-app-branch-event-list').length) {
        $('.js-app-branch-event-list').each(function (k, v) {
            new Vue({
                el: v,
                data: {}
            });
        });
    }
});
Vue.component('app-branch-event-points-calc', {
    template: '#app-template-branch-event-points-calc',
    props: ['id'],
    data: function () {
        return {
            filter: {
                date_begin: null,
                date_end: null
            },
            pointsGeneral: null,
            pointsExtra: null,
            isProcessingLoad: false,
            isCalc: false,
        };
    },
    watch: {},
    created: function () {

    },
    computed: {},
    mounted: function () {
        var calendarParams = {
            format: 'Y-m-d',
            timepicker: false,
            theme: 'bmstu-calendar',
            dayOfWeekStart: 1,
            scrollInput: false,
            onChangeDateTime: function (ct, $i) {
                $i.get(0).dispatchEvent(new Event('input'));
            }
        };
        $(this.$refs.date_begin).datetimepicker(calendarParams);
        $(this.$refs.date_end).datetimepicker(calendarParams);
    },
    methods: {
        load: function () {
            this.isProcessingLoad = true;
            var params = $.extend({}, this.filter, {user_id: this.id});
            ls.ajax.load('event/points-calc', params, function (res) {
                this.pointsGeneral = res.pointsGeneral;
                this.pointsExtra = res.pointsExtra;
                this.isCalc = true;
            }.bind(this), {
                onComplete: function () {
                    this.isProcessingLoad = false;
                }.bind(this)
            });
        },
    }
});

