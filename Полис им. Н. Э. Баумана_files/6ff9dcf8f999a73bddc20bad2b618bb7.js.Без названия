jQuery(document).ready(function ($) {
    // Хук начала инициализации javascript-составляющих шаблона
    ls.hook.run('ls_template_init_start', [], window);

    /**
     * Иниц-ия модулей ядра
     */
    ls.init({
        production: false
    });

    ls.dev.init({});

    /**
     * Notification
     */
    ls.notification.init();

    /**
     * Form validate
     */
    $('.js-form-validate').parsley();

    /**
     * Modals
     */
    $('.js-modal-default').lsModal();

    /**
     * Editor
     */
    $('.js-editor-default').lsEditor();

    /**
     * Лайтбокс
     */
    $('a.js-lbx').lsLightbox({
        width: "100%",
        height: "100%"
    });

    /**
     * Каптча
     */
    $('[data-type=captcha]').livequery(function () {
        $(this).lsCaptcha();
    });

    $("[name^='#']").click(function () {
        var _href = $(this).attr("name");
        $("html, body").animate({
            scrollTop: $(_href).offset().top + "px"
        });
        return false;
    });

    $('[data-type=recaptcha]').livequery(function () {
        $(this).lsReCaptcha({
            key: ls.registry.get('recaptcha.site_key')
        });
    });

    /**
     * Toolbar
     */
    $('.js-toolbar-default').lsToolbar({
        target: '.js-root-container',
        offsetX: 10
    });
    $('.js-toolbar-scrollup').lsToolbarScrollUp();

    $('.autocomplete-property-tags-sep').each(function (k, v) {
        $(v).lsAutocomplete({
            multiple: true,
            urls: {
                load: aRouter.ajax + 'property/tags/autocompleter/'
            },
            params: {
                property_id: $(v).data('lsfieldPropertyId')
            }
        });
    });

    /**
     * Авторизация/регистрация
     */
    ls.auth.init();

    ls.ajax.form(aRouter.user + 'ajax/settings-profile/update', '.js-user-settings-profile-form', function (result) {

    });
    ls.ajax.form(aRouter.user + 'ajax/settings-education/update', '.js-user-settings-education-form', function (result) {

    });
    $('.js-user-settings-education-faculty-select').change(function () {
        var val = $(this).val();
        var chairsSelect = $('.js-user-settings-education-chair-select');
        chairsSelect.empty().append('<option value="">Не указан</option>');
        ls.ajax.load(aRouter.user + 'ajax/settings-education/get-chair', {
            faculty: val
        }, function (result) {
            if (result.chairs) {
                $.each(result.chairs, function (k, v) {
                    chairsSelect.append('<option value="' + v.value + '">' + v.title + '</option>');
                });
            }
        });
    });

    $(".js-local-scroll").click(function () {
        if (location.pathname.replace(/^\//, '') == this.pathname.replace(/^\//, '') && location.hostname == this.hostname) {
            var target = $(this.hash);
            target = target.length ? target : $('[name=' + this.hash.slice(1) + ']');
            if (target.length) {
                $('html,body').animate({
                    scrollTop: target.offset().top
                }, 500);
                return false;
            }
        }
    });

    $(".js-profile-medical-group-save").click(function () {
        var user = $(this).data('user');
        ls.ajax.load('plugin-stashable/field-update', {
            entity: 'ModuleUser_EntityUser',
            id: user,
            field: 'medical_group_id',
            value: $('.js-profile-medical-group-select').val()
        }, function (result) {
            ls.msg.notice('Данные сохранены');
        });
    });

    ls.ajax.form(aRouter.branch + 'ajax/registration/update', '.js-branch-registration-update-form', function (result) {

    });

    ls.profile.init();
    $('.js-users-search-filter').nsUsersSearchFilter();


    $('.js-feedback-modal').each(function (k, v) {
        v = $(v);
        var url = 'feedback/submit-modal';
        v.lsModalToggle({
            url: url,
            modalOptions: {
                center: false
            }
        });
        v.bind('lsmodaltoggleshow', function () {
            ls.ajax.form('feedback/submit', '.js-feedback-submit-form', function (result) {
                $('#modal-feedback-submit').lsModal('hide');
            });
        }.bind(this));
    }.bind(this));

    // Хук конца инициализации javascript-составляющих шаблона
    ls.hook.run('ls_template_init_end', [], window);
});

/**
 * Общая шина событий для компонентов Vue
 * @type {Vue}
 */
var $bus = new Vue();
